{
    "resourcePath": "/api",
    "basePath": "http://localhost:8888/api",
    "apis": [
        {
            "path": "/occupations",
            "description": "List of occupations",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "order",
                            "description": "filter that changes the ordering of the content",
                            "defaultValue": "a-z",
                            "allowableValues": {
                                "values": [
                                    "a-z",
                                    "z-a"
                                ],
                                "valueType": "LIST"
                            },
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "limit",
                            "description": "number of results to be returned",
                            "defaultValue": "10",
                            "required": false,
                            "dataType": "integer",
                            "allowMultiple": false,
                            "paramType": "query",
                            "minimum": "10"
                        },
                        {
                            "name": "offset",
                            "description": "offset of the search",
                            "defaultValue": "0",
                            "required": false,
                            "dataType": "integer",
                            "allowMultiple": false,
                            "paramType": "query",
                            "minimum": "0"
                        }
                    ],
                    "nickname": "getOccupations",
                    "responseClass": "occupations",
                    "summary": "Retrieve a list of occupations",
                    "httpMethod": "GET",
                    "errorResponses": [
                        {
                            "reason": "Bad Request: syntax error",
                            "code": 400
                        },
                        {
                            "reason": "Unauthorized: authentication required",
                            "code": 401
                        },
                        {
                            "reason": "Forbidden",
                            "code": 403
                        },
                        {
                            "reason": "No results found",
                            "code": 404
                        }
                    ]
                }
            ]
        },
        {
            "path": "/occupations/{id}",
            "description": "details of an occupation",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "onet",
                            "description": "onet code of the occupation",
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "path"
                        }
                    ],
                    "nickname": "getOccupation",
                    "responseClass": "occupation",
                    "summary": "Get information of the occupation",
                    "httpMethod": "GET",
                    "errorResponses": [
                        {
                            "reason": "Bad Request: syntax error",
                            "code": 400
                        },
                        {
                            "reason": "Unauthorized: authentication required",
                            "code": 401
                        },
                        {
                            "reason": "Forbidden",
                            "code": 403
                        },
                        {
                            "reason": "No results found",
                            "code": 404
                        }
                    ]
                }
            ]
        },
        {
            "path": "/occupations/{id}/careerScape",
            "description": "career scape for an occupation",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "id",
                            "description": "id of the occupation",
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "path"
                        }
                    ],
                    "nickname": "getCareerScapeOccupation",
                    "responseClass": "careerScape",
                    "summary": "career scape for an occupation",
                    "httpMethod": "GET",
                    "errorResponses": [
                        {
                            "reason": "Bad Request: syntax error",
                            "code": 400
                        },
                        {
                            "reason": "Unauthorized: authentication required",
                            "code": 401
                        },
                        {
                            "reason": "Forbidden",
                            "code": 403
                        },
                        {
                            "reason": "No results found",
                            "code": 404
                        }
                    ]
                }
            ]
        }
    ],
