/*
General CSS
*/
@mixin main-block{
    width:$total-width;
    display:block;
    margin:auto;
    @media only screen and (max-width: 768px){
        width:97%;
    }
}
@mixin webkit-box{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow:hidden;

}
@mixin absoluteFill{
    position:absolute;left:0px;right:0px;top:0px;bottom:0px;
    width:100%;height:100%;
}
@mixin box-shadow($left,$bottom,$spread,$thickness,$opacity){
    box-shadow:$left $bottom $spread $thickness rgba(0,0,0,$opacity);
    -webkit-box-shadow: $left $bottom $spread $thickness rgba(0,0,0,$opacity);
}
.visible{
    display:block;
}
.invisible{
    display:none;
}
.clear{
    clear:both;
}
@mixin no_select{
   -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
.no_select{
    -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
.main-container{
    @include main-block;
}
.link{
    position:relative;
}
.link > a{
    @include absoluteFill;
    z-index:10;
}
.link:hover{
    cursor:pointer;
}
table{
    border-collapse:collapse;
}
table td{
    vertical-align:middle;
}
.left{
    float:left;
}
.right{
    float:right;
}
.toggleMenu,.toggleMenuSimple{
    position:relative;
    .subMenu{
        position:absolute;
        left:0px;
        z-index:100;
        width:100%;
        color:inherit;
        display:none;
        background-color:inherit;
    }
}
/*
Arrow
*/
@mixin arrow-up-down{
    width: 0; 
	height: 0; 
	border-left-style:solid;
    border-left-color:transparent;
    border-right-style:solid;  
    border-right-color:transparent;  
	font-size: 0;
	line-height: 0;
    
}
.arrow-up {
    @include arrow-up-down;
    border-bottom-style:solid; 
}
.arrow-down {    
    @include arrow-up-down;
	border-top-style:solid; 
}
/*
Leaflet
*/
.leaflet-top-pane {
    pointer-events: none;
}
.leaflet-control-attribution,.map-legends {
    display:none !important;
}
