			margin: 0px 3px;
            margin-top:1px;
			display: inline-block;
			vertical-align: top;
		}
        .icon-star,.icon-star-disabled{
            margin-left:-5px;
        }
        @media only screen and (max-width:768px){
          margin: 10px;margin-top:0px;margin-bottom:5px;
          .icon-star,.icon-star-disabled{
            margin-left:-2.5px;
          }
          .commentCount{
            margin-top:1px;
          }
        }
	}
	.text{
		overflow:hidden;
		margin: 0px 30px;
		margin-bottom: 25px;
		-webkit-line-clamp: 8;
        @media only screen and (max-width:768px){
            margin: 10px;margin-top:0px;margin-bottom:0px;
            overflow:hidden;
		    -webkit-line-clamp: 13;
        }
	}
    @include font-size-1;

}
@mixin a-2-3{
    position:relative;
    div.comment{
        display:none;
    }
    div.text,h3.subtitle,.img{
        width:100%;
    }
    .not-pictured{
        .left,.right{
            width:100%;
        }
    }
    .pictured{
        .left{
            float:left;
            clear:left;
        }
        .right{
            float:right;
        }
    }
}
/*
Small A2 or A3
*/
@mixin a-small{
     @media only screen and (max-width:768px){
        @include phone-height;
        //to be sure
        .pictured .left,.pictured .right,.not-pictured .left,.not-pictured .right{
            width:100%;
            display:block;
        }
        .pictured h3.subtitle,.pictured h2.title,.pictured div.text,.not-pictured h3.subtitle,.not-pictured h2.title,.not-pictured div.text{
            padding-left: 10px;padding-right: 10px;
        }
        .pictured h3.subtitle,.not-pictured h3.subtitle{
            padding-top:7px;
        }
        .pictured h2.title,not-pictured h2.title{
            width:auto;
            min-height:50px;
            -webkit-line-clamp: 3;
        }
        .pictured{
           .text{
              display: none;
            }
           .img{
               position:absolute;
               height:115px;
               bottom:0px;
           }
        }
        .not-pictured{
            div.text{
               padding-top:7px;
                max-height:100%;
              -webkit-line-clamp: 7;
            }
           .img{
                display:none;
           }
        }
        @include font-size-2;
    }
}
