}

.transp-black{
	background: rgba(0, 0, 0, 0.6);
	.title{
		color: white;
	}
	position:absolute;
}

@mixin fontMedia{
	overflow: hidden;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 1;
}
@mixin video-info-promo{
    position: relative;
    .transp-black{
        position:absolute;
        z-index: 2;
        h2.title{
			-webkit-line-clamp: 3;
        }
    }
    .comment{
        display:none;
    }
    .img{
		@include fontMedia;
	}
}
.infographic{
    @include video-info-promo;
