                margin:auto;
                height:80%;
                overflow:hidden;
            }
            .content-job{
                width:2700px;
            }
            .job-recommandation{
                text-transform:uppercase;
                height:100%;
                width:180px;
                .icon-suitcase-job{
                    margin-top:35px;
                    margin-bottom:10px;
                }
                @include inline-top;
                font-size:17px;
                line-height:23px;
                div{
                    text-align:center;
                }
                .title-job{
                    color:black;
                }
                .info-job{
                    color:$grey;
                }
            }
            .jobs-pages{
                text-align:center;
                .activated{
                    background-color:$navcolor !important;
                }
                .ball{
                    margin:0px 5px;
                    @include inline-top;
                    width:15px;height:15px;border-radius:10px;
                    background-color:$lightBlue;
                }
            }
        }
    }
    .dialog{
        z-index:300;
        display:none;
        position:fixed;top:0px;left:0px;bottom:0px;right:0px;width:100%;height:100%;
        .background{
            background:rgba(0,0,0,0.5);
            position:fixed;top:0px;left:0px;bottom:0px;right:0px;width:100%;height:100%;
        }
        .section{
            position:relative;display:none;
            width:60%;margin:auto;margin-top:100px;background-color:rgb(243,245,245);
            .header{
                font-size:27px;padding-left:30px;padding-right:15px;padding-top:20px;font-weight:bold;padding-bottom:12px;
                border-bottom:1px solid $lightGrey;
                .close{
                    float:right;
                }
            }
            .intro{
                width:100%;
                font-weight:18px;padding:22px;
                border-bottom:1px solid $lightGrey;
                td{vertical-align:middle;}
                .logo{position:relative;top:26px;left:17px;}
                strong{
                    font-weight:bold;
                }
            }
            .content{padding:15px;width:100%;}
            .footer-dialog{
                padding:22px;
            }
        }
    }
    .help-section{
        .section{
            width:85%;
        }
    }
    .save-section{
        .section{
            width:60%;
            .content{  border-bottom:1px solid $lightGrey;text-align:center;
                .logo{ display:block;margin:auto; }
                input{ margin:auto; }
            }
            .footer-dialog{
                
                .button{
                    float:right;
                }
            }
        }
    }
}
.locallyGrown{
    .selector{
        .input-zip{
            width:23%;
            min-width:267px;
            float:left;
            @include blue-element;
            height:65px;
        }
    }
    .mapbox{
        @include main-block;
        height:590px;
    }
}

/*
Sub menu design
*/
.subMenu{
    width:100%;
    color:inherit;
    display:none;
    background-color:inherit;
}
/*
Career Scape controller
*/
.careerScape{
    canvas{
        background-color:white;
        margin:auto;cursor:move;padding:0px;display:block;z-index:1;
    }
    .canvas-container{
        margin:auto;
        text-align:center;
        position:relative;
    }
    .buttons-changes{
            width:23%;
            min-width:267px;
            height:65px;
            float:left;
            background-color:$navcolor;
    }
    .button-change{
        color:white;
        font-size:18px;
        display:inline-block;
        height:100%;
        width:49%;
        line-height:65px;
        .carre{
            float:left;
            width:20px;
            height:20px;
            background:white;
            position:relative;
            top:19px;
            margin-left:15px;
            margin-right:10px;
        }
        .txt{
            float:left;            
        }
        
    }
    .button-change:hover{
        cursor:pointer
    }
    .button-change:hover:not(.selected) .txt{
        color:rgba(255,255,255,0.8);
    }
    .button-change:hover:not(.selected) .carre{
        background-color:rgba(255,255,255,0.8);
    }
    .buttons-changes .selected .txt,.buttons-changes .selected:hover .txt{
        color:rgb(238,120,53);
    }
    .buttons-changes .selected .carre,.buttons-changes .selected:hover .carre{
        background:rgb(238,120,53);
    }
    .zoom{
        width:38px;
        font-weight:600;
        font-size:25px;
        background:rgb(80,80,80);
        color:white;
        margin-bottom:12px;
        padding-top:5px;
        padding-bottom:3px;
    }
    .zoom:hover:not(.selected){
            background: rgba(50,50,50,0.8);
            cursor:pointer;
    }
    .button:hover{
        cursor:pointer;
    }
    #json{
        margin:auto;
        display:block;
        margin-top:40px;
        background:#FFFEAA;
        color:rgb(90,90,90);
        width:70%;
        height:400px;
    }
    /*
    Legend part
    */
    .legend{
        z-index:10;
        position:absolute;
        width:200px;
        height:300px;
        top:40px;
        right:22px;  
        -moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        -webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
    }
    .legend > div{
        position:relative;
        width:100%;
        height:150px;
        background:white;
        /*border-bottom:1.5px solid rgba(200,200,200,0.7);
        border-right:1.5px solid rgba(200,200,200,0.7);
        border-left:1.5px solid rgba(200,200,200,0.7);*/
    }
    .legend >div.growth{
        border-bottom:1.5px solid rgba(200,200,200,0.7);

    }
    .legend .ball{
        display:inline-block;
        position:absolute;
        border:2px solid grey;
        background:lightgrey;
    }
    .legend .big{
        top:24px;
        right:26px;
        width:68px;
        height:68px;
        border-radius:39px;
    }
    .legend .medium{
        top:24px;
        right:85px;
        width:54px;
        height:54px;
        border-radius:32px;
        
    }
    .legend .small{
        top:24px;
        right:133px;
        width:38px;
        height:38px;
        border-radius:24px;            
    }
    .legend .info{
        text-align:left;
        position:absolute;
        bottom:18px;
        left:22px;
    }
    .legend .info .title{
        font-weight:200;
        font-size:18px;
    }
    .legend .info .comment{
        font-style:italic;
        font-size:12px;
    }
}

