        .title{
            font-size:20px;
            padding-top:22px;
            padding-bottom:20px;
            border-bottom:1px solid $grey;
        }
        .jobs{
            height:275px;
            .jobs-recommandations{
                width:80%;
                margin:auto;
                height:80%;
                overflow:hidden;
            }
            .content-job{
                width:2700px;
            }
            .job-recommandation{
                text-transform:uppercase;
                height:100%;
                width:180px;
                .icon-suitcase-job{
                    margin-top:35px;
                    margin-bottom:10px;
                }
                @include inline-top;
                font-size:17px;
                line-height:23px;
                div{
                    text-align:center;
                }
                .title-job{
                    color:black;
                }
                .info-job{
                    color:$grey;
                }
            }
            .jobs-pages{
                text-align:center;
                .activated{
                    background-color:$navcolor !important;
                }
                .ball{
                    margin:0px 5px;
                    @include inline-top;
                    width:15px;height:15px;border-radius:10px;
                    background-color:$lightBlue;
                }
            }
        }
    }
}
/*
Sub menu design
*/
.subMenu{
    width:100%;
    color:inherit;
    display:none;
    background-color:inherit;
}
/*
Career Scape controller
*/
.careerScape{
    canvas{
        background-color:white;
        margin:auto;cursor:move;padding:0px;display:block;z-index:1;
    }
    .canvas-container{
        margin:auto;
        text-align:center;
        position:relative;
        margin-bottom:60px;
    }
    .zoom{
        width:38px;
        font-weight:600;
        font-size:25px;
        background:rgb(80,80,80);
        color:white;
        margin-bottom:12px;
        padding-top:5px;
        padding-bottom:3px;
    }
    .zoom:hover:not(.selected){
            background: rgba(50,50,50,0.8);
            cursor:pointer;
    }
    .button:hover{
        cursor:pointer;
    }
    #json{
        margin:auto;
        display:block;
        margin-top:40px;
        background:#FFFEAA;
        color:rgb(90,90,90);
        width:70%;
        height:400px;
    }
    /*
    Legend part
    */
    .legend{
        z-index:100;
        position:absolute;
        width:200px;
        height:300px;
        top:40px;
        right:22px;  
        -moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        -webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
    }
    .legend > div{
        position:relative;
        width:100%;
        height:150px;
        background:white;
        /*border-bottom:1.5px solid rgba(200,200,200,0.7);
        border-right:1.5px solid rgba(200,200,200,0.7);
        border-left:1.5px solid rgba(200,200,200,0.7);*/
    }
    .legend >div.growth{
        border-bottom:1.5px solid rgba(200,200,200,0.7);

    }
    .legend .ball{
        display:inline-block;
        position:absolute;
        border:2px solid grey;
        background:lightgrey;
    }
    .legend .big{
        top:24px;
        right:26px;
        width:68px;
        height:68px;
        border-radius:39px;
    }
    .legend .medium{
        top:24px;
        right:85px;
        width:54px;
        height:54px;
        border-radius:32px;
        
    }
    .legend .small{
        top:24px;
        right:133px;
        width:38px;
        height:38px;
        border-radius:24px;            
    }
    .legend .info{
        text-align:left;
        position:absolute;
        bottom:18px;
        left:22px;
    }
    .legend .info .title{
        font-weight:200;
        font-size:18px;
    }
    .legend .info .comment{
        font-style:italic;
        font-size:12px;
    }
}

