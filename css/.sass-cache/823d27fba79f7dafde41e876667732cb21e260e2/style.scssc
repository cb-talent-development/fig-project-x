@mixin fontMedia{
	overflow: hidden;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 1;
}
@mixin video-info-promo{
    position: relative;
    .transp-black{
        position:absolute;
        z-index: 2;
        h2.title{
			-webkit-line-clamp: 3;
        }
    }
    .comment{
        display:none;
    }
    .img{
		@include fontMedia;
	}
}
.infographic{
    @include video-info-promo;
	.icon{
		position:absolute;
		z-index: 3;
		bottom: 10px;
		right: 10px;
	}
}
@mixin video-info-promo-1{
    .transp-black{
        position:relative;
        top:0px;left:0px;
		@media only screen and (max-width:768px){
			width: 100%;
		}
		@media only screen and (min-width:769px){
			height: 100%;
			width: 363px;
		}
		h2.title{
			padding: 25px;
			padding-bottom: 0px;
			margin: 0px;
