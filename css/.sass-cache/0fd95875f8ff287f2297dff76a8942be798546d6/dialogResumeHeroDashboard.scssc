:@template"œZ/*
POPUPS PART (DASHBOARD , RESUME HERO AND OTHER MORE SIMPLE POPUPS)
*/
.dashboard-popup,.tool .get-started-resume{
    .section{
        .steps-content{
           @media only screen and (max-width: 1000px){
               display:none;
            }
        }
        .content{
            padding: 20px;
            border-bottom:1px solid $lightGrey;
            @media only screen and (max-width: 1000px){
                padding:10px;
            }
            .icon-step-1, .icon-step-2, .icon-step-3, .icon-step-4, .icon-step-5,.icon-step-1-grey, .icon-step-2-grey, .icon-step-3-grey, .icon-step-4-grey, .icon-step-5-grey{
                margin-right: -28px;
                display:inline-block;
                vertical-align:top;
                text-align:center;
                padding-top:26px;
                color: white;
                font-size:23px;
                @media only screen and (max-width: 1000px){
                    padding-top:13px;
                    font-size:18px;
                }
                span{
                   position:relative;right:15px;
                   @media only screen and (max-width: 1000px){
                   }
                }
            }
            .icon-step-5,.icon-step-5-grey{
                span{
                    left:15px;  
                    right:0px;
                }
            }
        }
    }
}
.dashboard-popup, .get-started-resume, .template-resume, .export-resume, .delete-resume{
    .scrollable-content{
        width:1250px;
		@media only screen and (max-width: 1200px){
            width:100%;
        }
    }
    .scrollable{
        //option 1
        width:800%;
        min-width:8 * 1250px;
        @media only screen and (max-width: 1200px){
            min-width:8 * 1000px;
        }
        @media only screen and (max-width: 1000px){
            min-width:0px;
        }
    }
    .section-wrapper{
        width:12.5%;
        float:left;
    }
    .section{
        //width:90%;
        width:1204px;
        @media only screen and (max-width: 1260px){
			width: 1005px;
		}
		@media only screen and (max-width: 1024px){
            width:975px;
        }
        @media only screen and (max-width: 1000px){
            width:90%;
        }
        .header{
            @include font-size(24px);
            .grey{
                color:$grey;
            }
        }
        .step{
            width:1204px;
            @media only screen and (max-width: 1000px){
                width:100%;
            }
        }
        .step1{
            display:none;
        }
        .custom-radio{
            margin-right:25px;
            position:relative;
            input[type=checkbox] {
                display: none;
            } 
            label{margin-left:40px;}
            label:hover{ cursor:pointer;}
            label:before{
                height:20px;
                width:20px;
                content:"";
                position:absolute;
                left:0px;
                top:50%;
                text-align: center;  
                margin-top:-15px;
                @include box-shadow(0.5px,-1px,2px,0px,0.3);
                border-right:1px solid $lightGrey;
                background:white;
            }
            input[type=radio]:checked + label:before { 
                background:$orange;
            }  
        }
        .custom-radio{
            @include font-size(23px);
            color:$darkGrey;
            @media only screen and (max-width: 1000px){
                font-size:17px;
            }
        }
        .message{
            @include font-size(17px);
            text-transform:uppercase;
            color:rgb(178,178,178);
            margin-bottom:10px;			
			@media only screen and (max-width: 1260px){
				@include font-size(15px);
			}
        }
        .optional{
            color:$lightGrey;
            text-transform:uppercase;
            @include font-size(17px);
			@media only screen and (max-width: 1260px){
				@include font-size(15px);
			}
            @media only screen and (max-width: 1000px){
                display:none;
            }
        }
        .select,input,input[placeholder]{  
            padding-top:20px;padding-bottom:14px;
            font-size:23px;
            @media only screen and (max-width: 1000px){
                padding-top:15px;
                padding-bottom:10px;
                font-size:16px
            }
        }   
        .large{width:465px;margin-right:25px;
            @media only screen and (max-width: 1000px){
                width:49%;
                margin-right:2%;
                margin-bottom:10px;
            }
        }
        .medium{width:370px;margin-right:25px;
            @media only screen and (max-width: 1260px){
                width:305px;
            }  
            @media only screen and (max-width: 1024px){
                width:295px;
            }   
            @media only screen and (max-width: 1000px){
                width:49%;
                margin-right:2%;
                margin-bottom:10px;
            }   
        }
        .last{
            margin-right:0px;
        }
        .right{
            .large,.medium{
                margin-right:0px;
            }
        }
        .email-address,.street,.industry-goal{
            @media only screen and (max-width: 1000px){
                width:100% !important;
                margin-right:0% !important;   
            }
        }
        .address{
            .street{margin-bottom:20px;}
            .large{
               width:365px;margin-right:25px;
			   @media only screen and (max-width: 1260px){
                    width:363px;
                    margin-right:2%;
                    margin-bottom:10px;
                }
			   @media only screen and (max-width: 1024px){
                    width:354px;
                }
               @media only screen and (max-width: 1000px){
                    width:49%;
                    margin-right:2%;
                    margin-bottom:10px;
                }  
            }
            .medium{width:170px;margin-right:25px;
                @media only screen and (max-width: 1024px){
                    width:164.5px;
                } 
                @media only screen and (max-width: 1000px){
                    width:49%;
                    margin-right:2%;
                    margin-bottom:10px;
                } 
            }
        }
        .general{
            .age,.gender{
                @media only screen and (max-width: 1000px){
                        width:50%;
                    }
            }
            .age{
              .large,.medium,.message{
                    @media only screen and (max-width: 1000px){
                        width:100%;
                        margin-right:0%;
                        margin-left:0%;
                        margin-bottom:10px;
                    } 
                }  
            }
            .gender{
                margin-right:149px;
                @media only screen and (max-width: 1000px){
                    margin-right:0px;
                }
                .custom-checkbox{margin-top:15px;}
            }
            
        }
        .job-phase{
            padding-top:25px;
            .cursor{
                margin-top:20px;
                margin-bottom:45px;
                position:relative;
                @media only screen and (max-width: 1000px){
                    margin-bottom:20px;
                    display:none;
                }
                .bar{
                    border-radius:8px;
                    height:15px;
                    background:rgb(231,235,235);
                }
                .icon-cursor-orange-down{
                    position:absolute;
                    top:-9px;
                    left:10px;
                }
            }
            .phase{
                opacity:0.6;
                @include whitebox;
                width:23.35%;
                height:295px;
                text-align:center;
                margin-bottom:15px;
                overflow:hidden;
                @media only screen and (max-width: 1000px){
                    width:49%;
                    height:275px;
                }
                .title{
                    @include font-size(20px);
                    padding-top:20px;padding-bottom:25px;
                    @media only screen and (max-width: 1000px){
                        padding-top:15px;padding-bottom:15px;
                    }
                    .logo{
                        margin-bottom:15px;
                    }
                    border-bottom:1px solid $lightGrey;
                }
                .description{
                    padding:0px 30px;
                    @include font-size(15px);
                    line-height:20px;
                    padding-top:25px;
                    @include webkit-box;
                    -webkit-line-clamp: 6;
                    max-height:142px;
                    @media only screen and (max-width: 1000px){
                        padding:10px;
                        max-height:166px;
                        padding-bottom:0px;
                        -webkit-line-clamp: 8;
                    }
                }
            }
            .left{margin-right:2.2%;
                @media only screen and (max-width: 1000px){
                    margin-right:2%;
                }
            }
            .before-last{
                margin-right:0px;
                @media only screen and (max-width: 1000px){
                    margin-right:2%;
                }
            }
             .phase[class~="selected"]{
                opacity:1;
            }
        }
        .job-occupation{
            height:200px;
            @media only screen and (max-width: 1000px){/* and (min-width: 481px){
                height:190px;
            }
            @media only screen and (max-width: 480px){*/
                height:auto;
            }
        }
        .school-name{
            height:210px;
            @media only screen and (max-width: 1000px){/* and (min-width: 481px){
                height:190px;
            }
            @media only screen and (max-width: 480px){*/
                height:auto;
            }
        }
        .job-occupation,.school-name{
            .large{
                width:465px;margin:0px;
                @media only screen and (max-width: 1000px){
                    width:100%;
                    margin-bottom:10px;
                }
            }
            .message-large{
                width:87%;margin:0px;
                @media only screen and (max-width: 1000px){
                    width:100%;
                }
            }
            .element{
                height:100%;
                width:42%;
                position:relative;
                .entry{position:absolute;bottom:10px;
                    .message{margin-bottom:10px;}
                }
                @media only screen and (max-width: 1000px){
                   // width:48%;
                    .entry{
                        width:100%;
                        bottom:0px;
                    }
                //}
                //@media only screen and (max-width: 480px){
                     width:100%;
                    .entry{
                        position:relative;
                    }
                }
                
            }
            .element-correction{
                height:100%;
                width:50.7%;
                position:relative;
                @media only screen and (max-width: 1000px){
                    width:100%;
                }
                .logo{
                    position:absolute;
                    bottom:10px;
                    @media only screen and (max-width: 1260px){
                        display:none;
                    }
                }
                .correction{
                    width:465px;
                    position:absolute;
                    bottom:10px;
                    right:0px;
                    .mess{
                        @include font-size(17px);
                        line-height:23px;
                        margin-bottom:10px;
                        text-transform:uppercase;
                        color:$orange;
                        @include webkit-box;
                        -webkit-line-clamp:4;
                    }
                    .wrong{color:black;}
                    .select{border:1px solid $orange;}
                    @media only screen and (max-width: 1000px){
                        width:100%;
                        bottom:0px;
                        .mess{
                            -webkit-line-clamp:5;
                            line-height:20px;
                        }
                   /* }
                    @media only screen and (max-width: 480px){*/
                        position:relative;
                    }
                }
            }
        }
        .job-occupation,.school-name,.job-info,.school-info{
            @media only screen and (min-width: 1001px){
                .message{
                    margin-top:5px;
                }
                .large{
                    width:455px;
                }
                >.left ,>.right{
                    width:49%;
                }
            }
            @media only screen and (max-width: 1000px){
                .message{
                    margin-top:5px;
                }
                .large{
                    width:100%;
                }
                >.left ,>.right{
                    width:100%;
                }
            }
            @media only screen and (max-width: 768px){
                >.left ,>.right{
                    width:100%;
                }
            }
        }
        .selectable-title{
            padding:20px;padding-bottom:5px;
            position:relative;
            padding-top:35px;
            font-size: 47px;
            @media only screen and (max-width: 1000px){
                font-size:25px;
                padding:10px;
                padding-top:20px;
                padding-bottom:5px;
                
            }
            .selectable-button{
                display:inline-block;
                width:33px;
                @media only screen and (max-width: 1000px){
                    width:18px;
                }
                .icon-plus{
                    position:relative;
                    top:1px;
                    display:block;
                }
                .icon-minus{
                    position:relative;
                    top:-14px;
                    display:none;
                    @media only screen and (max-width: 1000px){
                        top:-7px;
                    }
                }
            }
            .selectable-button[class~=selected]{
                .icon-plus{
                    display:none;
                }
                .icon-minus{
                    display:block;
                }
            }
            .icon-completed{
                i{
                    position:absolute;
                    right:30px;
                    top:30px;
                    @media only screen and (max-width: 1000px){
                        top:auto;
                        bottom:9px;
                        right:10px;
                    }
                    margin-right:0px;
                }
            }
            border-bottom:8px solid rgb(232,232,232);
        }
        .selectable-content{
            border-top:1px solid rgb(201,202,203);
            display:none;
            padding:50px 20px;
            padding-bottom:0px;
            @media only screen and (max-width: 1000px){
                padding:10px 10px;
                padding-top:20px;
                padding-bottom:0px;
            }
            .check-box-1{
                float:left;
                padding-bottom:30px;
                padding-right:3.5%;
                width:33.3%;
                @include font-size(19px);
                label{
                    max-height: none;
                    overflow: visible;
                }
                @media only screen and (max-width: 1000px){
                    width:100%;
                    padding-right:0%;
                    padding-bottom:10px;
                }
            }
        }
        .goals{
            @media only screen and (max-width: 1000px){
                .message{
                    margin-top:5px;
                }
            }
            .medium{
                width:371px;
                @media only screen and (max-width: 1260px){
                   width: 305px;
                }
                @media only screen and (max-width: 1024px){
                   width: 295px;
                }
                @media only screen and (max-width: 1000px){
                   width:100%;
                }
                @media only screen and (max-width: 1000px){
                   width:100%;
                }
            }
            .left,.right{
                @media only screen and (max-width: 1000px){
                   width:100%;
                }
            }
            .selectable-content{
                padding:20px;
                padding-top:30px;
                border-bottom:1px solid rgb(201,202,203);
                @media only screen and (max-width: 1000px){
                    padding:10px 10px;
                    padding-top:15px;
                }
            }
        }
        .footer-dialog{
            @media(min-width: 480px){
                padding-top:12px;
                padding-bottom:15px;
            }
        }
        .user-info{
            .message{
                @media only screen and (max-width: 1000px){
                    margin-right:0px;
                    width:100%;
                }
            }
            .presentation{
                //special effect here
                @media only screen and (max-width: 600px){
                    text-align:center;
                }
            }
            .name-info{
                margin-bottom:20px;
                @media only screen and (max-width: 1000px){
                    margin-bottom:0px;
                }
            }
            .explanation{
                width:59%;
                font-size:27px;
                .presentation{
                    margin-bottom:40px;
                }
                @media only screen and (max-width: 1000px){
                    font-size:18px;
                    width:57%;
                    padding-right:7px;
                    .presentation{
                        margin-bottom:15px;
                    }
                }
                @media only screen and (max-width: 700px){
                    width:51%;
                }
                //special responsive here (no choice)
                @media only screen and (max-width: 600px){
                    width:100%;
                }
            }
            .info{
                font-size:22px;
                @include white-box;
                .strength {
                    margin:15px 0px;
                }
                .icon{
                    margin:0px 3px;
                }
                @media only screen and (max-width: 1000px){
                    font-size:16px;
                    padding:10px;
                }
                @media only screen and (max-width: 600px){
                    width:auto;
                    display:inline-block;clear:both;
                    float:none;
                    margin:15px auto;
                }
            } 
        }
        .right-resp{
            @media only screen and (max-width: 1000px){
                float:right;
                margin-left:0px !important;
                margin-right:0px !important;
                margin-top:0px;
                margin-bottom:10px;
            }  
        }
    }
    .section-photo{
        width:1000px;
        @media only screen and (max-width: 1000px){
            width:90%;
        }
        .change-picture{
            padding:40px;
            text-align:center;
            border-bottom:1px solid rgb(201,202,203);
            @media only screen and (max-width: 1000px){
                padding:20px;
            }
            .photo{
                margin:auto;
                width:320px;
                height:320px;
                border-radius:200px;
                -webkit-box-shadow:0 1px 2px rgba(0,0,0,0.2);
                border: 7px solid white;
                margin-bottom:50px; 
                @media only screen and (max-width: 1000px){
                    width:200px;
                    height:200px;
                    border-radius:140px;
                    border: 5px solid white;
                    margin-bottom:40px; 
                }
            }
            .buttons{
                .button-upload-wrapper{
                    display:inline-block;
                    vertical-align:top;
                    position:relative;
                    .message{
                        position:absolute;
                        top:-30px;
                        left:0px;
                    }
                    @media only screen and (max-width: 1000px){
                        width:90%;
                        margin-bottom:10px;
                        .message{
                            top:-25px;
                            left:5%;
                        }
                    }
                }
                .button{
                    @extend .button-social
                }
                .button-upload{
                    width:330px;margin-right:25px;
                    background-color:rgb(63,93,149);
                    .message{
                        position:absolute;top:-30px;left:0px;
                    }
                    @media only screen and (max-width: 1000px){
                        width:100%;
                        margin-right:0px;
                        margin:auto;
                    }
                }
                .button-upload:hover{
                   background-color:rgb(53,83,139);
                }
                .button-edit{
                    margin-right:25px;
                    background-color:rgb(238,124,51); 
                    @media only screen and (max-width: 1000px){
                        margin-right:0px;
                        margin-left:5%;
                        float:left;
                    }
                }
                .button-edit:hover{
                   background-color:rgb(228,114,41);  
                }
                .button-delete{
                    background-color:rgb(233,63,51); 
                    @media only screen and (max-width: 1000px){
                        float:right;
                        margin-right:5%;
                    }
                }
                .button-delete:hover{
                    background-color:rgb(223,53,41);  
                }
            }
        }
    }
}
