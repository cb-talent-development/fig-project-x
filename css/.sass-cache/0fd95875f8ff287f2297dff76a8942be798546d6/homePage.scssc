/*
Home Page css (Post and buttons)
*/
//
/*
Font mixins
*/
@mixin font-size-1{
    h2.title{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:55px;
        line-height:58px;
        @media only screen and (max-width: 1000px) {
		      font-size:28px;
              line-height:30px;
        }
        @media only screen and (max-width: 480px) {
		      font-size:24px;
              line-height:26px;
        }
	}
	h3.subtitle{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:22px;
        @media only screen and (max-width: 1000px){
            font-size:16px;
            line-height:19px;
        }
        @media only screen and (max-width: 480px){
            font-size:13px;
            line-height:16px;
        }
	}
	div.comment{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:18px;
        @media only screen and (max-width: 1000px){
            font-size: 14px;line-height:18px;
        }
        @media only screen and (max-width: 480px){
            font-size: 10px;line-height:13px;
        }
	}
	div.text{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:16px;
        line-height:23px;
        @media only screen and (max-width: 1000px){  
            font-size:15px;line-height:18px;
        }
        @media only screen and (max-width: 480px){
            font-size:13px;line-height:16px;
        }
	}
    @include font-family;
}
@mixin media-font-size-2-3{
	@media only screen and (max-width: 1200px) {
		h3.subtitle{
			font-size: 15px;line-height: 17px;
		}
	}
    @media only screen and (max-width: 1000px) {
		h2.title{
			font-size: 17px;line-height:21px;
		}
		h3.subtitle{
			font-size:12px;line-height:16px;
		}
		div.text{
			font-size:12px;line-height:16px;
		}
    }
}
@mixin font-size-2{
    h2.title{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:35px;
        line-height:38px;
	}
	h3.subtitle{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:18px;line-height:20px;
	}
	div.text{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:15px;
        line-height:20px;
	}
    @include font-family;
    @include media-font-size-2-3;


}@mixin font-size-2-3{
    h2.title{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:28px;
        line-height:30px;
	}
	h3.subtitle{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:18px;line-height:20px;
	}
	div.text{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:15px;
        line-height:20px;
	}
    @include font-family;
    @include media-font-size-2-3;


}
@mixin font-size-3{
    h2.title{
		overflow: hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:20px;
        line-height:24px;
	}
	h3.subtitle{
		overflow: hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:14px;line-height:18px;
	}
	div.text{
		display: -webkit-box;
        overflow: hidden;
		-webkit-box-orient: vertical;
        font-size:14px;
        line-height:19px;
	}
    @include font-family;
    @include media-font-size-2-3;


} 
/*
Dimension mixins
*/
/*
Height
*/
@mixin phone-height{
    margin-bottom:30px;
    @media only screen and (max-width: 1000px){
        margin-bottom:10px;
        /*
        height: $med-box-height * $rapp-resp;
    }
    @media only screen and (max-width: 768px){
        //*/
        height: $phone-height-small;
	}
}
@mixin small-height{
	height: $small-box-height;
    @include phone-height;
}
@mixin med-height{
	height: $med-box-height;
    @include phone-height;
}
@mixin big-height{
	height: $large-box-height;
    margin-bottom:30px;
    @media only screen and (max-width: 1000px){
        margin-bottom:10px;
        /*
        height: $large-box-height * $rapp-resp;
    }
    @media only screen and (max-width: 768px){
        //*/
        height: $phone-height;
	}
}
/*
Width
*/
@mixin big-width{
    float:left;width:$total-width;margin-left:15px;margin-right:15px;
	@media only screen and (max-width: 1200px) and (min-width:1025px){
		width: 1005px;
	}
	@media only screen and (max-width: 1024px) and (min-width:1001px){
		width: 975px;
	}
	@media only screen and (max-width: 1000px){
	/*	width: 630px;margin-left:5px;margin-right:5px;
	}
	@media only screen and (max-width:650px){
		width: 460px;margin-left:5px;margin-right:5px;
	}
    @media only screen and (max-width:480px){
		*/
        //width: 300px;
        width:99%;
        //margin-left:5px;margin-right:5px;
        margin-left:0.5%;margin-right:0.5%;
	}
}
@mixin med-width{
    float:left;width:370px;margin-left:15px;margin-right:15px;
	@media only screen and (max-width: 1200px){
		width: 315px;
	}
	@media only screen and (max-width: 1024px){
		width: 305px;
	}
    @media only screen and (max-width: 1000px){
		margin-left:0.5%;margin-right:0.5%;
        width:32.33%;
	}
    @media only screen and (max-width:480px){
        //width: 145px;
        width:49%;
        //margin-left:5px;margin-right:5px;
	}
}
@mixin phone-width{
    float:left;width:300px;margin-left:5px;margin-right:5px;
	@media only screen and (max-width: 1000px){
		margin-left:0.5%;margin-right:0.5%;
        width:32.33%;
	}
    @media only screen and (max-width:480px){
        //width: 145px;
        width:49%;
        //margin-left:5px;margin-right:5px;
	}
}

.col-type-1{
    @include big-width;
}
.col-type-2{
    @include med-width;
}
.col-type-3{
    @include phone-width;
}

.button, .btn{
    @include shadow;
    @include button-style;
}
.button:hover:not(.no-hover){
    cursor:pointer;
    color: #6b6b6b;
	.grey-icon{
		opacity: 0.8;
	}
}
.select{
    @include shadow;
    @include button-style;
    //border-bottom:2px solid $lightGrey;
    b,i{margin-top:-4.5px;}
    .subMenu{
        @include shadow;
        border-top:1px solid $lightGrey;
        top:100%;
        padding:5px 15px;
        > div{
            margin:5px 0px
        }
        >div:hover{
            color:black !important;
        }
    }
}
.select:hover{cursor:pointer;}
.input-button{
    margin:0px;
    @extend .button;
    border-top: px solid $lightGrey;
    padding:0px;
    box-shadow:0 -2px 2px rgba(0,0,0,0.2);
    -webkit-box-shadow:0 -2px 2px rgba(0,0,0,0.2);
    @media only screen and (max-width: 1000px){
        @include box-shadow(0px,2px,2px,0px,0.2);
        border:none;
    }
    input{
        padding-top:16px;padding-bottom:9px;padding-left:15px;padding-right:15px; // default padding
        @include font-size(18px);
        border:none;
        display:block;
        width:100%;
        height:100%;
        @media only screen and (max-width: 1000px){
           padding-top:14px;padding-bottom:10px;padding-left:10px;padding-right:10px;  
        }
    }
    .icon-search{
        margin-top:-8px;
        opacity:0.8;
		@media only screen and (max-width: 1000px){
			margin-top: -11px !important;
			margin-right: -11px;
            right:50%;
		}
    }
}
/*
General post box template
*/
@mixin whitebox{
	background-color: white;
    @include shadow;
}
@mixin colorbox($color){
	@include whitebox;
	overflow: hidden;
	position: relative;
	border-left: solid 7px $color;
	@media only screen and (max-width: 1000px) {
		border-left: solid 3px $color;
	}
	.subtitle{
		color: $color;
	}
	.comment{
		color: $color;
	}
	a.transp-link:hover{
		background-color: $color;
	}
}
.menu-buttons.article{
	@media only screen and (max-width: 1000px){
		height: auto;
	}
}

.box-orange-1{ @include colorbox($orange-1); }
.box-orange-2{ @include colorbox($orange-2); }
.box-orange-3{ @include colorbox($orange-3); }
.box-orange-4{ @include colorbox($orange-4); }
.box-green-1{ @include colorbox($green-1); }
.box-green-2{ @include colorbox($green-2); }
.box-green-3{ @include colorbox($green-3); }
.box-green-4{ @include colorbox($green-4); }
.box-blue-1{ @include colorbox($blue-1); }
.box-blue-2{ @include colorbox($blue-2); }
.box-blue-3{ @include colorbox($blue-3); }
.box-blue-4{ @include colorbox($blue-4); }
.box-blue-5{ @include colorbox($blue-5); }
.box-blue-6{ @include colorbox($blue-6); }
.box-red-1{ @include colorbox($red-1); }
.box-red-2{ @include colorbox($red-2); }
.box-red-3{ @include colorbox($red-3); }
.box-red-4{ @include colorbox($red-4); }
.box-pink-1{ @include colorbox($pink-1); }
.box-pink-2{ @include colorbox($pink-2); }
.box-pink-3{ @include colorbox($pink-3); }

/*
week number css
*/
.end-week{
    width:100%;
    position:relative;
	margin-top: 25px;
	margin-bottom: 45px;
	width: 100%;
	color: #ccc;
	font-size: 1.7em;
    text-shadow : 0.00em 0.08em 0.04em #FFFFFF;
    text-align: center;
    .line{
        border-top:1px solid #bbb;
        border-bottom:1px solid #EEE;
        height:0px;
        position:absolute;
        width:35%;
        bottom:19px;

    }
    .left{
        left:15px;
    }
    .right{
        right:15px;
    }
}
@mixin color-article{
    h2.title{
        color:black;
    }
}
/*Elements home page css*/
/*
Article type 1
*/
.a1{
	@include big-height;
    h2.title{
        line-height:60px;
		padding: 30px;padding-bottom: 0px;
        margin: 0px;margin-bottom:5px;
        width: 800px;
		float: right;
		-webkit-line-clamp: 2;
		@media only screen and (max-width: 1200px){
			width: 688px;
		}
        @media only screen and (max-width: 1024px) and (min-width:1001px){
			width: 68%;
            padding-left:15px;
            padding-right:15px;
            min-height:20%;
            margin-bottom:12px;
		}
        @media only screen and (max-width: 1000px) {
			width: 100%;
            //min-height:20%;
            min-height: 50px;
            padding-top:15px;padding-left:15px;padding-right:15px;
            margin-bottom:12px;
		}
	}
    .left{
        float:left;
        width:363px;
		@media only screen and (max-width: 1200px) {
			width: 310px!important;
		}
		@media only screen and (max-width: 1024px) {
			width: 301px !important;
		}
        @media only screen and (max-width: 1000px) {
			width: 39% !important;
		}
        @media only screen and (max-width: 480px) {
			width: 100% !important;
		}
    }
    .right{
        float:left;
        width:800px;
        @media only screen and (max-width: 1200px) {
			width: 688px;
		}
        @media only screen and (max-width: 1024px) {
			width: 667px;
		}
		@media only screen and (max-width: 1000px) {
			width:57% !important;
		}
        @media only screen and (max-width: 480px) {
			width: 100% !important;
		}
    }
	h3.subtitle{
		display: inline-block;
		padding-top: 25px;
		padding-left: 25px;
		margin: 0px;
		@media only screen and (max-width: 1000px) {
            max-height:100px;
            -webkit-line-clamp: 2;
		    padding-left: 15px;padding-top: 10px;
            margin-bottom: 2px;
		}
	}
	.img{
        width:363px;
		height: 284px;
		@media only screen and (max-width: 1200px) {
			   width: 100%;
			}		
		@media only screen and (max-width: 1000px) {
		   height: 180px;
		   width: 100%;
		}
        @media only screen and (max-width: 480px) {
			height: 94px;
		}
	}
	div.comment{
		display: inline-block;
		margin: 30px;
        margin-top:8px;
        margin-bottom:16px;
		.commentCount{
			margin: 0px 3px;
            margin-top:1px;
			display: inline-block;vertical-align: top;
		}
        .icon-star,.icon-star-disabled{
            margin-left:-5px;
        }
        @media only screen and (max-width: 1000px) and (min-width:481px){
            .icon-talk{
                margin-top: 2px;
            }
        }
        @media only screen and (max-width: 1000px){
           margin: 10px;
           margin-top:0px;
           margin-bottom:5px;
          .icon-star,.icon-star-disabled{
            margin-left:-2.5px;
          }
          .commentCount{
            margin-top:1px;
          }
        }
	}
	.text{
		overflow:hidden;
		margin: 0px 30px;
		margin-bottom: 25px;
		-webkit-line-clamp: 6;
        @media only screen and (max-width: 1000px){
            margin: 10px;margin-top:0px;margin-bottom:0px;
            overflow:hidden;
		    -webkit-line-clamp: 10;
            max-height: 178px;
        }
        @media only screen and (max-width: 480px){
            max-height: 80px;
            -webkit-line-clamp: 5;
        }
	}
    @include font-size-1;

}
@mixin a-2-3{
    position:relative;
    div.comment{
        display:none;
    }
    div.text,h3.subtitle,.img{
        width:100%;
    }
    .not-pictured{
        .left,.right{
            width:100%;
        }
    }
    .pictured{
        .left{
            float:left;
            clear:left;
        }
    }
}
/*
Small A2 or A3
*/
@mixin a-small{
	@media only screen and (max-width: 1200px) and (min-width: 1025px){
        .not-pictured .left,.not-pictured .right{
            width: 100%;
        }
		.pictured .left{
			width: 105px;
		}
		.pictured .right{
			width: 193px;
		}
		.pictured{
			.title{
				width: 100%;
				padding: 15px;
			}
			.img{
               height:90px;
           }
		}
		.pictured, .not-pictured{	
			@include font-size-2-3;
		}
        @include font-size-3;
    }
     @media only screen and (max-width: 1024px) and (min-width:1001px){
        //to be sure
        .not-pictured .left,.not-pictured .right{
            width: 100%;
        }
		.pictured .left{
			width: 105px;
		}
		.pictured .right{
			width: 193px;
		}
		.pictured{
			.title{
				width: 100%;
				padding: 15px;
			}
			.img{
               height:90px;
           }
		}
		.pictured, .not-pictured{	
			@include font-size-2-3;
		}
        @include font-size-2;
    }
	@media only screen and (max-width: 1000px){
        @include phone-height;
        //to be sure
        .pictured .left,.pictured .right,.not-pictured .left,.not-pictured .right{
            width:100%;
            display:block;
        }
        .pictured h3.subtitle,.pictured h2.title,.pictured div.text,.not-pictured h3.subtitle,.not-pictured h2.title,.not-pictured div.text{
            padding-left: 10px;padding-right: 10px;
        }
        .pictured h3.subtitle,.not-pictured h3.subtitle{
            padding-top:7px;
        }
        .pictured h2.title,not-pictured h2.title{
            width:100%;
            min-height:50px;
            -webkit-line-clamp: 3;
        }
        .pictured{
           .text{
              display: none;
            }
           .img{
               position:absolute;
               height:115px;
               bottom:0px;
           }
        }
        .not-pictured{
            div.text{
               padding-top:7px;
                max-height:100%;
              -webkit-line-clamp: 7;
            }
           .img{
                display:none;
           }
        }
        @include font-size-2;
    }
}
/*
Article type 2
*/
.a2{
	@include med-height;
	h2.title{
		width:100%;
		overflow: hidden;
		padding: 20px;padding-top: 15px;padding-bottom: 0px;
		margin: 0px;
		-webkit-line-clamp: 3;
	}
	h3.subtitle{
		padding-left: 20px;padding-top: 15px;
		margin: 0px;
    }
	div.text{
		padding: 20px;
        padding-bottom:0px;
        padding-top:17px;
		margin: 0px;
		-webkit-line-clamp: 11;
	}
    .pictured{
       .left{
            width:128px;
        }
        .img{
          height: 125px;
          margin-top:17px;
	   }
        h3.subtitle{		
            -webkit-line-clamp: 2;
        }
        .right{
            width:235px;
        }
    }
    .not-pictured{
        .img{display:none;}
        .text{
            -webkit-line-clamp: 9;
			@media only screen and (max-width: 1200px){-webkit-line-clamp: 11;}
        }
    }
    @include font-size-2;
    @include a-2-3;
    @include a-small;
}
/*
Article type 3
*/
.a3{
	@include small-height;
    h3.subtitle{
        margin:0px;
		padding-top: 22px;
        padding-left:15px;
	}	
	.img{
		height: 115px;
		width: 100%;
	}	
	h2.title{
        width:235px;     
        margin:0px;
        padding-top:15px;padding-left:15px;padding-right:15px;
		-webkit-line-clamp: 3;
		overflow: hidden;
		@media only screen and (max-width: 1200px){
			-webkit-line-clamp: 2;
			max-height: 85px;
			padding-bottom: 0px !important;
		}
	}
	div.text{
		margin:0px;
        padding-top:4px;padding-left:15px;padding-right:15px;
		-webkit-line-clamp: 4;
		@media only screen and(max-width: 1200px){
			paddint-top: 0px;
		}
	}
    .pictured{
        h2.title{
            float:right;
			@media only screen and (max-width: 1200px) and (min-width: 1001px){
				margin-bottom: 13px;
			}
        }
        .right{
            width:235px;     
        }
        .left{
            width:128px;
        }
    }
    .not-pictured{
        div.text{
            max-height:80px;
            -webkit-line-clamp: 3;
        }
        h2.title{
            width:100%;
        }
        h3.subtitle{
            padding-top:13px;
			@media only screen and (max-width: 1200px){padding-bottom: 3px;}
        }
        .img{display:none;}
    }
    @include font-size-3;
    @include a-2-3;
    @include a-small;

}
/*
Small article (A2/A3)
*/
.a-small{
    @include phone-height;
	position: relative;
    .pictured{
	   .text{
		  display: none;
        }
	   .img{
           width: 100%;
           position:absolute;
           height:115px;
		   bottom:0px;
	   }
    }
    .not-pictured{
        .text{
		  margin: 10px;
		  margin-top: 0px;
		  padding: 0px;
		  -webkit-line-clamp: 7;
        }
	   .img{
            display:none;
	   }
    }
	.subtitle{
		padding: 0px;
		margin: 10px;
        margin-top:7px;
        margin-right:0px;
	}
	.title{
        min-height:50px;
		padding: 10px;
		padding-bottom: 0px;
		margin: 0px;margin-bottom:3px;
		-webkit-line-clamp: 3;
	}
    @include font-size-2;
}

.transp-black{
	background: rgba(0, 0, 0, 0.6);
	.title{
		color: white;
	}
	position:absolute;
}

@mixin fontMedia{
	overflow: hidden;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 1;
}
@mixin video-info-promo{
    position: relative;
    .transp-black{
        position:absolute;
        z-index: 2;
        h2.title{
			-webkit-line-clamp: 3;
        }
    }
    .comment{
        display:none;
        @media only screen and (max-width: 1000px) and (min-width:481px){
            .icon-talk{
                margin-top: 3px;
            }
        }
    }
    .img{
		@include fontMedia;
	}
}
.infographic{
    @include video-info-promo;
	.icon{
		position:absolute;
		z-index: 3;
		bottom: 10px;
		right: 10px;
	}
}
@mixin video-info-promo-1{
    .transp-black{
        position:relative;
        top:0px;left:0px;
		@media only screen and (max-width: 1000px){
			width: 100%;
		}
		@media only screen and (min-width:1001px){
			height: 100%;
			width: 363px;
		}
		h2.title{
			padding: 25px;
			padding-bottom: 0px;
			margin: 0px;
			//font-size: $large-title;
			@media only screen and (max-width: 1000px) {
				//font-size: 25px;
                padding:15px;padding-bottom: 0px;

			}
		}
    }
    
}
@mixin video-info-1{
    @include video-info-promo-1;
    .transp-black{
		h3.subtitle{
			margin: 0px;
			//font-size: 1.4em;
			padding-top: 17px;
			padding-bottom: 20px;
			padding-left: 30px;
            @media only screen and (max-width: 1000px) {
                padding-top:15px;
                padding-bottom: 55px;padding-left: 15px;
			}
		}
        .comment{
            width:95%;
            padding:12px 20px;
            z-index:4;
         	background: rgba(0, 0, 0, 0.7);
            position:absolute;bottom:0px;left:0px;
            .commentCount{
                color:white;
                font-weight:normal;
               // font-size:15px;
                display:inline-block;
                margin-top:2px;
                margin-left:3px;
            }
            @media only screen and (max-width: 1000px) {
                width:100%;
                padding-left:15px;padding-right:15px;
                padding-top:10px;padding-bottom:8px;
            }
            .icon-bar-grey{
                margin-left:10px;margin-right:8px;
            }
        }
	}
}
/* 
Inforgraphic
*/
@mixin i-small{
    @media only screen and (max-width: 1000px){
        padding:8px;
    }
	.transp-black{
        @media only screen and (max-width: 1000px) {
		    width: 100%;
            padding:10px;padding-bottom:0px;
        }
		h2.title{
			@media only screen and (max-width: 1000px) {
                -webkit-line-clamp: 5;
			}
		}
		h3.subtitle{
			@media only screen and (max-width: 1000px) {
				padding-top: 10px;
				padding-bottom: 0px;
			}
		}
	}
}
/*
Common css
*/
.i-v-p-2{
    .transp-black{
		padding: 20px;
		width: 100%;	
        top:0px;left:0px;
		padding-bottom: 0px;
		h2.title{
			//font-size: 30px;
			padding: 0px;
			margin: 0px;
		}
    }
}
.i-v-p-3{
    .transp-black{
        top:0px;left:0px;        
		padding: 15px;
        padding-bottom: 0px;
        @media only screen and (min-width:1001px){
			height: 100%;
			width: 50%;
		}
		h2.title{
			//font-size: 20px;
			//font-family: Helv-LTStd-LtCn, sans-serif;
			padding: 0px;
			margin: 0px;
		}
    }
}
.i1{
    @include big-height;
    @extend .infographic;
    @include video-info-1;
    @include font-size-1;

}
.i2{
    @include med-height;
    @extend .infographic;
    @extend .i-v-p-2;
	.transp-black{
		h3.subtitle{
			margin-top: 0%;
			padding: 0%;
			padding-top: 20px;
			padding-bottom: 20px;
		}
	}
    @include i-small;
    @include font-size-2;

}
.i3{
    @include small-height;
    @extend .infographic;
    @extend .i-v-p-3;
	.transp-black{
		h3.subtitle{
			margin-top: 0%;
			padding: 0%;
			padding-top: 20px;
			display: block;
		}
	}
    @include i-small;
    @include font-size-3;

}
/*
Video
*/
.video{
    @include video-info-promo;
    .iframe{
		@include fontMedia;
	}
	.icon{
		position:absolute;
		z-index: 3;
		top: 50%;
		left: 50%;
	}
}
@mixin video-promo-small{
    @media only screen and (max-width: 1000px){
        .transp-black{
            h2.title{
                color:black;
                -webkit-line-clamp: 2;
            }
            height:42.222%;
            color:black;
            background-color: white;
        }
        .img{
            top:auto;
            bottom:0px;
            height:57.778%
        }
    }
}
@mixin video-small{
    @include video-promo-small;
    @media only screen and (max-width: 1000px){
        .transp-black{
            h2.title{
                -webkit-line-clamp: 3;
            }
            h3.subtitle{
                padding-top: 5px;
		    } 
        }
        .icon{
		    top: auto;
		    left: 50%;
            bottom:28.899%;
            margin-bottom:-22.5px;
        //    margin-left:-11.5px;
	     }
    }
}
.v1{
	@include big-height;
    @extend .video;
    @include video-info-1; 
	.icon{
		margin-top: -35px;
		margin-left: -35px;
        @media only screen and (max-width: 1000px){
          //  margin-top: -17.5px;
		  //  margin-left: -17.5px;
        }
        
	}
    @include font-size-1;

}
.v3{
	@extend .i3;
    @extend .video;
    .icon{
		left: 75%;
        top:50%;
        margin-top: -22.5px;
		margin-left: -22.5px;
	}
    @include video-small;
    @include font-size-3;
}
.v2{
    @extend .i2;
    @extend .video;
    .icon{
		left: 50%;
        top:auto;
        bottom:26%;
        margin-top: -22.5px;
		margin-left: -22.5px;
	}
	.transp-black{
        h2.title{
            color:black;
            -webkit-line-clamp: 2;
        }
		background-color: white;
        height:140px;
	}
    .img{
        height:230px;
        top:auto;
        bottom:0px;
    }
    @include video-small;
    @include font-size-2;
}
/*
Promo
*/
@mixin button-promo{
    .transp-black{
        .button{
	       border: none;
	       background-color: $color-promo;
	       color: #ffffff;
           position:relative;
           padding-top:7px;
           padding-bottom:3px;
           padding-left:13px;
           margin-top:10px;
           text-align:left;
           margin-bottom:20px;
           text-transform:uppercase;
           i{
                position:absolute;right:3px;
           }
        }
    }
}
@mixin promo-small{
    @include i-small;
    @include video-promo-small;
    .transp-black{
        padding:10px;
        .button{
            i{
                top:6px;right:10px;
            }
            @media only screen and (max-width: 1000px) {
                font-size: 9px;
                width:100px;
                i{
                    top:3px;right:5px;
                }
            }
        }
    }
}
.p1{
	@include big-height;
    @include button-promo;
    @include video-info-promo;
    @include video-info-promo-1;
	.transp-black{
		.button{
            margin-left:25px;
            font-size: 18px;
            width:200px;
            i{
                top:9px;right:10px;
            }
            @media only screen and (max-width: 1000px) {
                margin-left:15px;
                font-size: 10px;
                width:130px;
                i{
                    top:6px;
                }
            }
		}
	}
	img{
		width: 100%;
	}
    @include font-size-1;
}

.p2{
	@include med-height;	
    @include button-promo;
    @include video-info-promo;
    @extend .i-v-p-2;
	.transp-black{
		h2.title{
			-webkit-line-clamp: 3;
		}
        .button{
            width:180px;
            font-size: 15px;
		}
	}
    @include font-size-2;
    @include promo-small;

}
.p3{
	@include small-height;
    @include button-promo;
    @include video-info-promo;
    @extend .i-v-p-3;
	.transp-black{
		h2.title{
			-webkit-line-clamp: 3;
		}
        .button{
            width:150px;
            font-size: 13px;
		}
	}
    @include font-size-3;
    @include promo-small;

}
/*
Load More button
*/
.load-more{
    @include main-block;
    margin-top:30px;
    margin-bottom:55px;
    text-align:center;
    i.left{
        margin-top:-4px;
        position:absolute;
        left:20px;
    }
    i.right{
        margin-top:-4px;
        position:absolute;
        right:20px;
    }
    @media only screen and (max-width: 1000px) {
        width:100%;
        margin-top:15px;
        margin-bottom:30px;
    }

}
