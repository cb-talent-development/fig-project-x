/*
RESUME HERO POP
*/

.template-resume{
	.section{
		.content{
			ul{
				top: 60px !important;
			}
			.label{
				padding-bottom: 20px;
				font-style: italic;
			}
			.scrollable-content{
				width:80%;
				height:82%;
				@media(max-width: 479px){
					width: 100%;
					height: 80%;
				}
			}
			.scrollable{
				height:100%;
			}
			.save{
				text-align:center;
				text-transform:uppercase;
				height:100%;
				width:303.5px;
				@media(max-width: 479px){
					width: 100px;
				}
				@include inline-top;
			}
			.pages{
				text-align:center;
				@media(max-width: 479px){
					border-top: 1px $grey solid;
					padding-top: 13px;
				}
				.activated{
					background-color:$navcolor !important;
				}
				.ball{
					margin:0px 5px;
					@include inline-top;
					width:15px;height:15px;border-radius:10px;
					background-color:$lightBlue;
					@media(max-width: 479px){
						width:10px;height:10px;border-radius:7px;
					}
				}
				.ball:hover{
					cursor:pointer;
				}
			}
		}
		.template-content{
			.icon-arrow-left-big-blue,.icon-arrow-right-big-blue{
				position:absolute;top:64%;margin-top:-26px;
				@include no_select;
				@media(max-width: 479px){
					top: 96%;
				}
			}
			.icon-arrow-left-big-blue{
				left:40px;
			}
			.icon-arrow-right-big-blue{
				right:40px;
			}
			i:hover{
				cursor:pointer;
			}
		}
	}
}
.export-resume, .template-resume{
	.section{
		width: 1170px !important;
		.header{
			padding-bottom: 20px;
		}
		.content{
			border-bottom: 1px solid $grey;
			.divider{
				width: 50%;
				margin-bottom: 20px;
				.button, input{
					height: 60px;
					width: 500px;
					font-size: 30px;
					padding:15px;
				}		
				input{
					@include invert-shadow;
				}
			}
		}
	}
}

/*
HEADER TOOL
*/
.tool{
	.header-tool.resume-hero{
        padding-bottom:0px;
		.title-social{
			width: 100%;
			float: none;
			margin: auto;
			.social{
				.icon-save, .icon-bar-grey,.icon-question-big {
					display: none !important;
				}
			}
            .icon-arrow-right-big-grey{
                left:531px;
            }
		}
		.help{
			display: none !important;
		}
	}
}



