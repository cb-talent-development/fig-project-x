/*
NAVBAR (extension menu)
*/

.nav-menu{
    @media only screen and (max-width: 768px){
        display:none;
    }
	background-color: $navcolor;
	width: 100%;
	position: absolute;
	z-index: 1000;
	top: 61px;
	border-top: 1px solid $lightnavcolor;
	-webkit-box-shadow:0px -1px $darknavcolor;
	padding-bottom: 50px;
	display: none;
	.content{
		@media (min-width: 1200px) { width: 1170px;}   
		@media (max-width: 1200px) and (min-width: 768px) { width: 940px; }
		@media (max-width: 979px) and (min-width: 768px) { width: 750px;} 
		margin: 0px auto;
		padding-top: 54px;
		.section{
			@inline-top;
			@media (min-width: 1200px) { width: 300px; padding-left: 30px;}   
			@media (max-width: 1200px) and (min-width: 980px) { width: 240px; padding-left: 20px;}
			@media (max-width: 979px) and (min-width: 768px) { width: 190px; padding-left: 10px;} 
			height: 140px;
			border-left: 1px dashed rgb(49,71,172);
			font-family: HelveticaCondensed, sans-serif;
			h3{
				margin: 0px;
				margin-bottom: 30px;
				color: $orange-5;
				font-size: 1.4em;
				@media (max-width: 979px) and (min-width: 768px) { font-size: 1.2em;} 
				font-family: HelveticaCondensed, sans-serif;
			}
			ul{
				margin: 0px;
				padding: 0px;
				li{
					list-style-type: none;
					margin: 0px;
					margin-top: 8px;
					margin-left: 4px;
					padding: 0px;
					a{
						color: white;
						font-size: 1.0em;
						@media (max-width: 979px) and (min-width: 768px) { font-size: 0.8em;} 
					}
					a:hover{
						text-decoration: none;
						color:rgba(255,255,255,0.8);
					}
				}
				li:nth-child(1){
					margin-top: 0px;
				}
			}
		}
		.section:last-child{
			@media (min-width: 1200px) { width:270px; }   
			@media (max-width: 1200px) and (min-width: 980px) { width: 220px;}
			@media (max-width: 979px) and (min-width: 768px) { width: 180px;} 
		}
	}
}




