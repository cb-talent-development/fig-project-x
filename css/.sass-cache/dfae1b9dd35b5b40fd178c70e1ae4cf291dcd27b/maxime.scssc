//Blue
$blue: rgb(9,51,61);
$dark-blue: rgb(0,31,41); 
@mixin back-blue{
    @include colorCommon;
    background-color:$blue !important;color:white;
}
.back-blue{
    @include back-blue;
}
.back-blue-no-hover{
    @include back-blue;
}
.back-blue:hover{
    cursor:pointer;
    background-color:$dark-blue !important;
	color: white !important;
}

