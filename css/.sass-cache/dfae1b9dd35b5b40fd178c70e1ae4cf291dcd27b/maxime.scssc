/*
NAVBAR
*/

.nav-menu{
	background-color: $navcolor;
	width: 100%;
	position: absolute;
	z-index: 1000;
	top: 61px;
	border-top: 1px solid $lightnavcolor;
	-webkit-box-shadow:0px -1px $darknavcolor;
	padding-bottom: 50px;
	display: none;
	.content{
		@media (min-width: 1200px) { width: 1170px;}   
		@media (max-width: 1200px) and (min-width: 768px) { width: 940px; }
		@media (max-width: 979px) and (min-width: 768px) { width: 750px;} 
		margin: 0px auto;
		padding-top: 54px;
		.section{
			@inline-top;
			@media (min-width: 1200px) { width: 300px; padding-left: 30px;}   
			@media (max-width: 1200px) and (min-width: 980px) { width: 240px; padding-left: 20px;}
			@media (max-width: 979px) and (min-width: 768px) { width: 190px; padding-left: 10px;} 
			height: 140px;
			border-left: 1px dashed rgb(49,71,172);
			font-family: HelveticaCondensed, sans-serif;
			h3{
				margin: 0px;
				margin-bottom: 30px;
				color: $orange-5;
				font-size: 1.4em;
				@media (max-width: 979px) and (min-width: 768px) { font-size: 1.2em;} 
				font-family: HelveticaCondensed, sans-serif;
			}
			ul{
				margin: 0px;
				padding: 0px;
				li{
					list-style-type: none;
					margin: 0px;
					margin-top: 8px;
					margin-left: 4px;
					padding: 0px;
					a{
						color: white;
						font-size: 1.0em;
						@media (max-width: 979px) and (min-width: 768px) { font-size: 0.8em;} 
					}
					a:hover{
						text-decoration: none;
						color:rgba(255,255,255,0.8);
					}
				}
				li:nth-child(1){
					margin-top: 0px;
				}
			}
		}
		.section:last-child{
			@media (min-width: 1200px) { width:270px; }   
			@media (max-width: 1200px) and (min-width: 980px) { width: 220px;}
			@media (max-width: 979px) and (min-width: 768px) { width: 180px;} 
		}
	}
}


/*
HEADER BUTTONS
*/

.dropdown{
	margin: 0px;
	padding: 0px;
	position:absolute; top:42px;
	-webkit-box-shadow:0px 1px 1px rgba(0,0,0,0.2);
	li{
		list-style-type: none;
		margin: 0px;
		padding: 10px 15px;
		width: 100%;
	}
	a:hover{
		text-decoration:none;
	}
}

.dropdown > li{
	b{
		position: relative;
		margin-top: -20px;
		margin-left: 10px;
	}
	li:nth-child(odd){
		background-color: #eee;
	}
}

.dubble.open{
	@media (min-width: 768px){
		width: 369px !important;
	}
	@media (max-width: 767px){
		width: 300px !important;
	}
}
.dubble.open + div{
	display: none;
}

.toggleMenu:not(.dubble){
	ul{
		z-index: 100;
		li:nth-child(odd){
			background-color: #eee;
		}
		a{
			color: #888;
		}
	}
}

.dubble{
	ul{
		z-index: 101 !important;
		li{
			position: relative;
			width: 50%;
			float:left;
			b,i{
				position:absolute;
				margin-top: -5px;
				right:15px;
			}
		}
		li:nth-child(4n + 2){
			background-color: #eee;
		}
		li:nth-child(4n + 1){
			background-color: #eee;
		}
	}
}

