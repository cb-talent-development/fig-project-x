			}
			.button:hover{
				color: $grey !important;
				.icon-check-big{
					display:none;
				}
				.icon-check-lightgreen{
					display: block;
				}
			}
		}
		.resume-content{
			@include whitebox;
			width: 770px;
			margin-left: 30px;
			display: inline-block;
			font-family:"Times New Roman",Georgia,Serif;
			@media(max-width: 768px){
				width: 100%;
				margin: 0px;
				margin-top: 10px;
			}
			h2{
				font-size: 20px;
				background-color: rgb(199, 199, 199);
				margin: 0px;
				padding: 20px;
				font-family:"Times New Roman",Georgia,Serif;
			}
			.presentation{
				padding: 20px;
				.name{
					font-size: 20px;
					font-weight: 500;
					@media(max-width: 768px){
						font-size: 15px;
					}
				}
				ul{
					padding-left: 20px;
					list-style-type: diamond;
					li{
						padding: 0px 25px;
						padding-left:0px;
						@media(max-width: 768px){
							font-size: 12px;
							padding-right: 18px;
						}
					}
				}
			}
			h3{
				text-align: center;
				margin: 20px 0px;
				font-weight: 900;
				font-size: 15px;
				font-family:"Times New Roman",Georgia,Serif;
				@media(max-width: 768px){
					font-size: 12px;
				}
			}
			ul.sortable{
				margin: 0px;
				padding: 0px;
				list-style-type: none;
				li.element{
					margin: 0px;
					padding: 0px;
					border: solid 2px white;
					position:relative;
					background-color: white;
					@media(max-width: 479px){
						font-size: 12px;
					}
					h4{
						text-align: center;
						padding: 0px;
						font-weight: 800;
						font-size: 16px;
						font-family:"Times New Roman",Georgia,Serif;
						@media (max-width: 768px){
							font-size: 14px;
						}
                        @media (max-width: 480px){
							width:65%;
                            margin-left:35%;
						}
					}
					.icons{
						position: absolute;
						top: 0px;
						left: 30px;
						@media (max-width: 768px){
							left: 20px;
						}
						.button{
							width: 30px;
							height: 30px;
							margin-left: 10px;
							i{
								top: 5px;
								left: 4px;
							}
							@media (max-width: 768px){
								width: 19px !important;
								height: 19px;
								padding: 0px;
								margin-left: 7px;
								i{
									top: 2px;
									left: 2px;
								}
							}
						}
						.button:first-child{
							margin-right: 10px;
							margin-left: 0px;
							@media (max-width: 768px){
								margin-right: 7px;
							}
						}
						.separation{
							width: 1px;
							height: 30px;
							border: 1px dotted $grey;
							display: inline-block;
							@media (max-width: 768px){
								height: 18px;
							}
						}
					}
					.section-content{
						margin: 30px;
						@media(max-width: 768px){
							margin:20px;
						}
					}
				}
				.element:hover{
					border: solid 2px rgb(230, 230, 230);
					cursor: pointer;
				}
			}
		}
	}
}

.get-started-resume{
	.section{
		.content{
			.label{
				font-style: italic;
				max-width: 900px;
				white-space: normal;
			}
			textarea#role, textarea#technical-skills, textarea#extra-skills{
				@include invert-shadow;
				width: 100%;
				height: 150px;
				margin-left: 0px;
				font-size: 25px;
				padding: 15px;
				@media only screen and (max-width: 768px){font-size: 17px;}
			}
			textarea#extra-skills{
				height: 400px;
                @media only screen and (max-width: 768px){
                    height:250px;
                }
			}	
			.little{
				width: 173px;
				margin-right:25px;
				@media only screen and (max-width: 768px){
					width: 49%;
					margin-right:2%;
				}
			}
			.little.last{margin-right: 0px;}
			.medium{
				.input-button, .select{
					width: 100%;
				}
			}
            .message{
                @media only screen and (max-width: 768px){
                    margin-top:5px;
                }
            }
			.large-resp{
				@media only screen and (max-width: 768px){width: 100%;}
			}
			.buttons-changes{
                margin-top:15px;
				.button-change{
					margin-bottom: 20px;
					.title{
						color: $grey;
					}
					.txt{margin-top: 10px;color:$grey;margin-left:30px;font-size:14px;}
				}
                @media only screen and (max-width: 768px){
                    margin-top:10px;
                    .button-change{
					   margin-bottom: 10px;
                    }
                }
				.button-change.selected{
					.title{
						color: black;
					}
					.txt{color:black;}
				}
				.button-change.medium{
					@media only screen and (max-width: 768px){
						width: 100%;
						margin-right: 0px;
					}
				}
				.first{
					margin-top: 0px;
				}
				
			}
		}
		.margin-bottom{
			@media only screen and (min-width: 768px){
				margin-bottom: 25px !important;
			}
		}
		.margin-top{
			@media only screen and (min-width: 768px){
				margin-top: 25px !important;
			}
		}
	}
}
