@mixin shadow-effect{
    box-shadow:0 1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow:0 1px 1px rgba(0,0,0,0.2);
    border:none;
}

@mixin shadow{
    @include shadow-effect;
    background:white;
}
@mixin invert-shadow-effect{
    box-shadow:0px -1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow:0px -1px 1px rgba(0,0,0,0.2);
    border:none;
}
@mixin invert-shadow{
    @include invert-shadow-effect;
    background:white;
}
@mixin whitebox{
	background-color: white;
    @include shadow;
}

@mixin no_select{
   -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
@mixin inline-top{
   display:inline-block;
   vertical-align:top;
}
@mixin colorCommon{
    padding-top:12px;padding-bottom:8px;
    @include box-shadow(0px,1px,1px,0px,0.3);
}
@mixin box-shadow($left,$bottom,$spread,$thickness,$opacity){
    box-shadow:$left $bottom $spread $thickness rgba(0,0,0,$opacity);
    -webkit-box-shadow: $left $bottom $spread $thickness rgba(0,0,0,$opacity);
}

.visible-xs{
	@media only screen and (min-width: 768px){
		display: none !important;
	}
}
.hidden-xs{
	@media only screen and (max-width: 767px){
		display: none !important;
	}
}

@mixin main-block{
    width:$total-width;
    display:block;
    margin:auto;
	@media only screen and (max-width: 1200px) and (min-width: 980px){
        width:975px;
    }
	@media only screen and (max-width: 979px) and (min-width: 768px){
        width:765px;
    }
    @media only screen and (max-width: 767px){
        width:300px;
    }
}

