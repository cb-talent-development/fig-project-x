/*
RESUME HERO POP
*/

.template-resume{
	.section{
		.content{
			ul{
				top: 60px !important;
			}
			.label{
				padding-bottom: 20px;
				font-style: italic;
			}
			.scrollable-content{
				width:80%;
				height:82%;
				@media(max-width: 479px){
					width: 100%;
					height: 80%;
				}
			}
			.scrollable{
				height:100%;
			}
			.save{
				text-align:center;
				text-transform:uppercase;
				height:100%;
				width:303.5px;
				@media(max-width: 479px){
					width: 100px;
				}
				@include inline-top;
			}
			.pages{
				text-align:center;
				@media(max-width: 479px){
					border-top: 1px $grey solid;
					padding-top: 13px;
				}
				.activated{
					background-color:$navcolor !important;
				}
				.ball{
					margin:0px 5px;
					@include inline-top;
					width:15px;height:15px;border-radius:10px;
					background-color:$lightBlue;
					@media(max-width: 479px){
						width:10px;height:10px;border-radius:7px;
					}
				}
				.ball:hover{
					cursor:pointer;
				}
			}
		}
		.template-content{
			.icon-arrow-left-big-blue,.icon-arrow-right-big-blue{
				position:absolute;top:64%;margin-top:-26px;
				@include no_select;
				@media(max-width: 479px){
					top: 96%;
				}
			}
			.icon-arrow-left-big-blue{
				left:40px;
			}
			.icon-arrow-right-big-blue{
				right:40px;
			}
			i:hover{
				cursor:pointer;
			}
		}
	}
}
.export-resume, .template-resume{
	.section{
		width: 1170px !important;
		.header{
			padding-bottom: 20px;
		}
		.content{
			border-bottom: 1px solid $grey;
			.divider{
				width: 50%;
				margin-bottom: 20px;
				.button, input{
					height: 60px;
					width: 500px;
					font-size: 30px;
					padding:15px;
				}		
				input{
					@include invert-shadow;
				}
			}
		}
	}
}

.get-started-resume{
	.section{
		width: 1204px !important;
		@media(max-width: 767px){
			width: 470px !important;
		}
		@media(max-width: 480px){
			width: 300px !important;
		}
		.header{
			.title{
				font-weight: normal;
			}
			.label{
				display: inline-block;
				font-size: 27px;
				@media(max-width: 479px){
					font-size: 13px;
				}
			}
		}
		.button-container{
			margin-left: -20px;
		}
		.content{
			border-bottom: 1px solid $lightGrey;
			margin-right: -20px;
			@media (max-width: 480px){
				padding:10px;
			}
			.clear{
				margin-bottom: 20px;
			}
			.icon-step-1, .icon-step-2, .icon-step-3, .icon-step-4, .icon-step-5{
				margin-right: -28px;
				padding: 21px 95px;
				color: white;
				font-size: 28px;
				@media(max-width: 479px){
					font-size: 20px;
					padding: 8px 45px;
				}
			}
			.icon-step-1{
				padding: 21px 70px;
				margin-bottom: 20px;
				@media(max-width: 479px){
					padding: 8px 32px;
				}
			}
			.icon-step-1 > div:not(.content){
				margin-bottom: 0px !important;
			}
			.icon-step-5{
				padding-right: 0px;
			}
			input, .button, textarea{
				@include invert-shadow;
				margin-left: 25px;
				width: 370px;
				height: 70px;
				border: none;
				display: inline-block;
				padding: 10px;
				font-size: 30px;
				text-overflow: ellipsis;
				@media (max-width: 767px)and(min-width: 480px){
					margin-left: 10px;
					height: 50px;
					font-size: 20px;
					width: 210px !important;
				}
				@media (max-width: 479px){
					margin-left: 10px;
					height: 30px;
					font-size: 20px;
					width: 135px !important;
				}
			}
			.button{
				@include shadow;
				margin-right: -6px;
				padding-top: 17px;
				ul{
					top: 70px;
				}
			}
			.button:not(.open){
				overflow: hidden;
			}
			textarea{
				margin-top: 20px;
			}
			.margin-top{
				margin-top: 20px;
			}
			.link{
				@media(max-width: 479px){
					padding-top: 4px;
				}
				.subMenu{
					@media(max-width: 479px){
						top: 31px;
						li{
							height: 30px;
							padding: 5px 10px;
							font-size: 19px;
						}
					}
				}
			}
		}
		.divider{
			width: 390px;
			padding: 20px;
			padding-left: 0px;
			padding-right: 25px;
			input, .button{
				width:100%;
			}
			input{	
				margin-left: 0px;
			}
			.label{
				margin-bottom: 20px;
			}
			.button.date{
				width: 175px;
			}
			.button.first{
				margin-left: 0px;
			}
		}
	}
	.buttons-changes{
		width: 100%;
		margin-top: 20px;
		.button-change{
			width: 33%;
			color: rgb(130, 130, 130);
			.title{
				font-size: 27px;
			}
			.txt{
				padding: 10px 42px;
				padding-right: 35px;
				font-family: HelveticaRoman, sans-serif;
				font-size: 16px;
			}
			.carre{
				height: 30px;
				width: 30px;
				@media(max-width: 479px){
					height: 15px;
					width: 15px;
				}
			}
		}
		.button-change.selected{
			.title{
				color: black;
			}
		}
		.button-change:not(.selected){
			.carre{
				border-top: 1px outset rgb(200, 200, 200);
			}
		}
		.button-change.txt, .button-change.title{
			color: rgb(130, 130, 130);
		}
	}
	.footer-dialog{
		.button:first-child{
			margin-right: 20px;
		}
		.inactive{
			opacity: 0.5;
		}
		#next-button{
			padding-right: 35px;
		}
		i{
			top: 14px;
		}
		@media(max-width:479px){
			padding: 10px;
			.button:first-child{
				margin-right: 10px;
			}
			.button{
				font-size: 15px;
				padding: 5px;
			}
			#next-button{
				padding-right: 20px;
			}
			i{
				top: 7px;
				right: 7px;
			}
		}
	}
}


