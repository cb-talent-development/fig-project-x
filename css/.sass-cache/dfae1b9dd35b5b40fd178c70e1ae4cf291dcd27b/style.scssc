/*
General social CSS
*/
.social{
    .icon-social{
        @include inline-top;
        border-radius:30px;
        margin-right:12px;
    }
    .icon-social:hover{
        cursor:pointer;
        @include shadow-effect;        
    }
    .icon-save{
        margin-right:3px;
    }
    .icon-bar-grey{
        margin:9px;
    }
}


/*
Post page css
*/
@mixin full-size-phone{
    @media only screen and (max-width: 768px){
        width:100%;
    }
}
/*
Related 
*/
.related{
    h2.related-title{
        @include main-block;
        font-size:22px;text-transform:uppercase;margin-top:50px;margin-bottom:50px;
    }
    .related-container{
        @extend .home-container;margin-top:5px;
        .a-small,.a3,.i3,.v3,.p3{
            @include med-width;
        }
        margin-bottom:50px;
    }
}
.bottom-post{
        margin-top:25px;
}
/*
POST CSS
*/
.post{
    @include main-block;
    .title{
        font-size:90px;
        margin-top:57px;margin-bottom:37px;
        line-height:90px;
        width:80%;
        min-width:240px;
        margin-left:0px;
        @include full-size-phone;
    }
    .complement{
        width:31.5%;
        float:right;
        @include full-size-phone;
        .social,.rating,.comments{
            @include shadow;
            display:block;
            width:100%;
        }
        .social{
            height:90px;margin-bottom:30px;padding-top:23px;
            padding-bottom:23px;padding-left:18px;
            .icon-social{
                @include inline-top;
               // height:40px;width:40px;background-color:blue;border-radius:30px;
                margin-right:12px;
            }
            .icon-social:hover{
                cursor:pointer;
                @include shadow-effect;
                
            }
            .icon-save{
                margin-right:3px;
            }
            .icone-bar{
                margin-top:8px;
            }
        }
        .rating{
            padding:0px;padding-top:23px;
            padding-bottom:23px;
            text-align:center;
            margin-bottom:30px;
            .icon{
                margin:0px 4px;
            }
        }
        .comments{
            min-height:40px;margin-bottom:0px;
        }
    }
    .img-background{
        float:left;
        background:black;
        margin-bottom:47px;
        width:100%;
        height:500px;
        .img{
            width:100%;
            height:100%;
        }
    }
    .infographic,.content{
        width:65.6%;
        @include full-size-phone;
    }
    .content{
        float:left;
        .intro{
            font-size:27px;
            line-height:36px;
            text-transform:uppercase;
            padding-bottom:3px;
        }
        .hightlight{
            background-color:yellow;
            text-decoration:underline;
            padding-top:5px;padding-bottom:2px;padding-left:2px;padding-right:2px;
        }
        img,.img{
            margin-top:20px;margin-bottom:10px;
            width:100%;
        }
        h3{
            margin-top:30px;margin-bottom:15px;
            font-size:18px;
        }
        p{
            font-size:18px;
            line-height:24px;
            margin:0px;
            padding-top:11px;
            padding-bottom:16px;
        }
    }
    /*
    Comment-section
    */
    .comments{
        .new-comment{
            padding:20px;
            position:relative;
            border-bottom:1px solid $lightGrey;
            .img-user{
                float:left;
                width:40px;height:40px;
            }
            .input{
                float:left;
                position:relative;
                height:40px;
                width:287px;
                background-color:$backgroundGrey;color:black;
                input{
                    z-index:4;
                    padding:10px;
                    position:absolute;
                    padding:10px;
                    top:0px;left:0px;width:100%;
                    background-color:rgba(0,0,0,0);
                    height:40px;
                    border:none;
                }
                .placeholder{
                    z-index:1;
                    text-transform:capitalize;
                    position:absolute;
                    line-height:40px;height:40px;padding-left:10px;
                    top:0px;left:0px;width:100%;
                }
            }
        }
        .comments-list{
            padding:20px;
            padding-bottom:0px;
            padding-right:0px;
            background-color:white;
            .level-2 .content,.level-3 .content{
                border-left:1px solid $backgroundGrey;               
            }
            .level-2 .img{
                margin-right:20px;
            }
            .level-3 .img{
                margin-right:40px;
            }
            .level-1 .content{
                width:86%;
                .new-comment .input{
                    width:85%;
                }
            }
            .level-2 .content{
                width:81%;
                .new-comment .input{
                    width:84%;
                }
            }
            .level-3 .content{
                width:73%;
                .new-comment .input{
                    width:82%;
                }
            }
            .separator{
                height:1px;
                border-bottom:1px solid $backgroundGrey;
                margin-bottom: 15px;
                margin-top:15px;
                margin-left:-20px;
            }
            .comment{
                font-size:13px;
                .img{
                    height:40px;width:40px;float:left;
                }
                .name{
                    font-weight:bold;
                    margin-right:12px;
                }
                .time{
                    color:$lightGrey;
                }
                .statement{
                    margin:10px 0px;
                }
                .reply{
                    position:relative;
                    margin-right:5px;
                    text-transform:capitalize;
                    font-weight:bold;
                    color:$orange;
                }
                .reply:hover{
                    cursor:pointer;
                    text-decoration:underline;
                }
                .closeReply{
                    @extend .reply;
                    color:rgb(212,98,104);
                }
                .content{
                    padding-left:20px;
                    float:left;
                    transition: padding-bottom, 0.5s;
                    padding-bottom:30px;
                    .new-comment{
                        width:100%;
                        border:none;
                        padding:15px 0px;
                        display:none;
                        .placeholder{
                            color:$grey;
                        }
