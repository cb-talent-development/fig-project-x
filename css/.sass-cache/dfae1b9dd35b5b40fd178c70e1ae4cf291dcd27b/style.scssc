/*
Footer css
*/
.footer{
	background-color: #cccccc;
    .container{
        margin:auto;
        padding-top:80px;
        padding-bottom:50px;
    }
	div.section{
		h4{
			margin-bottom: 50px;
			mark{ 
				background-color: #ffff00; 
			}
		}
		div.column{
			width: 50%;
			float: left;
			a{
				color: #444444;
				text-decoration: underline;
				display: block;
			}
		}
	}
}

a.transp-link{
	position: absolute;
	width: 100%;
	height: 100%;
	opacity: 0;
	z-index: 5;
	top: 0;
	left: 0;
}


/*
General social CSS
*/
.social{
    .icon-social{
        @include inline-top;
        border-radius:30px;
        margin-right:12px;
    }
    .icon-social:hover{
        cursor:pointer;
        @include shadow-effect;        
    }
    .icon-save{
        margin-right:3px;
    }
    .icon-bar-grey{
        margin:9px;
    }
}


/*
Post page css
*/
@mixin full-size-phone{
    @media only screen and (max-width: 768px){
        width:100%;
    }
}
/*
Related 
*/
.related{
    h2.related-title{
        @include main-block;
        font-size:22px;text-transform:uppercase;margin-top:50px;margin-bottom:50px;
    }
    .related-container{
        @extend .home-container;margin-top:5px;
        .a-small,.a3,.i3,.v3,.p3{
            @include med-width;
        }
        margin-bottom:50px;
    }
}
.bottom-post{
        margin-top:25px;
}
/*
POST CSS
*/
.post{
    @include main-block;
    .title{
        font-size:90px;
        margin-top:57px;margin-bottom:37px;
        line-height:90px;
        width:80%;
        min-width:240px;
        margin-left:0px;
        @include full-size-phone;
		@media (max-width: 1200px) and (min-width: 980px){
			font-size: 80px;
			line-height:80px;
			margin-top:40px;margin-bottom:30px;
		}
		@media (max-width: 979px) and (min-width: 768px){
			font-size: 65px;
			line-height:65px;
			margin-top:25px;margin-bottom:20px;
		}
		@media (max-width: 767px){
			font-size: 30px;
			line-height:30px;
			margin-top:15px;margin-bottom:15px;
		}
    }
    .complement{
        width:37%;
        float:right;
        @include full-size-phone;
        .social,.rating,.comments{
            @include shadow;
            display:block;
            width:100%;
        }
		.social,.rating{
			@media (max-width: 767px){
				width:48%;
				float:left;
			}
		}
        .social{
            height:90px;margin-bottom:30px;padding-top:23px;
            padding-bottom:23px;padding-left:5px;padding-right: 5px;
			text-align: center;
			@media (max-width: 1200px) and (min-width: 980px){
				height:70px;padding-top:15px;
				padding-bottom:15.5px;
			}
			@media (max-width: 979px) and (min-width: 768px){
				height:50px;padding-top:13.5px;
				padding-bottom:12.5px;
			}
			@media (max-width: 767px){
				height:75px;margin-bottom:45px;padding-top:23px;
				padding-bottom:23px;
				margin-right: 4%;
			}
			@media (max-width: 480px){
				height:37.5px;margin-bottom:22.5px;padding-top:12px;
				padding-bottom:12px;
			}
            .icon-social{
                @include inline-top;
               // height:40px;width:40px;background-color:blue;border-radius:30px;
                margin-right:10px;
				@media (max-width: 1200px){
					margin-right:5px;
				}
				@media (max-width: 979px){
					margin-right:5px;
				}
				@media (max-width: 767px){
					margin-right:10px;
				}
				@media (max-width: 479px){
					margin-right:2px;
				}
            }
            .icon-social:hover{
                cursor:pointer;
                @include shadow-effect;
            }
            .icon-save{
                margin-right:2px;
            }
            .icon-bar-grey{
                margin-top:8px;
				margin-left:5px;
				margin-right:8px;
				@media (max-width: 1200px){
					margin-top:7px;
					margin-right:5px;
				}
				@media (max-width: 979px){
					margin-top:0px;
					margin-right:5px;
				}
				@media (max-width: 767px){
					margin-top:7px;
					margin-right:15px;
					margin-left:6px;
					margin-right:8px;
				}
				@media (max-width: 479px){
					margin-top:1px;
					margin-left:2px;
					margin-right:4px;
				}
            }
        }
        .rating{
            padding:0px;padding-top:23px;
            padding-bottom:23px;
            text-align:center;
            margin-bottom:30px;
            .icon{
                margin:0px 5px;
            }
			@media (max-width: 1200px) and (min-width: 980px){
				height:70px;padding-top:15px;
				padding-bottom:15.5px;
			}
			@media (max-width: 979px) and (min-width: 768px){
				height:50px;padding-top:13.5px;
				padding-bottom:12.5px;
			}
			@media (max-width: 767px){
				height:75px;margin-bottom:5px;padding-top:20px;
				padding-bottom:14px;
			}
			@media (max-width: 480px){
				height:37.5px;margin-bottom:5px;padding-top:9px;
				padding-bottom:9px;
			}
        }
        .comments{
            min-height:40px;margin-bottom:0px;
        }
    }
    .img-background{
        float:left;
        background:black;
        margin-bottom:47px;
        width:100%;
        height:500px;
        .img{
            width:100%;
            height:100%;
        }
		@media (max-width: 1200px) and (min-width: 980px){
			height:450px;
			margin-bottom:40px;
		}
		@media (max-width: 979px) and (min-width: 768px){
			height:400px;
			margin-bottom:33px;
		}
		@media (max-width: 767px){
			height:300px;
			margin-bottom:20px;
		}
		@media (max-width: 480px){
			height:200px;
			margin-bottom:10px;
		}
    }
    .infographic,.content{
        width:62.6%;
        @include full-size-phone;
    }
    .content{
        float:left;
		padding: 0 10px;
        .intro{
            font-size:27px;
            line-height:36px;
            text-transform:uppercase;
            padding-bottom:3px;
			@media (max-width: 1200px) and (min-width: 980px){
				font-size:25px;
				line-height:33px;
			}
			@media (max-width: 979px) and (min-width: 768px){
				font-size:22px;
				line-height:30px;
			}
			@media (max-width: 767px){
				font-size:17px;
				line-height:27px;
			}
			@media (max-width: 480px){
				font-size:15px;
				line-height:25px;
			}
        }
        .hightlight{
            background-color:yellow;
            text-decoration:underline;
            padding-top:5px;padding-bottom:2px;padding-left:2px;padding-right:2px;
        }
        img,.img{
            margin-top:20px;margin-bottom:10px;
            width:100%;
        }
        h3{
            margin-top:30px;margin-bottom:15px;
            font-size:18px;
			@media (max-width: 1200px) and (min-width: 980px){
				margin-top:25px;margin-bottom:13px;
				font-size:17px;
			}
			@media (max-width: 979px) and (min-width: 768px){
				margin-top:20px;margin-bottom:10px;
				font-size:16px;
			}
			@media (max-width: 767px){
				margin-top:15px;margin-bottom:8px;
				font-size:15px;
			}
			@media (max-width: 480px){
				margin-top:12px;margin-bottom:6px;
				font-size:14px;
			}
        }
        p{
            font-size:18px;
            line-height:24px;
            margin:0px;
            padding-top:11px;
            padding-bottom:16px;
			@media (max-width: 1200px) and (min-width: 980px){
				font-size:17px;
				line-height:23px;
				padding-top:10px;
				padding-bottom:15px;
			}
			@media (max-width: 979px) and (min-width: 768px){
				font-size:16px;
				line-height:21px;
				padding-top:9px;
				padding-bottom:13px;
			}
			@media (max-width: 767px){
				font-size:14px;
				line-height:19px;
				padding-top:8px;
				padding-bottom:12px;
			}
			@media (max-width: 480px){
				font-size:13px;
				line-height:17px;
				padding-top:7px;
				padding-bottom:10px;
			}
        }
    }
    /*
    Comment-section
    */
    .comments{
        .new-comment{
			@media(max-width: 1200px) and (min-width: 980px){
				font-size: 13px;
			}
			@media(max-width: 979px) and (min-width: 768px){
				font-size: 12px;
			}
			@media(max-width: 479px){
				font-size: 12px;
			}
            padding:20px;
            position:relative;
            border-bottom:1px solid $lightGrey;
            .img-user{
                float:left;
                width:40px;height:40px;
            }
            .input{
                float:left;
                position:relative;
                height:40px;
                width:287px;
                background-color:$backgroundGrey;color:black;
                input{
                    z-index:4;
                    padding:10px;
                    position:absolute;
                    padding:10px;
                    top:0px;left:0px;width:100%;
                    background-color:rgba(0,0,0,0);
                    height:40px;
                    border:none;
                }
                .placeholder{
                    z-index:1;
                    text-transform:capitalize;
                    position:absolute;
                    line-height:40px;height:40px;padding-left:10px;
                    top:0px;left:0px;width:100%;
                }
				@media(max-width: 1200px) and (min-width: 980px){
					width: 220px;
					input{
						//height:24px;
						padding:3px;
					}
				}
				@media(max-width: 979px) and (min-width: 768px){
					width: 200px;
					input{
						//height:20px;
						padding: 4px;
					}
				}
				@media(max-width: 479px){
					width: 80%;
					input{
						//height:20px;
						padding: 4px;
					}
				}
            }
        }
        .comments-list{
            padding:20px;
            padding-bottom:0px;
            padding-right:0px;
            background-color:white;
            .level-2 .content,.level-3 .content{
                border-left:1px solid $backgroundGrey;               
            }
            .level-2 .img{
                margin-right:20px;
            }
            .level-3 .img{
                margin-right:40px;
            }
            .level-1 .content{
                width:80%;
                .new-comment .input{
                    width:69%;
            
                }
                @media(max-width: 768px){

                    width:84%;
                    .new-comment .input{
                        width:80%;
            
                    }
                }
            }
            .level-2 .content{
                width:73%;
                .new-comment .input{
                        width:69%;
            
                }
                @media(max-width: 768px){
                    width:78%;
                    .new-comment .input{
                        width:78%;
                    }
                }
            }
            .level-3 .content{
                width:64%;
                .new-comment .input{
                        width:64%;
            
                }
                @media(max-width: 768px){
                    width:70%;
                    .new-comment .input{
                        width:76%;
                        
                    }
                }
            }
            .separator{
                height:1px;
                border-bottom:1px solid $backgroundGrey;
                margin-bottom: 15px;
                margin-top:15px;
                margin-left:-20px;
            }
            .comment{
                font-size:13px;
                .img{
                    height:40px;width:40px;float:left;
                }
                .name{
                    font-weight:bold;
                    margin-right:12px;
                }
                .time{
                    color:$lightGrey;
                }
                .statement{
                    margin:10px 0px;
                }
                .reply{
                    position:relative;
                    margin-right:5px;
                    text-transform:capitalize;
                    font-weight:bold;
                    color:$orange;
                }
                .reply:hover{
                    cursor:pointer;
                    text-decoration:underline;
                }
                .closeReply{
                    @extend .reply;
                    color:rgb(212,98,104);
                }
                .content{
                    padding-left:20px;
                    float:left;
                    transition: padding-bottom, 0.5s;
                    padding-bottom:30px;
                    .new-comment{
                        width:100%;
                        border:none;
                        padding:15px 0px;
                        display:none;
                        .placeholder{
                            color:$grey;
                        }
                    }
                    .closeReply{
                        display:none;
                    }
                    @media(max-width: 768px){
                        padding-left:20px * $rapp-resp;
                        padding-bottom:30px * $rapp-resp;
                    }
                }
            }
            .no-first-bar .separator,.no-second-bar .separator{
                display:none;
            }
            .no-padding-bottom .comment .content{
                padding-bottom:0px;
            }
        }
    }
}

.post-article{
	.video-media,.image-media{
		display: none;
	}
}
.post-video{
	.article-media,.image-media{
		display: none;
	}
	.video-media{
		position: relative;
		.icon-play-button-large{
			position: absolute;
			top: 50%;
			left: 50%;
			z-index: 10;
			margin-left: -35px;
			margin-top: -35px;
		}
		.video-bar{
			bottom: 10px;
			left: 10px;
			z-index: 10;
			height: 40px;
			width: 1100px;
			.icon-play{
				position: absolute;
				left: 25px;
				top: 20px;
			}
			.play-bar{
				height: 10px;
				width: 1030px;
				position: absolute;
				left: 50px;
				top: 15px;
				background-color: rgb(10, 10, 10);
				.played-bar{
					position: absolute;
					width: 40%;
					height: 10px;
					background-color: white;
				}
			}
		}
		.icon-image{
			position: absolute;
			right: 10px;
			bottom: 10px;
		}
	}
}
.post-image{
	.article-media,.video-media{
		display: none;
	}
	.img-background{
        width:65.6%;
		.icon-image{
			position: absolute;
			top: 10px;
			right: 10px;
		}
    }
	.complement{
		position: absolute;
		left: 65%;
		width:26.6% !important;
	}
}

/*
Tools dashboard & tool css
*/
.tools-dashboard,.tool{
    .title{
        .part1{
            color:black;
        }
        .part2{
            color:$grey;
        }
    }
}
.tools-dashboard{
    .first{
        margin-top:60px;
    }
    .last{
        margin-bottom:75px !important;
    }
    .tool{
        @include big-height;
        .link{
            width:100%;
            height:100%;
            z-index:10;
            position:absolute;top:0px;left:0px;right:0px;bottom:0px;
        }
        .link:hover{
            background-color:rgba(100,100,100,0.1);
        }
        .left{
            position:relative;
            height:100%;width:30.77%;
            float:left;
            text-align:center;
            .logo{
                position:absolute;top:25px;left:50%;
            }
            .icon-careerscape-small{
                margin-left:-70px;
            }
            .icon-resumeHero-small{
                margin-left:-73.5px;
            }
            .icon-jobinality-small{
                margin-left:-60px;
            }
            .icon-localygrown-small{
                margin-left:-79px;
            }
            .info{
                width:65%;margin:auto;margin-top:200px;
                .title{
                    font-size:36px;
                }
                .summary{
                    margin-top:20px;font-size:23px;
                }
                .icon-arow-right-light{
                    margin-left:3px;margin-top:2px;
                }
            }
        }
        .right{
            height:100%;
            background-color:$grey;
            width:69.2%;
        }
    }
}
@mixin blue-element{
    background-color:$navcolor;
    color:white;
}
/*
DIALOG
*/
.dialog{
    z-index:300;
    display:none;
    position:absolute;top:0px;left:0px;bottom:0px;right:0px;width:100%;height:100%;
    .background{
        background:rgba(0,0,0,0.5);
        position:fixed;top:0px;left:0px;bottom:0px;right:0px;width:100%;height:100%;
    }
    .section{
        position:relative;display:none;
        width:60%;margin:auto;margin-top:140px;background-color:rgb(243,245,245);
		@media(max-width:479px){
			width: 300px;
		}
        .header{
            position:relative;
            font-size:27px;padding-left:30px;
            padding-right:15px;
            padding-top:20px;
			padding-bottom: 20px;
            font-weight:bold;
            border-bottom:1px solid $lightGrey;
			@media(max-width: 479px){
				font-size: 15px;
				padding: 10px;
				height: 60px;
			}
            .close{
                position:absolute;
				right: 20px;
				top:20px;
				@media (max-width: 479px){
					right:10px;
					top:10px;
				}
            }
        }
        .intro{
            width:100%;
            font-weight:18px;padding:22px;
            border-bottom:1px solid $lightGrey;
            td{vertical-align:middle;}
            .logo{position:relative;top:26px;left:17px;}
            strong{
                font-weight:bold;
            }
        }
        .content{width:100%;}
        .footer-dialog{
            padding:20px;
			.button-save,.button-next,.button-previous{
				position:relative;
				padding:0px 14px;
				padding-top:12px;
				padding-bottom:7px;
			}
			.button-next,.button-previous{
				i{
					position:absolute;
					top:50%;
					margin-top:-8.5px;
				}
			}
			.button-previous[class~=selected],.button-next[class~=selected]{
				opacity:0.5;
			}
			.button-previous{
				padding-left:40px;
				margin-right:15px;
				i{left:15px;}
			}
			.button-next{
				padding-right:40px;
				i{right:15px;}
			}
			@media(max-width:479px){
				padding: 10px;
				.button-save,.button-next,.button-previous{
					font-size: 15px;
					padding:5px
				}
				.button-next,.button-previous{
					i{
						position:absolute;
						top:10px;
						right: 10px;
					}
				}
				.button:first-child{
					margin-right: 10px;
				}
			}
        }
    }
}
/*
Tool CSS
*/
.tool{
    @include main-block;
    /*
    Button for tools
    */
    .header-tool{
        width:83%;
        margin:auto;
        margin-top:60px;
        margin-bottom:60px;
        .title-social{
            @include shadow;
            position:relative;
            float:left;width:840px;margin-right:28px;
            height:94px;
            padding-top:27px;
            padding-bottom:27px;
            .social{
                float:right;
                height:100%;
                margin-right:18px;
            }
            .icon-jobinality-small,.icon-resumeHero-small,.icon-localygrown-small,.icon-careerscape-small{
                position:absolute;
                top:-20px;left:73px;
            }
            .icon-arrow-right-big-grey{
                position:absolute;top:50%;margin-top:-26px;
                left:431px;
            }
            .title{
                top:0px;
                text-transform:capitalize;
                font-size:37px;
                position:absolute;
                line-height:108px;
                height:94px;
                left:238px;
            }
        }
        .help{
            @include shadow;
            float:right;
            height:94px;
            padding:25px;
            width:90px;
            .icon-help:hover{
                cursor:pointer;
            }
        }
        .icon-bar-grey{
            margin:9px;
        }
		@media(max-width: 479px){
			width: 300px;
			height: 300px;
			margin: 0px;
			margin-bottom: 10px;
			margin-top: 10px;
			.title-social{
				width: 100%;
				height: 100%;
				.icon-jobinality-small,.icon-resumeHero-small,.icon-localygrown-small,.icon-careerscape-small{
					position:absolute;
					top:30px;left:50%;margin-left: -80px;
				}
				.title{
					top:160px;
					width: 300px;
					text-align: center;
					text-transform:capitalize;
					font-size:27px;
					line-height:108px;
					height:60px;
					left:0px;
				}
				.social{
					width: 100%;
					height: 30px;
					text-align: center;
					clear: both;
					position: absolute;
					bottom: 0px;
				}
							}
			.icon-bar-grey{
				padding-top: 3px;
			}
			.icon-question{
				display: inline-block !important;
			}
		}
    }
    .buttons_zoom{
        position:absolute;
        z-index:100;
        top:15px;
        left:12px;
        .selected{
            background:rgba(110,110,110,0.8);
        }
    }
    .selector{
        width:100%;
        margin:auto;
        margin-bottom:30px;
		@media(max-width: 479px){
			margin-bottom: 10px;
		}
        .input-button{
            padding-top:21px;padding-bottom:17px;
            padding-left:15px;
            width:100%;height:100%;color:white;
            font-size:22px;
            border:none;background:none;
			@media(max-width: 479px){
				width: 300px;
				padding-top: 12px;
				padding-bottom: 10px;
				font-size:18px;
			}
        }
        .industries,.occupations{
            padding-top:21px;padding-bottom:17px;
            font-size:22px;
