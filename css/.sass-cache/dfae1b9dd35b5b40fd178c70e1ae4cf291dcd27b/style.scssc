        }
        .detail{
            margin-left:52px;//20+32
            width:75%;
        }
        .timeline{
            position:absolute;
            height:100%;
            .dashed-bar{
                width:0px;
                margin-left:17px;
                height:90%;
                border-left:1px dashed $lightGrey;
            }
        }
        .work-experience{
            .period{margin-bottom:3px;}
            .detail{
                padding-bottom:40px;
            }
            .website{
                text-transform:uppercase;
            }
           .occupation,.income{
                text-transform:uppercase;
                padding-left:15px;padding-right:15px;
            }
            .occupation{
                margin-right:10px;
            }
            .income{
                background:rgb(107,212,138);color:white;
            }
        }
        .education{
            .dashed-bar{margin-left:16px;}
            .detail{
                padding-bottom:30px;
            }
        }
        .add-elem,.technology-elem,.skill,.occupation,.income{
            font-size:16px;
            text-align:center;
            padding-top:12px;
            padding-bottom:8px;
        }
        .occupation,.technology-elem{
            @include blue-element;
        }
        .add-elem{
            background:rgb(237,237,237);
            border:1px dashed rgb(190,190,190);
        }
        .skills{
            .left{
                margin-right:26px;
            }
            .add-elem,.skill{
                width:46%;
            }
            .add-elem{margin:0;}
            .skill{
                color:white;
                margin-bottom:20px;
                background:rgb(92,26,71);
            }
        }
        .technology{
            .add-tech,.technology-elem{
                padding-top:12px;
                padding-bottom:8px;
            }
            .technology-elem{
                margin-bottom:20px;
            }
        }
        .career-goals{
            font-size:19px;
            .content{
                .goal{margin-top:31px;width:80%;}
                .first{margin-top:0px;}
            }
        }
    }
    .popup{
        .scrollable{
            //option 1
            width:500%;
            min-width:5 * 1200px;
        }
        .section-wrapper{
            width:20%;
            display:inline-block;
            vertical-align:top;
        }
        .section{
            //width:90%;
            width:1150px;
            .header{
                font-size:24px;
                .grey{
                    color:$grey;
                }
            }
            .step{
                width:1150px;
            }
            .step1{
                display:none;
            }
            .content{
                .custom-radio{
                    margin-right:25px;
                    position:relative;
                    input[type=checkbox] {
                        display: none;
                    } 
                    label{margin-left:40px;}
                    label:hover{ cursor:pointer;}
                    label:before{
                        height:20px;
                        width:20px;
                        content:"";
                        position:absolute;
                        left:0px;
                        top:50%;
                        text-align: center;  
                        margin-top:-15px;
                        @include box-shadow(0.5px,-1px,2px,0px,0.3);
                        border-right:1px solid $lightGrey;
                        background:white;
                    }
                    input[type=radio]:checked + label:before { 
                        background:$orange;
                    }  
                }
                .custom-radio{
                    font-size:23px;
                    color:$darkGrey;
                }
                .message{
                    font-size:17px;
                    text-transform:uppercase;
                    color:rgb(178,178,178);
                    margin-bottom:10px;
                }
                .optional{color:$lightGrey;text-transform:uppercase;font-size:17px;}
                .select,input,input[placeholder]{  
                    padding-top:20px;padding-bottom:16px;
                    font-size:23px;
                }   
                .large{width:370px;margin-right:25px;}
                .medium{width:170px;margin-right:25px;}
                .address{
                    padding:17px;
                    .street{margin-bottom:20px;}
                }
                .general{
                    padding:17px;
                    border-bottom:1px solid $lightGrey;
                    .gender{margin-right:135px;
                        .custom-checkbox{margin-top:15px;}
                    }
                    
                }
                .job-phase{
                    padding:0px 15px;
                    padding-top:25px;
                    .cursor{
                        margin-top:20px;
                        margin-bottom:45px;
                        position:relative;
                        .bar{
                            border-radius:8px;
                            height:15px;
                            background:rgb(231,235,235);
                        }
                        .icon-cursor-orange-down{
                            position:absolute;
                            top:-5px;
                            left:10px;
                            background:orange;
                            height:30px;
                            width:20px
                        }
                    }
                    .phase{
                        opacity:0.6;
                        @include whitebox;
                        width:23.35%;
                        height:295px;
                        text-align:center;
                        margin-bottom:15px;
                        .title{font-size:20px;padding-top:20px;padding-bottom:25px;
                            .logo{
                                margin-bottom:15px;
                            }
                            border-bottom:1px solid $lightGrey;
                        }
                        .description{
                            padding:0px 30px;
                            font-size:15px;
                            line-height:20px;
                            padding-top:25px;
                            @include webkit-box;
                            -webkit-line-clamp: 6;
                        }
                    }
                    .left{margin-right:2.2%;}
                    .before-last{
                        margin-right:0px;
                    }
                     .phase[class~="selected"]{
                        opacity:1;
                    }
                }
                .job-occupation{
                    .large{width:465px;margin:0px;}
                    padding:0px 20px;
                    height:200px;
                    border-bottom:1px solid $lightGrey;
                    .job{
                        height:100%;
                        width:42%;
                        position:relative;
                        .entry{position:absolute;bottom:30px;
                            .message{margin-bottom:10px;}
                        }
                        
                    }
                    .job-correction{
                        height:200px;
                        width:50.7%;
                        position:relative;
                        .logo{
                            position:absolute;
                            bottom:30px;
                        }
                        .correction{
                            position:absolute;
                            bottom:30px;
                            right:0px;
                            .mess{
                                font-size:16px;
                                margin-bottom:10px;
                                text-transform:uppercase;
                                color:$orange;
                                @include webkit-box;
                                -webkit-line-clamp:4;
                            }
                            .wrong{color:black;}
                            .select{border:1px solid $orange;}
                        }
                    }
                }
                .job-info{
                    padding:0px 15px;
                    padding-top:25px;padding-bottom:30px;
                    
                }
            }
            .footer-dialog{
                border-top:1px solid $lightGrey;
				@media(min-width: 480px){
					padding-top:12px;
					padding-bottom:15px;
				}
            }
        }
    }
}
.saved-content-page{
    .saved-content{
        margin-top:65px;
        .content{
            height:auto;
            padding:0px 100px;
            padding-bottom:40px;
            .save:not(.right){
                margin-right:8px;
            }
            .fourth{
                margin-right:0px !important;
            }
            .save{
                height:265px;
                overflow:hidden;
                margin-top:40px;
                .delete{
                    color:$red-delete;
                    margin-top:25px;
                    margin-bottom:10px;
                }
            }
        }
    }
