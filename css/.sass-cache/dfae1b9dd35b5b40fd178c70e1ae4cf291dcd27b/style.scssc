        top:24px;
        right:133px;
        width:38px;
        height:38px;
        border-radius:24px;
		@media(max-width: 479px){
			top:6px;
			right:33px;
			width:9.5px;
			height:9.5px;
			border-radius:6px;
		}		
    }
    .legend .info{
        text-align:left;
        position:absolute;
        bottom:18px;
        left:22px;
		@media(max-width:479px){
			bottom: 5px;
			left: 7px;
		}
    }
    .legend .info .title{
        font-weight:200;
        font-size:18px;
		@media(max-width:479px){
			font-size: 10px;
		}
    }
    .legend .info .comment{
        font-style:italic;
        font-size:12px;
		@media(max-width:479px){
			font-size: 7px;
		}
    }
    /*
    /END Legend part
    */
}

/*
JOB RESULTS/RECOMMENDATIONS
*/
.scrollable-content{
    margin:auto;
    overflow:hidden;
}
.scrollable{
    text-align:left;
    width:10000px;
    .step{
        display:inline-block;
    }
}
.delete-resume{
	.section{
		@media(min-width: 480px){
			width: 450px;
		}
	}
	.save{
		text-align: center;
	}
	.footer-dialog{
		border-top:1px solid $lightGrey;
	}
}

.job-results,.saved-content, .delete-items{
	@include main-block;
	text-align:center;
	position:relative;
	@include shadow;
	margin-bottom:60px;
	background:white;
	@media(max-width:479px){
		margin: 0px;
		margin-bottom:60px;
		width: 300px;
	}
	.icon-arrow-left-big-blue,.icon-arrow-right-big-blue{
		position:absolute;top:50%;margin-top:-26px;
		@include no_select;
		@media(max-width: 479px){
			top: 96%;
		}
	}
	.icon-arrow-left-big-blue{
		left:40px;
	}
	.icon-arrow-right-big-blue{
		right:40px;
	}
	i:hover{
		cursor:pointer;
	}
	.title{
		font-size:20px;
		padding:0px 25px;
		padding-top:22px;
		padding-bottom:20px;
		border-bottom:1px solid $grey;
		@media(max-width: 479px){
			font-size:14px;
			padding:0px 15px;
			padding-top:12px;
			padding-bottom:10px;
		}
	}
	.content{
		position:relative;
		.scrollable-content{
			width:80%;
			height:82%;
			@media(max-width: 479px){
				width: 100%;
				height: 80%;
			}
		}
		.scrollable{
			height:100%;
		}
		.job-recommandation,.save{
			text-align:center;
			text-transform:uppercase;
			height:100%;
			width:187px;
			@media(max-width: 479px){
				width: 100px;
			}
			@include inline-top;
		}
		.pages{
			text-align:center;
			@media(max-width: 479px){
				border-top: 1px $grey solid;
				padding-top: 13px;
			}
			.activated{
				background-color:$navcolor !important;
			}
			.ball{
				margin:0px 5px;
				@include inline-top;
				width:15px;height:15px;border-radius:10px;
				background-color:$lightBlue;
				@media(max-width: 479px){
					width:10px;height:10px;border-radius:7px;
				}
			}
			.ball:hover{
				cursor:pointer;
			}
		}
	}
}

.job-results{
	.content{
		.scrollable-content{
			@media(max-width: 479px){
				height: 75%;
			}
		}
	}
}
.resume-hero{
    .saved-content{
        .save:not(.selected){
			opacity: 0.7;
		}
    }
}
.saved-content, .delete-items{
    .title{text-align:left;}
    .content{
        height:325px;
		@media (max-width: 479px){
			height: 200px;
		}
        .scrollable-content{
            height:85%;
			@media(max-width: 479px){
				height: 80%;
			}
        }
		
        .save{
            margin-top:45px;
            position:relative;
            height:210px;
            font-size:18px;
			@media (max-width: 479px){
				height: 160px;
				margin-top:20px;
			}
            .content-save{
                position:absolute;
                top:145px;
                width:100%;
				@media (max-width: 479px){
					top: 75px;
				}
                .name{
                    color:black;
                    margin-bottom:3px;
					@media (max-width: 479px){
						font-size: 15px;
					}
                }
                .type{
                    font-size:16px;
                    line-height:18px;
                    color:$grey;
					@media(max-width: 479px){
						font-size:11px;
						line-height:13px;
					}
                }
                .date, .level{
                    font-size:14px;
                    line-height:16px;
                    color:$grey;
					@media(max-width: 479px){
						font-size:9px;
						line-height:11px;
					}
                }
            }
        }
    }
}
.job-results{
    .content{
        height:275px;
        .job-recommandation{
            .icon-suitcase-job{
                margin-top:40px;
                margin-bottom:20px;
            }
            font-size:17px;
            .title-job{
                color:black;
                line-height:26px;
            }
            .info-job{
                line-height:23px;
                color:$grey;
            }
        }
		@media(max-width:479px){
			height: 150px;
			.job-recommandation{
				 .icon-suitcase-job{
					margin-top:15px;
					margin-bottom:5px;
				}
				.title-job{
					font-size: 13px;
				}
				.info-job{
					font-size: 10px;
					line-height: 12px;
				}
			}
		}
    }
}
/*
Dashboard
*/
.dashboard{
    @include main-block;
    .job-results{
        margin-bottom:25px;
    }
    .resume{
        @include whitebox;
        position:relative;
        margin-top:60px;
        margin-bottom:25px;
        > .left,>.right{
            position:relative;
            height:350px;
            padding:45px 30px;
            width:33.31%;
        }
        >.left{
            border-right:1px solid $lightGrey;
        }
        .person{
            text-align:center;
            .icon-pencil{
                position:absolute;
                top:45px;
                left:30px;
                
            }
            .picture{
                width:175px;height:175px;
                border-radius:100px;
                border:2px solid white;
                @include box-shadow(0px,1px,2px,0px,0.2);
                margin:auto;margin-bottom:20px;
                
            }
            .name{
                font-size:35px;
            }
            .mail{
                color:$grey;
                font-size:20px;                                                              
            }
        }
        .presentation{
            padding-top:30px;
            padding-bottom:0px;
            font-size:20px;
            line-height:20px;
            table{
                tr{
                 height:74px;
                    .logo{
                        margin:0px 15px;
                    }
                }
            }
        }
        .job-phase{
            font-size:20px;
            .title{
                margin-bottom:10px;
                .txt{width:70%;
                    margin-left:20px;
                }
            }
            .phases{position:relative;}
            .phase-content{
                .cursor{
                    padding-top:6px;
                    margin-right:30px;
                    margin-left:6px;
                    position:relative;
                    .icon-cursor-red{
                        position:absolute;
                        left:-4px;top:0px;
                    }
                }
                .phase{
                    margin-bottom:13px;
                    i{
                        position:relative;top:auto;right:auto;
                        margin-right:15px;}
                    >span{
                        line-height:40px;
                        position:relative;
                        top:1px;
                    }
                }
            }
        }
    }
    .saved-content{
        margin-bottom:25px;
    }
    .job-results,.saved-content{
        .title{text-align:left;}
        .button-edit{
            margin-top:-8px;
            margin-right:-12px;
            font-size:14px;
            position:relative;padding-left:10px;padding-right:40px;
        }
    }
    .content{
        .col-left,.col-right{
            width:370px;
        }
        .col-left{
            margin-right:25px;
        }
        .education,.work-experience,.skills,.technology,.career-goals{
            @include whitebox;
            margin-bottom:25px;
            .title-section{
                font-size:19px;
                padding:22px;
                padding-bottom:19px;
                .title{
                    margin-left:5px;
                }
                border-bottom:1px solid $lightGrey;  
            }
        }
        .work-experience-elem,.education-elem{
            position:relative;
            overflow:hidden;

        }
        .skills,.technology,.career-goals{
            .content{
                padding:20px;
            }
        }
        .education,.work-experience{
            .content{
                padding:20px;padding-bottom:0px;
            }
        }
        .website,.period,.place{
            font-size:15px;
            line-height:27px;
        }
        .website,.place{
            color:rgb(238,120,53);
        }
        .period{
            color:rgb(128,128,128);
        }
        .job-title,.education-elem .name{
            font-size:20px;line-height:30px;
        }
        .detail{
            margin-left:52px;//20+32
            width:75%;
        }
        .timeline{
            position:absolute;
            height:100%;
            .dashed-bar{
                width:0px;
                margin-left:17px;
                height:90%;
                border-left:1px dashed $lightGrey;
            }
        }
        .work-experience{
            .period{margin-bottom:3px;}
            .detail{
                padding-bottom:40px;
            }
            .website{
                text-transform:uppercase;
            }
           .occupation,.income{
                text-transform:uppercase;
                padding-left:15px;padding-right:15px;
            }
            .occupation{
                margin-right:10px;
            }
            .income{
                background:rgb(107,212,138);color:white;
            }
        }
        .education{
            .dashed-bar{margin-left:16px;}
            .detail{
                padding-bottom:30px;
            }
        }
        .add-elem,.technology-elem,.skill,.occupation,.income{
            @include font-size(16px);
            text-align:center;
            padding-top:12px;
            padding-bottom:8px;
        }
        .occupation,.technology-elem{
            @include blue-element;
        }
        .add-elem-input{
            @include font-size(16px);
            input{
                padding:12px;
                margin:0px;
                padding-top:12px;
                padding-bottom:8px;
                background:rgb(237,237,237);
                border:1px dashed rgb(190,190,190);
            }
        }
        .add-elem{
            background:rgb(237,237,237);
            border:1px dashed rgb(190,190,190);
        }
        .skills{
            .left{
                margin-right:26px;
            }
            .add-elem,.add-elem-input,.skill{
                width:46%;
            }
            .add-elem-input{
                float:left;
            }
            .add-elem{margin:0;}
            .skill{
                color:white;
                margin-bottom:20px;
                background:rgb(92,26,71);
            }
        }
        .technology{
            .add-elem,.add-elem-input input,.technology-elem{
                width:100%;
            }
            .technology-elem{
                padding-top:12px;
                padding-bottom:8px;
                margin-bottom:20px;
            }
        }
        .career-goals{
            font-size:19px;
            .content{
                .goal{margin-top:31px;width:80%;}
                .first{margin-top:0px;}
            }
        }
    }
}
.saved-content-page{
    .saved-content{
        margin-top:65px;
        .content{
            height:auto;
            padding:0px 100px;
            padding-bottom:40px;
            .save:not(.right){
                margin-right:8px;
            }
            .fourth{
                margin-right:0px !important;
            }
            .save{
                height:265px;
                overflow:hidden;
                margin-top:40px;
                .delete{
                    color:$red-delete;
                    margin-top:25px;
                    margin-bottom:10px;
                }
            }
        }
    }
