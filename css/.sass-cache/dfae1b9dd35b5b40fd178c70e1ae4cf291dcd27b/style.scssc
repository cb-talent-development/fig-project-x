/*
General buttons classes
*/
@mixin shadow-effect{
    box-shadow:0 1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow:0 1px 1px rgba(0,0,0,0.2);
    border:none;
}
@mixin shadow{
    @include shadow-effect;
    background:white;
}
@mixin invert-shadow-effect{
    box-shadow:0px -1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow:0px -1px 1px rgba(0,0,0,0.2);
    border:none;
}
@mixin invert-shadow{
    @include invert-shadow-effect;
    background:white;
}
@mixin button-style{
    font-size:18px;
    @include inline-top; 
    position:relative;
    padding-top:11px;padding-bottom:11px;
    padding-left:15px;padding-right:15px;
    text-align:left;
    color:$darkGrey;
     b,i{
        position:absolute;
        top:50%;
        right:15px;
    }
}
.green{
    background:green;
    color:white;
}
.button, .btn{
    @include shadow;
    @include button-style;
}
