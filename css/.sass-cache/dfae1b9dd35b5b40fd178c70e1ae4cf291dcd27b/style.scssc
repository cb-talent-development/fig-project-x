	h3.subtitle{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:22px;
        @media only screen and (max-width: 768px){
            font-size:13px;
            line-height:16px;
        }
	}
	div.comment{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:18px;
        @media only screen and (max-width: 768px){
            font-size: 10px;line-height:13px;
