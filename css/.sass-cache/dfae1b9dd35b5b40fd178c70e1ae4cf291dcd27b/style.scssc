/*
Small article (A2/A3)
*/
.a-small{
    @include phone-height;
	position: relative;
    .pictured{
	   .text{
		  display: none;
        }
	   .img{
           width: 100%;
           position:absolute;
           height:115px;
		   bottom:0px;
	   }
    }
    .not-pictured{
        .text{
		  margin: 10px;
		  margin-top: 0px;
		  padding: 0px;
		  -webkit-line-clamp: 7;
        }
	   .img{
            display:none;
	   }
    }
	.subtitle{
		padding: 0px;
		margin: 10px;
        margin-top:7px;
        margin-right:0px;
	}
	.title{
        min-height:50px;
		padding: 10px;
		padding-bottom: 0px;
		margin: 0px;margin-bottom:3px;
		-webkit-line-clamp: 3;
