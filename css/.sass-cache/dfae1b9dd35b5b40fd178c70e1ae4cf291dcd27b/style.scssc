/*
Small A2 or A3
*/
@mixin a-small{
     @media only screen and (max-width: 768px){
        @include phone-height;
        //to be sure
        .pictured .left,.pictured .right,.not-pictured .left,.not-pictured .right{
            width:100%;
            display:block;
        }
        .pictured h3.subtitle,.pictured h2.title,.pictured div.text,.not-pictured h3.subtitle,.not-pictured h2.title,.not-pictured div.text{
            padding-left: 10px;padding-right: 10px;
        }
        .pictured h3.subtitle,.not-pictured h3.subtitle{
            padding-top:7px;
        }
        .pictured h2.title,not-pictured h2.title{
            width:auto;
            min-height:50px;
            -webkit-line-clamp: 3;
        }
        .pictured{
           .text{
              display: none;
            }
           .img{
               position:absolute;
               height:115px;
               bottom:0px;
           }
        }
        .not-pictured{
