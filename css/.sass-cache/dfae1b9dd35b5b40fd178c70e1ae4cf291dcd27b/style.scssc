/*
COLOR
*/
@mixin shadow-color{
    @include box-shadow(0px,1px,1px,0px,0.3);
}
$grey-color:rgb(237,237,237);
$dark-grey-color:rgb(207,207,207);
$grey-font:rgb(157,157,157);
@mixin colorCommon{
    padding-top:12px;padding-bottom:8px;
    @include box-shadow(0px,1px,1px,0px,0.3);
}
@mixin back-grey{
    @include colorCommon;
    background-color:$grey-color !important;color:black;
}
.back-grey{
    @include back-grey;
}
.back-grey-no-hover{
    @include back-grey;
}
.back-grey:hover{
    cursor:pointer;
    background-color:$dark-grey-color !important;
}
.grey{
    color:$grey-font;
}
//green
$green-color:rgb(112,194,57);
$dark-green-color:rgb(95,174,45);
@mixin back-green{
    @include colorCommon;
    background-color:$green-color !important;color:white;
}
.back-green-no-hover{
    @include back-green;
}
.back-green{
    @include back-green;
}
.back-green:hover{
    cursor:pointer;
    background-color:$dark-green-color !important;
}
.green{
    color:$green-color !important;
}
//white
@mixin back-white{
    @include colorCommon;
    background-color:white !important;color:black;
}
.back-white-no-hover{
    @include back-white;
}
.back-white{
    @include back-white;
}
.back-white:hover{
    cursor:pointer;
    color:rgb(80,80,80) !important;
}
.white{
    color:white !important;
}
/*
Arrow
*/
@mixin arrow-up-down{
    width: 0; 
	height: 0; 
	border-left-style:solid;
    border-left-color:transparent;
    border-right-style:solid;  
    border-right-color:transparent;  
	font-size: 0;
	line-height: 0;
    
}
.arrow-up {
    @include arrow-up-down;
    border-bottom-style:solid; 
}
.arrow-down {    
    @include arrow-up-down;
	border-top-style:solid; 
}
