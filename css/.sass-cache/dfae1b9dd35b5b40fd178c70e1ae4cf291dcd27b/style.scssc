@mixin font-size-1{
    h2.title{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:55px;
        line-height:58px;
        @media only screen and (max-width:768px) {
		      font-size:24px;
              line-height:26px;
        }
	}
	h3.subtitle{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:22px;
        @media only screen and (max-width:768px){
            font-size:13px;
            line-height:16px;
        }
	}
	div.comment{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:18px;
        @media only screen and (max-width:768px){
            font-size: 10px;line-height:13px;
        }
	}
	div.text{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:16px;
        line-height:23px;
        @media only screen and (max-width:768px){  
              font-size:13px;line-height:16px;
        }
	}
    @include font-family;
}
@mixin media-font-size-2-3{
    @media only screen and (max-width:768px) {
            h2.title{
				font-size: 17px;line-height:21px;
            }
        	h3.subtitle{
                font-size:12px;line-height:16px;
            }
            div.text{
                font-size:12px;line-height:16px;
            }
    }
}
@mixin font-size-2{
    h2.title{
        overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:35px;
        line-height:38px;
	}
	h3.subtitle{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:18px;line-height:20px;
	}
	div.text{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:15px;
        line-height:20px;
	}
