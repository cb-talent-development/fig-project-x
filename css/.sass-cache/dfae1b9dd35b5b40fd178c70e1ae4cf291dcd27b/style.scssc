@mixin font-size-1{
    h2.title{
		overflow:hidden;
		display: -webkit-box;
		-webkit-box-orient: vertical;
        font-size:55px;
        line-height:58px;
        @media only screen and (max-width: 768px) {
		      font-size:24px;
