	.text{
		overflow:hidden;
		margin: 0px 30px;
		margin-bottom: 25px;
		-webkit-line-clamp: 6;
        @media only screen and (max-width: 768px){
            margin: 10px;margin-top:0px;margin-bottom:0px;
            overflow:hidden;
		    -webkit-line-clamp: 13;
