		margin: 0px;
		@media only screen and (max-width:768px) {
            max-height:100px;
            -webkit-line-clamp: 2;
		    padding-left: 15px;padding-top: 15px;
		}
	}
	.img{
        width:363px;
		height: 284px;		
		@media only screen and (max-width:768px) {
		   height: 94px;
		   width: 100%;
		}
	}
	div.comment{
		display: inline-block;
		margin: 30px;
        margin-top:8px;
        margin-bottom:16px;
		.commentCount{
			margin: 0px 3px;
            margin-top:1px;
			display: inline-block;
			vertical-align: top;
		}
        .icon-star,.icon-star-disabled{
            margin-left:-5px;
        }
        @media only screen and (max-width:768px){
          margin: 10px;margin-top:0px;margin-bottom:5px;
          .icon-star,.icon-star-disabled{
            margin-left:-2.5px;
          }
          .commentCount{
            margin-top:1px;
          }
        }
	}
	.text{
		overflow:hidden;
		margin: 0px 30px;
		margin-bottom: 25px;
		-webkit-line-clamp: 6;
        @media only screen and (max-width:768px){
            margin: 10px;margin-top:0px;margin-bottom:0px;
            overflow:hidden;
		    -webkit-line-clamp: 13;
        }
	}
    @include font-size-1;

}
@mixin a-2-3{
    position:relative;
    div.comment{
        display:none;
    }
    div.text,h3.subtitle,.img{
        width:100%;
    }
    .not-pictured{
        .left,.right{
            width:100%;
        }
    }
    .pictured{
        .left{
            float:left;
            clear:left;
        }
    }
}
/*
Small A2 or A3
*/
@mixin a-small{
     @media only screen and (max-width:768px){
        @include phone-height;
        //to be sure
        .pictured .left,.pictured .right,.not-pictured .left,.not-pictured .right{
            width:100%;
            display:block;
        }
        .pictured h3.subtitle,.pictured h2.title,.pictured div.text,.not-pictured h3.subtitle,.not-pictured h2.title,.not-pictured div.text{
            padding-left: 10px;padding-right: 10px;
        }
        .pictured h3.subtitle,.not-pictured h3.subtitle{
            padding-top:7px;
        }
        .pictured h2.title,not-pictured h2.title{
            width:auto;
            min-height:50px;
            -webkit-line-clamp: 3;
        }
        .pictured{
           .text{
              display: none;
            }
           .img{
               position:absolute;
               height:115px;
               bottom:0px;
           }
        }
        .not-pictured{
            div.text{
               padding-top:7px;
                max-height:100%;
              -webkit-line-clamp: 7;
            }
           .img{
                display:none;
           }
        }
        @include font-size-2;
    }
}
/*
Article type 2
*/
.a2{
	@include med-height;
	h2.title{
		width:100%;
		overflow: hidden;
		padding: 20px;padding-top: 15px;padding-bottom: 0px;
		margin: 0px;
		-webkit-line-clamp: 3;
	}
	h3.subtitle{
		padding-left: 20px;padding-top: 15px;
		margin: 0px;
    }
	div.text{
		padding: 20px;
        padding-bottom:0px;
        padding-top:17px;
		margin: 0px;
		-webkit-line-clamp: 11;
	}
    .pictured{
       .left{
            width:128px;
        }
        .img{
          height: 125px;
          margin-top:17px;
	   }
        h3.subtitle{		
            -webkit-line-clamp: 2;
        }
        .right{
            width:235px;
        }
    }
    .not-pictured{
        .img{display:none;}
        .text{
            -webkit-line-clamp: 9;
        }
    }
    @include font-size-2;
    @include a-2-3;
    @include a-small;
}
/*
Article type 3
*/
.a3{
	@include small-height;
    h3.subtitle{
        margin:0px;
		padding-top: 22px;
        padding-left:15px;
	}	
	.img{
		height: 115px;
		width: 100%;
	}	
	h2.title{
        width:235px;     
        margin:0px;
        padding-top:15px;padding-left:15px;padding-right:15px;
		-webkit-line-clamp: 3;
	}
	div.text{
		margin:0px;
        padding-top:4px;padding-left:15px;padding-right:15px;
		-webkit-line-clamp: 4;
	}
    .pictured{
        h2.title{
            float:right;
        }
        .right{
            width:235px;     
        }
        .left{
            width:128px;
        }
    }
    .not-pictured{
        div.text{
            padding-top:13px;
            max-height:80px;
            -webkit-line-clamp: 3;
        }
        h2.title{
            width:100%;
        }
        h3.subtitle{
            padding-top:13px;
        }
        .img{display:none;}
    }
    @include font-size-3;
    @include a-2-3;
    @include a-small;

}
/*
Small article (A2/A3)
*/
.a-small{
    @include phone-height;
	position: relative;
    .pictured{
	   .text{
		  display: none;
        }
	   .img{
           width: 100%;
           position:absolute;
           height:115px;
		   bottom:0px;
	   }
    }
    .not-pictured{
        .text{
		  margin: 10px;
		  margin-top: 0px;
		  padding: 0px;
		  -webkit-line-clamp: 7;
        }
	   .img{
            display:none;
	   }
    }
	.subtitle{
		padding: 0px;
		margin: 10px;
        margin-top:7px;
        margin-right:0px;
	}
	.title{
        min-height:50px;
		padding: 10px;
		padding-bottom: 0px;
		margin: 0px;margin-bottom:3px;
		-webkit-line-clamp: 3;
	}
