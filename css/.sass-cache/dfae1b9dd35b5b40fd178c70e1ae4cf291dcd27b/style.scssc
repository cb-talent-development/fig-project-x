            $rapp-helper:0.6;
            .logo{position:relative;top:26px;left:17px;
                @media only screen and (max-width: 768px){
                    top:26px * $rapp-helper;
                    left:17px * $rapp-helper;
                }
            }
            strong{
                font-weight:bold;
            }
        }
        .third{
