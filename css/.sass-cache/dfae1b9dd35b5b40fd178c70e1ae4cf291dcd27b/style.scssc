    .button-change:hover{
        cursor:pointer
    }
    .buttons-changes .selected .carre,.buttons-changes .selected:hover .carre{
        background:rgb(238,120,53);
    }
}
/*
Locally Grown
*/
.locallyGrown{
    .selector{
        .input-zip{
            width:23%;
            min-width:267px;
            float:left;
            @include blue-element;
			@media only screen and (max-width: 768px){
				width: 100%;
			}
        }
    }
    .mapbox{
        @include main-block;
        height:590px;
		@media only screen and (max-width: 768px){
			width: 100%;
			margin: 0px;
            height:590px * $rapp-resp * $rapp-resp;
		}
    }
}

/*
Career Scape
*/
.careerScape{
    canvas{
        background-color:white;
        margin:auto;cursor:move;padding:0px;display:block;z-index:1;
		@media only screen and (max-width:768px){
			width: 100%;
			margin: 0px;
		}
    }
    .canvas-container{
        margin:auto;
        text-align:center;
        position:relative;
		@media only screen and (max-width:768px){
			width: 100%;
			margin: 0px;
		}
    }
    .buttons-changes{
            width:23%;
            min-width:267px;
            height:64px;
            float:left;
            background-color:$navcolor;
			.button-change{
				padding-top:23px;padding-bottom:10px;
				height:100%;
				width:49%;
				.carre{
					margin-left:15px;
				}
			}
			@media only screen and (max-width: 768px){
				width: 100%;
				height: 64px * $rapp-resp;
                min-width:0px;
				.button-change{
					padding-top:23px * $rapp-resp;
                    padding-bottom:10px * $rapp-resp;
					.carre{
						margin-left: 11px;
						width: 17px;
						height: 17px;
					}
				}
			}
			.button-change:hover:not(.selected) .txt{
				color:rgba(255,255,255,0.8);
			}
			.button-change:hover:not(.selected) .carre{
				background-color:rgba(255,255,255,0.8);
			}
			.buttons-changes .selected .txt,.buttons-changes .selected:hover .txt{
				color:rgb(238,120,53);
			}
    }
    .zoom{
        width:38px;
        font-weight:600;
        font-size:25px;
        background:rgb(80,80,80);
        color:white;
        margin-bottom:12px;
        padding-top:5px;
        padding-bottom:3px;
    }
    .zoom:hover:not(.selected){
            background: rgba(50,50,50,0.8);
            cursor:pointer;
    }
    .button:hover{
        cursor:pointer;
    }
    #json{
        margin:auto;
        display:block;
        margin-top:40px;
        background:#FFFEAA;
        color:rgb(90,90,90);
        width:70%;
        height:400px;
    }
    /*
    Legend part
    */
    $rapp-legend: 0.6;
    
    .legend{
        z-index:10;
        position:absolute;
        width:200px;
        height:300px;
        top:40px;
        right:22px;  
        -moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        -webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
		@media only screen and (max-width:768px){
			width:230px;
            height:300px * $rapp-legend * 1.3 / 2;
            top:auto;
            bottom:80px;
            right:22px * $rapp-legend;  
		}
    }
    .legend > div{
        position:relative;
        width:100%;
        height:150px;
        background:white;
        /*border-bottom:1.5px solid rgba(200,200,200,0.7);
        border-right:1.5px solid rgba(200,200,200,0.7);
        border-left:1.5px solid rgba(200,200,200,0.7);*/
		@media only screen and (max-width: 768px){
            float:left;
            width:50%;
			height:150px * $rapp-legend * 1.3;
		}
    }
    .legend >div.growth{
        border-bottom:1.5px solid rgba(200,200,200,0.7);
        @media only screen and (max-width: 768px){
            border-bottom:none;
        }

    }
    .legend .ball{
        display:inline-block;
        position:absolute;
        border:2px solid grey;
        background:lightgrey;
    }
    .legend .big{
        top:24px;
        right:26px;
        width:68px;
        height:68px;
        border-radius:39px;
		@media only screen and (max-width: 768px){
			top:24px * $rapp-legend;
            left:55px;
            width:68px * $rapp-legend;
            height:68px * $rapp-legend;
            border-radius:39px * $rapp-legend;
		}
    }
    .legend .medium{
        top:24px;
        right:85px;
        width:54px;
        height:54px;
        border-radius:32px;
        @media only screen and (max-width: 768px){
			top:24px * $rapp-legend;
            left:35px;
            width:54px * $rapp-legend;
            height:54px * $rapp-legend;
            border-radius:32px * $rapp-legend;
		}
    }
    .legend .small{
        top:24px;
        right:133px;
        width:38px;
        height:38px;
        border-radius:24px;
		@media only screen and (max-width: 768px){
			top:24px * $rapp-legend;
            left:20px;
            width:38px * $rapp-legend;
            height:38px * $rapp-legend;
            border-radius:24px * $rapp-legend;
		}		
    }
    .legend .info{
        text-align:left;
        position:absolute;
        bottom:18px;
        left:22px;
		@media only screen and (max-width:768px){
			bottom:18px * $rapp-legend;
            left:22px * $rapp-legend;
		}
    }
    .legend .info .title{
        font-weight:200;
        @include font-size(18px);
    }
    .legend .info .comment{
        font-style:italic;
        @include font-size(12px);
    }
    /*
    /END Legend part
    */
}

/*
JOB RESULTS/RECOMMENDATIONS
*/
.scrollable-content{
    margin:auto;
    overflow:hidden;
}
.scrollable{
    text-align:left;
    width:10000px;
    .step{
        display:inline-block;
    }
}
.delete-resume{
	.section{
		@media(min-width: 480px){
			width: 450px;
		}
	}
	.save{
		text-align: center;
	}
	.footer-dialog{
		border-top:1px solid $lightGrey;
	}
}

.job-results,.saved-content, .delete-items{
	width:100%;
	text-align:center;
	position:relative;
	@include shadow;
	margin-bottom:60px;
	background:white;
	@media only screen and (max-width:768px){
		margin-bottom:30px;
	}
	.icon-arrow-left-big-blue,.icon-arrow-right-big-blue{
		position:absolute;top:50%;margin-top:-26px;
		@include no_select;
		@media only screen and (max-width: 768px){
            top:auto;
			bottom:10px;
		}
	}
	.icon-arrow-left-big-blue{
		left:40px;
        @media only screen and (max-width: 768px){
            left:30px;
		}
	}
	.icon-arrow-right-big-blue{
		right:40px;
        @media only screen and (max-width: 768px){
            right:30px;
		}
	}
	i:hover{
		cursor:pointer;
	}
	.title{
		@include font-size(20px);
		padding:0px 25px;
		padding-top:22px;
		padding-bottom:20px;
		border-bottom:1px solid $grey;
		@media only screen and (max-width: 768px){
			font-size:14px;
			padding:0px 15px;
			padding-top:17px;
			padding-bottom:14px;
		}
	}
	.content{
		position:relative;
		.scrollable-content{
			width:80%;
			height:82%;
            @media only screen and (max-width: 768px){
				height: 84%;
			}
            @media only screen and (max-width: 768px) and (min-width:601px){
				width: 560px;
			}
			@media only screen and (max-width: 600px) and (min-width:481px){
				width: 440px;
			}
            @media only screen and (max-width: 480px){
				width: 300px;
			}
		}
		.scrollable{
			height:100%;
		}
		.job-recommandation,.save{
			text-align:center;
			text-transform:uppercase;
			height:100%;
			width:187px;
            //oblicated to do more decomposition because here the size can be given in percentage
            @media only screen and (max-width: 768px) and (min-width:601px){
				width: 112px;
			}
			@media only screen and (max-width: 600px) and (min-width:481px){
				width: 110px;
			}
            @media only screen and (max-width: 480px){
				width: 100px;
			}
			@include inline-top;
		}
		.pages{
			text-align:center;
			@media only screen and (max-width: 768px){
				border-top: 1px $grey solid;
				padding-top: 15px;
			}
			.activated{
				background-color:$navcolor !important;
			}
			.ball{
				margin:0px 5px;
				@include inline-top;
				width:15px;
                height:15px;
                border-radius:10px;
				background-color:$lightBlue;
				@media only screen and (max-width: 768px){
					width:10px;height:10px;border-radius:7px;
				}
			}
			.ball:hover{
				cursor:pointer;
			}
		}
	}
}


.resume-hero{
