	h2.title{
		width:100%;
		overflow: hidden;
		padding: 20px;padding-top: 15px;padding-bottom: 0px;
		margin: 0px;
		-webkit-line-clamp: 3;
	}
	h3.subtitle{
		padding-left: 20px;padding-top: 15px;
		margin: 0px;
    }
	div.text{
		padding: 20px;
        padding-bottom:0px;
        padding-top:17px;
		margin: 0px;
		-webkit-line-clamp: 11;
	}
    .pictured{
       .left{
            width:128px;
        }
        .img{
          height: 125px;
          margin-top:17px;
