@mixin main-block{
    width:$total-width;
    display:block;
    margin:auto;
	@media only screen and (max-width: 1200px) and (min-width: 980px){
        width:975px;
    }
	@media only screen and (max-width: 979px) and (min-width: 768px){
        width:765px;
    }
    @media only screen and (max-width: 767px){
        width:95%;
    }
}
@mixin webkit-box{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow:hidden;

}
@mixin absoluteFill{
    position:absolute;left:0px;right:0px;top:0px;bottom:0px;
    width:100%;height:100%;
}
@mixin box-shadow($left,$bottom,$spread,$thickness,$opacity){
    box-shadow:$left $bottom $spread $thickness rgba(0,0,0,$opacity);
    -webkit-box-shadow: $left $bottom $spread $thickness rgba(0,0,0,$opacity);
}
@mixin inline-top{
   display:inline-block;
   vertical-align:top;
}
@mixin font-size($font-size){
    font-size:$font-size;
    @media only screen and (max-width:768px) {
        font-size:$font-size * $rapp-resp-text;
    }
}
