                        .logo{
                            position:absolute;
                            bottom:30px;
                        }
                        .correction{
                            position:absolute;
                            bottom:30px;
                            right:0px;
                            .mess{
                                font-size:16px;
                                margin-bottom:10px;
                                text-transform:uppercase;
                                color:$orange;
                                @include webkit-box;
                                -webkit-line-clamp:4;
                            }
                            .wrong{color:black;}
                            .select{border:1px solid $orange;}
                        }
                    }
                }
                .job-info{
                    padding:0px 15px;
                    padding-top:25px;padding-bottom:30px;
                    
                }
            }
            .footer-dialog{
                border-top:1px solid $lightGrey;
                padding-top:12px;
                padding-bottom:15px;
            }
        }
    }
