                margin-left:60px;
                @media only screen and (max-width: 480px){
                    margin-left:55px;
                }
            }
            .level-3 .content{
                margin-left:80px;
                @media only screen and (max-width: 480px){
                    margin-left:70px;
                }
            }
            .separator{
                height:1px;
                border-bottom:1px solid $backgroundGrey;
                margin-bottom: 15px;
                margin-top:15px;
                margin-left:-20px;
            }
            .comment{
                font-size:13px;
                .img{
                    height:40px;width:40px;float:left;
                }
                .name{
                    font-weight:bold;
                    margin-right:12px;
                }
                .time{
                    color:$lightGrey;
                }
                .statement{
                    margin:10px 0px;
                    overflow:hidden;
                }
                .reply{
                    position:relative;
                    margin-right:5px;
                    text-transform:capitalize;
                    font-weight:bold;
                    color:$orange;
                }
                .reply:hover{
                    cursor:pointer;
                    text-decoration:underline;
                }
                .closeReply{
                    @extend .reply;
                    color:rgb(212,98,104);
                }
                .content{
                    padding-left:20px;
                    float:none;
                    width:auto;
                    padding-right:20px;
                    transition: padding-bottom, 0.5s;
                    padding-bottom:30px;
                    .new-comment{
                        width:100%;
                        border:none;
                        padding:15px 0px;
                        display:none;
                        .placeholder{
                            color:$grey;
                        }
                    }
                    .closeReply{
                        display:none;
                    }
                    @media only screen and (max-width: 768px){
                        padding-left:20px * $rapp-resp;
                        padding-bottom:30px * $rapp-resp;
                        padding-right:15px;
                    }
                }
            }
            .no-first-bar .separator,.no-second-bar .separator{
                display:none;
            }
            .no-padding-bottom .comment .content{
                padding-bottom:0px;
            }
        }
    }
