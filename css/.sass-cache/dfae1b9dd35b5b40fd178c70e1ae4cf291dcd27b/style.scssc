    width:130px;
    font-size:27px;
    padding-top:18px;padding-bottom:12px;
    padding-left:20px;padding-right:20px;
    @media only screen and (max-width: 768px){
        width:44%;
        font-size:19px;
        padding:0px 15px;
        padding-top:14px;
        padding-bottom:8px;
        margin-bottom:10px;
        
    }
}
.button-google{
    background-color:rgb(194,83,57);
    @media only screen and (max-width: 480px){
        width:100% !important;
    }
}
.button-google:hover{
    background-color:rgb(184,73,47);
}
.button-facebook{
    background-color:rgb(64,93,149);
    @media only screen and (max-width: 480px){
        width:100% !important;
    }
}
.button-facebook:hover{
    background-color:rgb(54,83,139);
}
.dashboard-popup,.tool .get-started-resume{
    .section{
        .steps-content{
           @media only screen and (max-width: 768px){
               display:none;
            }
        }
        .content{
            padding: 20px;
            border-bottom:1px solid $lightGrey;
            @media only screen and (max-width: 768px){
                padding:10px;
            }
            .icon-step-1, .icon-step-2, .icon-step-3, .icon-step-4, .icon-step-5,.icon-step-1-grey, .icon-step-2-grey, .icon-step-3-grey, .icon-step-4-grey, .icon-step-5-grey{
                margin-right: -28px;
                display:inline-block;
                vertical-align:top;
                text-align:center;
                padding-top:26px;
                color: white;
                font-size:23px;
                @media only screen and (max-width: 768px){
                    padding-top:13px;
                    font-size:18px;
                }
                span{
                   position:relative;right:15px;
                   @media only screen and (max-width: 768px){
                   }
                }
            }
            .icon-step-5,.icon-step-5-grey{
                span{
                    left:15px;  
                    right:0px;
                }
            }
        }
    }
}
.dashboard-popup, .get-started-resume{
    .scrollable-content{
        width:1250px;
        @media only screen and (max-width: 768px){
            width:100%;
        }
    }
    .scrollable{
        //option 1
        width:800%;
        min-width:8 * 1250px;
        @media only screen and (max-width: 768px){
            min-width:0px;
        }
    }
    .section-wrapper{
        width:12.5%;
        float:left;
    }
    .section{
        //width:90%;
        width:1204px;
        @media only screen and (max-width: 768px){
            width:90%;
        }
        .header{
            @include font-size(24px);
            .grey{
                color:$grey;
            }
        }
        .step{
            width:1204px;
            @media only screen and (max-width: 768px){
                width:100%;
            }
        }
        .step1{
            display:none;
        }
        .custom-radio{
            margin-right:25px;
            position:relative;
            input[type=checkbox] {
                display: none;
            } 
            label{margin-left:40px;}
            label:hover{ cursor:pointer;}
            label:before{
                height:20px;
                width:20px;
                content:"";
                position:absolute;
                left:0px;
                top:50%;
                text-align: center;  
                margin-top:-15px;
                @include box-shadow(0.5px,-1px,2px,0px,0.3);
                border-right:1px solid $lightGrey;
                background:white;
            }
            input[type=radio]:checked + label:before { 
                background:$orange;
            }  
        }
        .custom-radio{
            @include font-size(23px);
            color:$darkGrey;
            @media only screen and (max-width: 768px){
                font-size:17px;
            }
        }
        .message{
            @include font-size(17px);
            text-transform:uppercase;
            color:rgb(178,178,178);
            margin-bottom:10px;
        }
        .optional{
            color:$lightGrey;
            text-transform:uppercase;
            @include font-size(17px);
            @media only screen and (max-width: 768px){
                display:none;
            }
        }
        .select,input,input[placeholder]{  
            padding-top:20px;padding-bottom:14px;
            font-size:23px;
            @media only screen and (max-width: 768px){
                padding-top:15px;
                padding-bottom:10px;
                font-size:16px
            }
        }   
        .large{width:465px;margin-right:25px;
            @media only screen and (max-width: 768px){
                width:49%;
                margin-right:2%;
                margin-bottom:10px;
            }
        }
        .medium{width:370px;margin-right:25px;
            @media only screen and (max-width: 768px){
                width:49%;
                margin-right:2%;
                margin-bottom:10px;
            }   
        }
        .last{
            margin-right:0px;
        }
        .right{
            .large,.medium{
                margin-right:0px;
            }
        }
        .email-address,.street,.industry-goal{
            @media only screen and (max-width: 768px){
                width:100% !important;
                margin-right:0% !important;   
            }
        }
        .address{
            .street{margin-bottom:20px;}
            .large{
               width:365px;margin-right:25px;
               @media only screen and (max-width: 768px){
                    width:49%;
                    margin-right:2%;
                    margin-bottom:10px;
                }  
            }
            .medium{width:170px;margin-right:25px;
                @media only screen and (max-width: 768px){
                    width:49%;
                    margin-right:2%;
                    margin-bottom:10px;
                } 
            }
        }
        .general{
            .age,.gender{
                @media only screen and (max-width: 768px){
                        width:50%;
                    }
            }
            .age{
              .large,.medium,.message{
                    @media only screen and (max-width: 768px){
                        width:100%;
                        margin-right:0%;
                        margin-left:0%;
                        margin-bottom:10px;
                    } 
                }  
            }
            .gender{
                margin-right:149px;
                @media only screen and (max-width: 768px){
                    margin-right:0px;
                }
                .custom-checkbox{margin-top:15px;}
            }
            
        }
        .job-phase{
            padding-top:25px;
            .cursor{
                margin-top:20px;
                margin-bottom:45px;
                position:relative;
                @media only screen and (max-width: 768px){
                    margin-bottom:20px;
                    display:none;
                }
                .bar{
                    border-radius:8px;
                    height:15px;
                    background:rgb(231,235,235);
                }
                .icon-cursor-orange-down{
                    position:absolute;
                    top:-5px;
                    left:10px;
                    background:orange;
                    height:30px;
                    width:20px
                }
            }
            .phase{
                opacity:0.6;
                @include whitebox;
                width:23.35%;
                height:295px;
                text-align:center;
                margin-bottom:15px;
                @media only screen and (max-width: 768px){
                    width:49%;
                    height:275px;
                }
                .title{
                    @include font-size(20px);
                    padding-top:20px;padding-bottom:25px;
                    @media only screen and (max-width: 768px){
                        padding-top:15px;padding-bottom:15px;
                    }
                    .logo{
                        margin-bottom:15px;
                    }
                    border-bottom:1px solid $lightGrey;
                }
                .description{
                    padding:0px 30px;
                    @include font-size(15px);
                    line-height:20px;
                    padding-top:25px;
                    @include webkit-box;
                    -webkit-line-clamp: 6;
                    @media only screen and (max-width: 768px){
                        padding:10px;
                        padding-bottom:0px;
                        -webkit-line-clamp: 8;
                    }
                }
            }
            .left{margin-right:2.2%;
                @media only screen and (max-width: 768px){
                    margin-right:2%;
                }
            }
            .before-last{
                margin-right:0px;
                @media only screen and (max-width: 768px){
                    margin-right:2%;
                }
            }
             .phase[class~="selected"]{
                opacity:1;
            }
        }
        .job-occupation{
            height:200px;
            @media only screen and (max-width: 768px){/* and (min-width: 481px){
                height:190px;
            }
            @media only screen and (max-width: 480px){*/
                height:auto;
            }
        }
        .school-name{
            height:210px;
            @media only screen and (max-width: 768px){/* and (min-width: 481px){
                height:190px;
            }
            @media only screen and (max-width: 480px){*/
                height:auto;
            }
        }
        .job-occupation,.school-name{
            .large{
                width:465px;margin:0px;
                @media only screen and (max-width: 768px){
                    width:100%;
                    margin-bottom:10px;
                }
            }
            .message-large{
                width:87%;margin:0px;
                @media only screen and (max-width: 768px){
                    width:100%;
                }
            }
            .element{
                height:100%;
                width:42%;
                position:relative;
                .entry{position:absolute;bottom:10px;
                    .message{margin-bottom:10px;}
                }
                @media only screen and (max-width: 768px){
                   // width:48%;
                    .entry{
                        width:100%;
                        bottom:0px;
                    }
                //}
                //@media only screen and (max-width: 480px){
                     width:100%;
                    .entry{
                        position:relative;
                    }
                }
                
            }
            .element-correction{
                height:100%;
                width:50.7%;
                position:relative;
                @media only screen and (max-width: 768px) {/*and (min-width: 481px){
                    width:48%;
                }
                @media only screen and (max-width: 480px){*/
                    width:100%;
                }
                .logo{
                    position:absolute;
                    bottom:10px;
                    @media only screen and (max-width: 768px){
                        display:none;
                    }
                }
                .correction{
                    width:465px;
                    position:absolute;
                    bottom:10px;
                    right:0px;
                    .mess{
                        @include font-size(17px);
                        line-height:23px;
                        margin-bottom:10px;
                        text-transform:uppercase;
                        color:$orange;
                        @include webkit-box;
                        -webkit-line-clamp:4;
                    }
                    .wrong{color:black;}
                    .select{border:1px solid $orange;}
                    @media only screen and (max-width: 768px){
                        width:100%;
                        bottom:0px;
                        .mess{
                            -webkit-line-clamp:5;
                            line-height:20px;
                        }
                   /* }
                    @media only screen and (max-width: 480px){*/
                        position:relative;
                    }
                }
            }
        }
        .job-occupation,.school-name,.job-info,.school-info{
            
            @media only screen and (max-width: 768px){
                .message{
                    margin-top:5px;
                }
                .large{
                    width:100%;
                }
               /* >.left ,>.right{
                    width:49%;
                }
            }
            @media only screen and (max-width: 480px){*/
                >.left ,>.right{
                    width:100%;
                }
            }
        }
        .selectable-title{
            padding:20px;
            padding-bottom:5px;
            position:relative;
            padding-top:35px;
            font-size: 47px;
            @media only screen and (max-width: 768px){
                font-size:25px;
                padding:10px;
                padding-top:20px;
                padding-bottom:5px;
                
            }
            .selectable-button{
                display:inline-block;
                width:33px;
                @media only screen and (max-width: 768px){
                    width:18px;
                }
                .icon-plus{
                    position:relative;
                    top:1px;
                    display:block;
                }
                .icon-minus{
                    position:relative;
                    top:-14px;
                    display:none;
                    @media only screen and (max-width: 768px){
                        top:-7px;
                    }
                }
            }
            .selectable-button[class~=selected]{
                .icon-plus{
                    display:none;
                }
                .icon-minus{
                    display:block;
                }
            }
            .icon-completed{
                i{
                    position:absolute;
                    right:30px;
                    top:30px;
                    @media only screen and (max-width: 768px){
                        top:auto;
                        bottom:9px;
                        right:10px;
                    }
                    margin-right:0px;
                }
            }
            border-bottom:8px solid rgb(232,232,232);
        }
        .selectable-content{
            border-top:1px solid rgb(201,202,203);
            display:none;
            padding:50px 20px;
            padding-bottom:0px;
            @media only screen and (max-width: 768px){
                padding:10px 10px;
                padding-top:20px;
                padding-bottom:0px;
            }
            .check-box-1{
                float:left;
                padding-bottom:30px;
                padding-right:3.5%;
                width:33.3%;
                @include font-size(19px);
                @media only screen and (max-width: 768px){
                    width:100%;
                    padding-right:0%;
                    padding-bottom:10px;
                }
            }
        }
        .goals{
            @media only screen and (max-width: 768px){
                .message{
                    margin-top:5px;
                }
            }
            .medium{
                width:371px;
                @media only screen and (max-width: 768px){
                   width:100%;
                }
            }
            .left,.right{
                @media only screen and (max-width: 768px){
                   width:100%;
                }
            }
            .selectable-content{
                padding:20px;
                padding-top:30px;
                border-bottom:1px solid rgb(201,202,203);
                @media only screen and (max-width: 768px){
                    padding:10px 10px;
                    padding-top:15px;
                }
            }
        }
        .footer-dialog{
            @media(min-width: 480px){
                padding-top:12px;
                padding-bottom:15px;
            }
        }
        .user-info{
            .message{
                @media only screen and (max-width: 768px){
                    margin-right:0px;
                    width:100%;
                }
            }
            .presentation{
                //special effect here
                @media only screen and (max-width: 600px){
                    text-align:center;
                }
            }
            .name-info{
                margin-bottom:20px;
                @media only screen and (max-width: 768px){
                    margin-bottom:0px;
                }
            }
            .explanation{
                width:59%;
                font-size:27px;
                .presentation{
                    margin-bottom:40px;
                }
                @media only screen and (max-width: 768px){
                    font-size:18px;
                    width:57%;
                    padding-right:7px;
                    .presentation{
                        margin-bottom:15px;
                    }
                }
                @media only screen and (max-width: 700px){
                    width:51%;
                }
                //special responsive here (no choice)
                @media only screen and (max-width: 600px){
                    width:100%;
                }
            }
            .info{
                font-size:22px;
                @include white-box;
                .strength {
                    margin:15px 0px;
                }
                .icon{
                    margin:0px 3px;
                }
                @media only screen and (max-width: 768px){
                    font-size:16px;
                    padding:10px;
                }
                @media only screen and (max-width: 600px){
                    width:auto;
                    display:inline-block;clear:both;
                    float:none;
                    margin:15px auto;
                }
            } 
        }
        .right-resp{
            @media only screen and (max-width: 768px){
                float:right;
                margin-left:0px !important;
                margin-right:0px !important;
                margin-top:0px;
                margin-bottom:10px;
            }  
        }
    }
    .section-photo{
        width:1000px;
        @media only screen and (max-width: 768px){
            width:90%;
        }
        .change-picture{
            padding:40px;
            text-align:center;
            border-bottom:1px solid rgb(201,202,203);
            @media only screen and (max-width: 768px){
                padding:20px;
            }
            .photo{
                margin:auto;
                width:320px;
                height:320px;
                border-radius:200px;
                -webkit-box-shadow:0 1px 2px rgba(0,0,0,0.2);
                border: 7px solid white;
                margin-bottom:50px; 
                @media only screen and (max-width: 768px){
                    width:200px;
                    height:200px;
                    border-radius:140px;
                    border: 5px solid white;
                    margin-bottom:40px; 
                }
            }
            .buttons{
                .button-upload-wrapper{
                    display:inline-block;
                    vertical-align:top;
                    position:relative;
                    .message{
                        position:absolute;
                        top:-30px;
                        left:0px;
                    }
                    @media only screen and (max-width: 768px){
                        width:90%;
                        margin-bottom:10px;
                        .message{
                            top:-25px;
                            left:5%;
                        }
                    }
                }
                .button{
                    @extend .button-social
                }
                .button-upload{
                    width:330px;margin-right:25px;
                    background-color:rgb(63,93,149);
                    .message{
                        position:absolute;top:-30px;left:0px;
                    }
                    @media only screen and (max-width: 768px){
                        width:100%;
                        margin-right:0px;
                        margin:auto;
                    }
                }
                .button-upload:hover{
                   background-color:rgb(53,83,139);
                }
                .button-edit{
                    margin-right:25px;
                    background-color:rgb(238,124,51); 
                    @media only screen and (max-width: 768px){
                        margin-right:0px;
                        margin-left:5%;
                        float:left;
                    }
                }
                .button-edit:hover{
                   background-color:rgb(228,114,41);  
                }
                .button-delete{
                    background-color:rgb(233,63,51); 
                    @media only screen and (max-width: 768px){
                        float:right;
                        margin-right:5%;
                    }
                }
                .button-delete:hover{
                    background-color:rgb(223,53,41);  
                }
            }
        }
    }
}

.careerScape, .resume-hero{
	.button-change{
        color:white;
        font-size:18px;
        display: inline-block;
        .carre{
            float:left;
            width:20px;
            height:20px;
            background:white;
            position:relative;
            margin-right:10px;
        }
        .txt{
            float:left;            
        }
        
    }
    .button-change:hover{
        cursor:pointer
    }
    .buttons-changes .selected .carre,.buttons-changes .selected:hover .carre{
        background:rgb(238,120,53);
    }
}
/*
Locally Grown
*/
.locallyGrown{
    .selector{
        .input-zip{
            width:23%;
            min-width:267px;
            float:left;
            @include blue-element;
			@media only screen and (max-width: 768px){
				width: 100%;
			}
        }
    }
    .mapbox{
        @include main-block;
        height:590px;
		@media only screen and (max-width: 768px){
			width: 100%;
			margin: 0px;
            height:590px * $rapp-resp * $rapp-resp;
		}
    }
}

/*
Career Scape
*/
.careerScape{
    canvas{
        background-color:white;
        margin:auto;cursor:move;padding:0px;display:block;z-index:1;
		@media only screen and (max-width:768px){
			width: 100%;
			margin: 0px;
		}
    }
    .canvas-container{
        margin:auto;
        text-align:center;
        position:relative;
		@media only screen and (max-width:768px){
			width: 100%;
			margin: 0px;
		}
    }
    .buttons-changes{
            width:23%;
            min-width:267px;
            height:64px;
            float:left;
            background-color:$navcolor;
			.button-change{
				padding-top:23px;padding-bottom:10px;
				height:100%;
				width:49%;
				.carre{
					margin-left:15px;
				}
			}
			@media only screen and (max-width: 768px){
				width: 100%;
				height: 64px * $rapp-resp;
                min-width:0px;
				.button-change{
					padding-top:23px * $rapp-resp;
                    padding-bottom:10px * $rapp-resp;
					.carre{
						margin-left: 11px;
						width: 17px;
						height: 17px;
					}
				}
			}
			.button-change:hover:not(.selected) .txt{
				color:rgba(255,255,255,0.8);
			}
			.button-change:hover:not(.selected) .carre{
				background-color:rgba(255,255,255,0.8);
			}
			.buttons-changes .selected .txt,.buttons-changes .selected:hover .txt{
				color:rgb(238,120,53);
			}
    }
    .zoom{
        width:38px;
        font-weight:600;
        font-size:25px;
        background:rgb(80,80,80);
        color:white;
        margin-bottom:12px;
        padding-top:5px;
        padding-bottom:3px;
    }
    .zoom:hover:not(.selected){
            background: rgba(50,50,50,0.8);
            cursor:pointer;
    }
    .button:hover{
        cursor:pointer;
    }
    #json{
        margin:auto;
        display:block;
        margin-top:40px;
        background:#FFFEAA;
        color:rgb(90,90,90);
        width:70%;
        height:400px;
    }
    /*
    Legend part
    */
    $rapp-legend: 0.6;
    
    .legend{
        z-index:10;
        position:absolute;
        width:200px;
        height:300px;
        top:40px;
        right:22px;  
        -moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        -webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
		@media only screen and (max-width:768px){
			width:230px;
            height:300px * $rapp-legend * 1.3 / 2;
            top:auto;
            bottom:80px;
            right:22px * $rapp-legend;  
		}
    }
    .legend > div{
        position:relative;
        width:100%;
        height:150px;
        background:white;
        /*border-bottom:1.5px solid rgba(200,200,200,0.7);
        border-right:1.5px solid rgba(200,200,200,0.7);
        border-left:1.5px solid rgba(200,200,200,0.7);*/
		@media only screen and (max-width: 768px){
            float:left;
            width:50%;
			height:150px * $rapp-legend * 1.3;
		}
    }
    .legend >div.growth{
        border-bottom:1.5px solid rgba(200,200,200,0.7);
        @media only screen and (max-width: 768px){
            border-bottom:none;
        }

    }
    .legend .ball{
        display:inline-block;
        position:absolute;
        border:2px solid grey;
        background:lightgrey;
    }
    .legend .big{
        top:24px;
        right:26px;
        width:68px;
        height:68px;
        border-radius:39px;
		@media only screen and (max-width: 768px){
			top:24px * $rapp-legend;
            left:55px;
            width:68px * $rapp-legend;
            height:68px * $rapp-legend;
            border-radius:39px * $rapp-legend;
		}
    }
    .legend .medium{
        top:24px;
        right:85px;
        width:54px;
        height:54px;
        border-radius:32px;
        @media only screen and (max-width: 768px){
			top:24px * $rapp-legend;
            left:35px;
            width:54px * $rapp-legend;
            height:54px * $rapp-legend;
            border-radius:32px * $rapp-legend;
		}
    }
    .legend .small{
        top:24px;
        right:133px;
        width:38px;
        height:38px;
        border-radius:24px;
		@media only screen and (max-width: 768px){
			top:24px * $rapp-legend;
            left:20px;
            width:38px * $rapp-legend;
            height:38px * $rapp-legend;
            border-radius:24px * $rapp-legend;
		}		
    }
    .legend .info{
        text-align:left;
        position:absolute;
        bottom:18px;
        left:22px;
		@media only screen and (max-width:768px){
			bottom:18px * $rapp-legend;
            left:22px * $rapp-legend;
		}
    }
    .legend .info .title{
        font-weight:200;
        @include font-size(18px);
    }
    .legend .info .comment{
        font-style:italic;
        @include font-size(12px);
    }
    /*
    /END Legend part
    */
}

/*
JOB RESULTS/RECOMMENDATIONS
*/
.scrollable-content{
    margin:auto;
    overflow:hidden;
}
.scrollable{
    text-align:left;
    width:10000px;
    .step{
        display:inline-block;
    }
}
.delete-resume{
	.section{
		@media(min-width: 480px){
			width: 450px;
		}
	}
	.save{
		text-align: center;
	}
	.footer-dialog{
		border-top:1px solid $lightGrey;
	}
}

.job-results,.saved-content, .delete-items{
	width:100%;
	text-align:center;
	position:relative;
	@include shadow;
	margin-bottom:60px;
	background:white;
	@media only screen and (max-width:768px){
		margin-bottom:30px;
	}
	.icon-arrow-left-big-blue,.icon-arrow-right-big-blue{
		position:absolute;top:50%;margin-top:-26px;
		@include no_select;
		@media only screen and (max-width: 768px){
            top:auto;
			bottom:10px;
		}
	}
	.icon-arrow-left-big-blue{
		left:40px;
        @media only screen and (max-width: 768px){
            left:30px;
		}
	}
	.icon-arrow-right-big-blue{
		right:40px;
        @media only screen and (max-width: 768px){
            right:30px;
		}
	}
	i:hover{
		cursor:pointer;
	}
	.title{
		@include font-size(20px);
		padding:0px 25px;
		padding-top:22px;
		padding-bottom:20px;
		border-bottom:1px solid $grey;
		@media only screen and (max-width: 768px){
			font-size:14px;
			padding:0px 15px;
			padding-top:17px;
			padding-bottom:14px;
		}
	}
	.content{
		position:relative;
		.scrollable-content{
			width:80%;
			height:82%;
            @media only screen and (max-width: 768px){
				height: 84%;
			}
            @media only screen and (max-width: 768px) and (min-width:601px){
				width: 560px;
			}
			@media only screen and (max-width: 600px) and (min-width:481px){
				width: 440px;
			}
            @media only screen and (max-width: 480px) and (min-width:401px){
				width: 330px;
			}
            @media only screen and (max-width: 400px){
				width: 276px;
			}
		}
		.scrollable{
			height:100%;
		}
		.job-recommandation,.save{
			text-align:center;
			text-transform:uppercase;
			height:100%;
			width:187px;
            //oblicated to do more decomposition because here the size can be given in percentage
            //5 elements
            @media only screen and (max-width: 768px) and (min-width:601px){
				width: 112px;
			}
            //4 elements
			@media only screen and (max-width: 600px) and (min-width:481px){
				width: 110px;
			}
            //3 elements
            @media only screen and (max-width: 480px) and (min-width:401px){
				width: 110px;
			}
            @media only screen and (max-width: 400px){
				width: 92px;
			}
			@include inline-top;
		}
		.pages{
			text-align:center;
			@media only screen and (max-width: 768px){
				border-top: 1px $grey solid;
				padding-top: 15px;
			}
			.activated{
				background-color:$navcolor !important;
			}
			.ball{
				margin:0px 5px;
				@include inline-top;
				width:15px;
                height:15px;
                border-radius:10px;
				background-color:$lightBlue;
				@media only screen and (max-width: 768px){
					width:10px;height:10px;border-radius:7px;
				}
			}
			.ball:hover{
				cursor:pointer;
			}
		}
	}
}


.resume-hero{
    .saved-content{
        .save:not(.selected){
			opacity: 0.7;
		}
    }
}
.saved-content, .delete-items{
    .title{text-align:left;}
    .content{
        height:325px;
		@media only screen and (max-width: 768px){
			height: 260px;
		}
        .save{
            margin-top:45px;
            position:relative;
            height:210px;
            @include font-size(18px);
			@media only screen and (max-width: 768px){
				height: auto;
				margin-top:20px;
			}
            .content-save{
                position:absolute;
                top:145px;
                width:100%;
				@media only screen and (max-width: 768px){
					top: 110px;
				}
                .name{
                    color:black;
                    margin-bottom:3px;

                }
                .type{
                    @include font-size(16px);
                    line-height:18px;
                    color:$grey;
                }
                .date, .level{
                    @include font-size(14px);
                    line-height:16px;
                    color:$grey;
                }
            }
        }
    }
}
.job-results{
    .content{
        height:275px;
        .job-recommandation{
            .icon-suitcase-job{
                margin-top:40px;
                margin-bottom:20px;
            }
            @include font-size(17px);
            .title-job{
                color:black;
                line-height:26px;
            }
            .info-job{
                @include font-size(15px);
                line-height:23px;
                color:$grey;
            }
        }
		@media only screen and (max-width:768px){
            height: 210px;
            .scrollable-content{
                height:81%;
            }
			.job-recommandation{
				 .icon-suitcase-job{
					margin-top:20px;
					margin-bottom:10px;
                }
			}
		}
    }
}
/*
Dashboard
*/
.dashboard{
    @include main-block;
    .job-results,.saved-content{
        margin-bottom:25px;
        @media only screen and (max-width: 768px){
            margin-bottom:15px;
        }
    }
    .resume{
        @include whitebox;
        position:relative;
        margin-top:60px;
        margin-bottom:25px;
        @media only screen and (max-width: 768px){
            margin-top:15px;
            margin-bottom:15px;
        }
        > .left,>.right{
            position:relative;
            height:350px;
            padding:45px 30px;
            width:33.31%;
            @media only screen and (max-width: 768px){
                padding:10px;
                padding-top:20px;
                width:100%;
                height:auto;
                border-bottom:1px solid $lightGrey;
            }
        }
        >.left{
            border-right:1px solid $lightGrey;
            @media only screen and (max-width: 768px){
                border-right:none;
            }
        }
        .person{
            @media only screen and (max-width: 768px) and (min-width: 481px){
                width:50%;
                height:300px;
            }
            text-align:center;
            .icon-pencil{
                position:absolute;
                top:45px;
                left:30px;
                @media only screen and (max-width: 768px){
                    top:20px;
                    left:10px;
                }
                
            }
            .edit-name{
                position:absolute;
                top:247px;
                left:30px;
                @media only screen and (max-width: 768px){
                    top:223px;
                    left:10px;
                }
            }
            .picture{
                width:175px;height:175px;
                border-radius:100px;
                border:2px solid white;
                @include box-shadow(0px,1px,2px,0px,0.2);
                margin:auto;margin-bottom:20px;
                
            }
            .name{
                @include font-size(35px);
            }
            .mail{
                color:$grey;
                @include font-size(20px);
            }
        }
        .presentation{
            padding-top:30px;
            padding-bottom:0px;
            @include font-size(20px);
            line-height:20px;
            @media only screen and (max-width: 768px){
                padding-top:20px;
                padding-bottom:15px;
            }
            table{
                tr{
                    height:74px;
                    .logo{
                        margin:0px 15px;
                    }
                    @media only screen and (max-width: 768px){
                        height:74px * $rapp-resp;
                    }
                }
                
                .icon-pencil{
                    @media only screen and (max-width: 768px){
                        margin-top:3px;
                    }
                }
            }
        }
        .job-phase{
            @media only screen and (max-width: 768px) and (min-width: 481px){
                width:50%;
                height:300px;
            }
            @include font-size(20px);
            .title{
                margin-bottom:10px;
                @media only screen and (max-width: 768px) and (min-width: 481px){
                    margin-bottom:30px;
                    height:60px;
                }
                .txt{
                    width:70%;
                    margin-left:20px;
                }
            }
            .phases{position:relative;}
            .phase-content{
                .cursor{
                    padding-top:6px;
                    margin-right:30px;
                    margin-left:6px;
                    position:relative;
                    .icon-cursor-red{
                        position:absolute;
                        left:-4px;top:0px;
                    }
                }
                .phase{
                    margin-bottom:13px;
                    @media only screen and (max-width: 768px){
                        margin-bottom:0px;
                    }
                    i{
                        position:relative;top:auto;right:auto;
                        margin-right:15px;
                        @media only screen and (max-width: 768px){
                            margin-right:8px;
                            top:5px;
                        }
                    }
                    >span{
                        line-height:40px;
                        position:relative;
                        top:1px;
                    }
                }
            }
        }
    }
    .job-results,.saved-content{
        .title{text-align:left;}
        .button-edit{
            margin-top:-8px;
            margin-right:-12px;
            @include font-size(14px);
            position:relative;padding-left:10px;padding-right:40px;
            @media only screen and (max-width: 768px){
                padding-top:10px; padding-bottom:4px;
                margin-right:-7px;
                margin-top:-9px;
            }
        }
    }
    .content{
        .col-left,.col-right{
            width:370px;
            @media only screen and (max-width: 768px){
                width:100%;
            }
        }
        .col-left{
            margin-right:25px;
            @media only screen and (max-width: 768px){
                margin-right:0px;
            }
        }
        .education,.work-experience,.skills,.technology,.career-goals{
            @include whitebox;
            margin-bottom:25px;
            @media only screen and (max-width: 768px){
                margin-bottom:15px;
            }
            .title-section{
                @include font-size(20px);
                padding:22px;
                padding-bottom:19px;
                @media only screen and (max-width: 768px){
                    padding:17px;
                    padding-bottom:14px;
                }
                .title{
                    margin-left:5px;
                }
                border-bottom:1px solid $lightGrey;  
            }
        }
        .work-experience-elem,.education-elem{
            position:relative;
            overflow:hidden;

        }
        .skills,.technology,.career-goals{
            .content{
                padding:20px;
            }
        }
        .education,.work-experience{
            .content{
                padding:20px;padding-bottom:0px;
            }
        }
        .website,.period,.place{
            @include font-size(15px);
            line-height:27px;
        }
        .website,.place{
            color:rgb(238,120,53);
        }
        .period{
            color:rgb(128,128,128);
        }
        .job-title,.education-elem .name{
            @include font-size(20px);
            line-height:30px;
        }
        .detail{
            margin-left:52px;//20+32
            width:75%;
            @media only screen and (max-width: 768px){
                margin-left:15%;
                width:auto;
            }
        }
        .timeline{
            position:absolute;
            height:100%;
            .dashed-bar{
                width:0px;
                margin-left:17px;
                height:90%;
                border-left:1px dashed $lightGrey;
                @media only screen and (max-width: 768px){
                    margin-left:13px;
                }
            }
        }
        .work-experience{
            .period{margin-bottom:3px;}
            .detail{
                padding-bottom:40px;
                @media only screen and (max-width: 768px){
                    padding-bottom:25px;    
                }
                
            }
            .website{
                text-transform:uppercase;
            }
           .occupation,.income{
                text-transform:uppercase;
                padding-left:15px;padding-right:15px;
                @media only screen and (max-width: 768px){
                    padding-left:12px;padding-right:12px;
                    margin-bottom:5px;
                }
            }
            .occupation{
                margin-right:10px;
            }
            .income{
                background:rgb(107,212,138);color:white;
            }
        }
        /*
        EDUCATION
        */
        .education{
            .dashed-bar{
                margin-left:16px;
                @media only screen and (max-width: 768px){
                    margin-left:13px;    
                }
            }
            .detail{
                padding-bottom:30px;
                @media only screen and (max-width: 768px){
                    padding-bottom:25px;    
                }
            }
        }
        /*
        TECHNOLOGY+SKILLS
        */
        .add-elem,.technology-elem,.skill,.occupation,.income{
            @include font-size(16px);
            text-align:center;
            padding-top:12px;
            padding-bottom:8px;
        }
        .occupation,.technology-elem{
            @include blue-element;
        }
        .add-elem-input{
            @include font-size(16px);
            input{
                width:100%;
                padding:12px;
                margin:0px;
                padding-top:12px;
                padding-bottom:8px;
                background:rgb(237,237,237);
                border:1px dashed rgb(190,190,190);
            }
        }
        .add-elem{
            background:rgb(237,237,237);
            border:1px dashed rgb(190,190,190);
        }
        .skills{
            .left{
                margin-right:26px;
                @media only screen and (max-width: 768px){
                    margin-right:8%;
                }
            }
            .add-elem,.add-elem-input,.skill{
                width:46%;
            }
            .add-elem-input{
                float:left;
            }
            .add-elem{margin:0;}
            .skill{
                color:white;
                margin-bottom:20px;
                background:rgb(92,26,71);
            }
        }
        .technology{
            .add-elem,.add-elem-input input,.technology-elem{
                width:100%;
            }
            .technology-elem{
                padding-top:12px;
                padding-bottom:8px;
                margin-bottom:20px;
            }
            @media only screen and (max-width: 768px){
                margin-bottom:30px;
            }
        }
        .career-goals{
            @include font-size(19px);
            .content{
                .goal{
                    margin-top:31px;
                    width:80%;
                    @media only screen and (max-width: 768px){
                        margin-top:20px; 
                    }
                }
                .first{margin-top:0px;}
            }
        }
    }
}
.saved-content-page{
    @include main-block;
    .saved-content{
        margin-top:65px;
        @media only screen and (max-width: 768px){
            margin-top:15px;
        }
        .content{
            height:auto;
            padding:0px 100px;
            padding-bottom:40px;
            .save:not(.right){
                margin-right:8px;
            }
            .fourth{
                margin-right:0px !important;
            }
            .save{
                height:265px;
                overflow:hidden;
                margin-top:40px;
                .delete{
                    color:$red-delete;
                    margin-top:25px;
                    margin-bottom:10px;
                }
            }
            @media only screen and (max-width: 768px){
                padding:20px;
                .save{
                    height:240px;
                    margin-left:0px;
                    margin-right:0px;
                    margin-top:0px;margin-bottom:0px;
                    display:inline-block;
                    vertical-align:top;float:none;
                }
                .save:not(.right){
                    margin-right:0px;
                }
            }
        }
    }
