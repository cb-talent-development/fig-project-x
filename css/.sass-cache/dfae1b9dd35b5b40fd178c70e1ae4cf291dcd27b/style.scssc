.job-results{
	.content{
		.scrollable-content{
			@media only screen and (max-width: 479px){
				height: 75%;
			}
		}
	}
}
.resume-hero{
    .saved-content{
        .save:not(.selected){
			opacity: 0.7;
		}
    }
}
.saved-content, .delete-items{
    .title{text-align:left;}
    .content{
        height:325px;
		@media only screen and (max-width: 479px){
			height: 200px;
		}
        .scrollable-content{
            height:85%;
			@media only screen and (max-width: 479px){
				height: 80%;
			}
        }
		
        .save{
            margin-top:45px;
            position:relative;
            height:210px;
            font-size:18px;
			@media only screen and (max-width: 479px){
				height: 160px;
				margin-top:20px;
			}
            .content-save{
                position:absolute;
                top:145px;
                width:100%;
				@media only screen and (max-width: 479px){
					top: 75px;
				}
                .name{
                    color:black;
                    margin-bottom:3px;
					@media only screen and (max-width: 479px){
						font-size: 15px;
					}
                }
                .type{
                    font-size:16px;
                    line-height:18px;
                    color:$grey;
					@media only screen and (max-width: 479px){
						font-size:11px;
						line-height:13px;
					}
                }
                .date, .level{
                    font-size:14px;
                    line-height:16px;
                    color:$grey;
					@media only screen and (max-width: 479px){
						font-size:9px;
						line-height:11px;
					}
                }
            }
        }
    }
}
.job-results{
    .content{
        height:275px;
        .job-recommandation{
            .icon-suitcase-job{
                margin-top:40px;
                margin-bottom:20px;
            }
            font-size:17px;
            .title-job{
                color:black;
                line-height:26px;
            }
            .info-job{
                line-height:23px;
                color:$grey;
            }
        }
		@media only screen and (max-width:479px){
			height: 150px;
			.job-recommandation{
				 .icon-suitcase-job{
					margin-top:15px;
					margin-bottom:5px;
				}
				.title-job{
					font-size: 13px;
				}
				.info-job{
					font-size: 10px;
					line-height: 12px;
				}
			}
		}
    }
}
/*
Dashboard
*/
.dashboard{
    @include main-block;
    .job-results{
        margin-bottom:25px;
    }
    .resume{
        @include whitebox;
        position:relative;
        margin-top:60px;
        margin-bottom:25px;
        @media only screen and (max-width: 768px){
            margin-top:30px;
            margin-bottom:15px;
        }
        > .left,>.right{
            position:relative;
            height:350px;
            padding:45px 30px;
            width:33.31%;
            @media only screen and (max-width: 768px){
                padding:20px;
                width:100%;
                height:auto;
                border-bottom:1px solid $lightGrey;
            }
        }
        >.left{
            border-right:1px solid $lightGrey;
            @media only screen and (max-width: 768px){
                border-right:none;
            }
        }
        .person{
            text-align:center;
            .icon-pencil{
                position:absolute;
                top:45px;
                left:30px;
                @media only screen and (max-width: 768px){
                    top:20px;
                    left:20px;
                }
                
            }
            .edit-name{
                position:absolute;
                top:247px;
                left:30px;
                @media only screen and (max-width: 768px){
                    top:223px;
                    left:20px;
                }
            }
            .picture{
                width:175px;height:175px;
                border-radius:100px;
                border:2px solid white;
                @include box-shadow(0px,1px,2px,0px,0.2);
                margin:auto;margin-bottom:20px;
                
            }
            .name{
                @include font-size(35px);
            }
            .mail{
                color:$grey;
                @include font-size(20px);
            }
        }
        .presentation{
            padding-top:30px;
            padding-bottom:0px;
            @include font-size(20px);
            line-height:20px;
            @media only screen and (max-width: 768px){
                padding-top:20px;
                padding-bottom:15px;
            }
            table{
                tr{
                    height:74px;
                    .logo{
                        margin:0px 15px;
                    }
                    @media only screen and (max-width: 768px){
                        height:74px * $rapp-resp;
                    }
                }
                
                .icon-pencil{
                    @media only screen and (max-width: 768px){
                        margin-top:3px;
                    }
                }
            }
        }
        .job-phase{
            @include font-size(20px);
            .title{
                margin-bottom:10px;
                .txt{
                    width:70%;
                    margin-left:20px;
                }
            }
            .phases{position:relative;}
            .phase-content{
                .cursor{
                    padding-top:6px;
                    margin-right:30px;
                    margin-left:6px;
                    position:relative;
                    .icon-cursor-red{
                        position:absolute;
                        left:-4px;top:0px;
                    }
                }
                .phase{
                    margin-bottom:13px;
                    @media only screen and (max-width: 768px){
                        margin-bottom:0px;
                    }
                    i{
                        position:relative;top:auto;right:auto;
                        margin-right:15px;
                        @media only screen and (max-width: 768px){
                            margin-right:8px;
                            top:5px;
                        }
                    }
                    >span{
                        line-height:40px;
                        position:relative;
                        top:1px;
                    }
                }
            }
        }
    }
    .saved-content{
        margin-bottom:25px;
    }
    .job-results,.saved-content{
        .title{text-align:left;}
        .button-edit{
            margin-top:-8px;
            margin-right:-12px;
            @include font-size(14px);
            position:relative;padding-left:10px;padding-right:40px;
            @media only screen and (max-width: 768px){
                padding-top:10px; padding-bottom:4px;
            }
        }
    }
    .content{
        .col-left,.col-right{
            width:370px;
            @media only screen and (max-width: 768px){
                width:100%;
            }
        }
        .col-left{
            margin-right:25px;
            @media only screen and (max-width: 768px){
                margin-right:0px;
            }
        }
        .education,.work-experience,.skills,.technology,.career-goals{
            @include whitebox;
            margin-bottom:25px;
            @media only screen and (max-width: 768px){
                margin-bottom:15px;
            }
            .title-section{
                @include font-size(20px);
                padding:22px;
                padding-bottom:19px;
                @media only screen and (max-width: 768px){
                    padding:17px;
                    padding-bottom:14px;
                }
                .title{
                    margin-left:5px;
                }
                border-bottom:1px solid $lightGrey;  
            }
        }
        .work-experience-elem,.education-elem{
            position:relative;
            overflow:hidden;

        }
        .skills,.technology,.career-goals{
            .content{
                padding:20px;
            }
        }
        .education,.work-experience{
            .content{
                padding:20px;padding-bottom:0px;
            }
        }
        .website,.period,.place{
            @include font-size(15px);
            line-height:27px;
        }
        .website,.place{
            color:rgb(238,120,53);
        }
        .period{
            color:rgb(128,128,128);
        }
        .job-title,.education-elem .name{
            @include font-size(20px);
            line-height:30px;
        }
        .detail{
            margin-left:52px;//20+32
            width:75%;
            @media only screen and (max-width: 768px){
                margin-left:15%;
                width:auto;
            }
        }
        .timeline{
            position:absolute;
            height:100%;
            .dashed-bar{
                width:0px;
                margin-left:17px;
                height:90%;
                border-left:1px dashed $lightGrey;
                @media only screen and (max-width: 768px){
                    margin-left:13px;
                }
            }
        }
        .work-experience{
            .period{margin-bottom:3px;}
            .detail{
                padding-bottom:40px;
                @media only screen and (max-width: 768px){
                    padding-bottom:25px;    
                }
                
            }
            .website{
                text-transform:uppercase;
            }
           .occupation,.income{
                text-transform:uppercase;
                padding-left:15px;padding-right:15px;
                @media only screen and (max-width: 768px){
                    padding-left:12px;padding-right:12px;
                    margin-bottom:5px;
                }
            }
            .occupation{
                margin-right:10px;
            }
            .income{
                background:rgb(107,212,138);color:white;
            }
        }
        /*
        EDUCATION
        */
        .education{
            .dashed-bar{
                margin-left:16px;
                @media only screen and (max-width: 768px){
                    margin-left:13px;    
                }
            }
            .detail{
                padding-bottom:30px;
                @media only screen and (max-width: 768px){
                    padding-bottom:25px;    
                }
            }
        }
        /*
        TECHNOLOGY+SKILLS
        */
        .add-elem,.technology-elem,.skill,.occupation,.income{
            @include font-size(16px);
            text-align:center;
            padding-top:12px;
            padding-bottom:8px;
        }
        .occupation,.technology-elem{
            @include blue-element;
        }
        .add-elem-input{
            @include font-size(16px);
            input{
                padding:12px;
                margin:0px;
                padding-top:12px;
                padding-bottom:8px;
                background:rgb(237,237,237);
                border:1px dashed rgb(190,190,190);
            }
        }
        .add-elem{
            background:rgb(237,237,237);
            border:1px dashed rgb(190,190,190);
        }
        .skills{
            .left{
                margin-right:26px;
                @media only screen and (max-width: 768px){
                    margin-right:8%;
                }
            }
            .add-elem,.add-elem-input,.skill{
                width:46%;
            }
            .add-elem-input{
                float:left;
            }
            .add-elem{margin:0;}
            .skill{
                color:white;
                margin-bottom:20px;
                background:rgb(92,26,71);
            }
        }
        .technology{
            .add-elem,.add-elem-input input,.technology-elem{
                width:100%;
            }
            .technology-elem{
                padding-top:12px;
                padding-bottom:8px;
                margin-bottom:20px;
            }
        }
        .career-goals{
            @include font-size(19px);
            .content{
                .goal{
                    margin-top:31px;
                    width:80%;
                    @media only screen and (max-width: 768px){
                        margin-top:20px; 
                    }
                }
                .first{margin-top:0px;}
            }
        }
    }
}
.saved-content-page{
    .saved-content{
        margin-top:65px;
        .content{
            height:auto;
            padding:0px 100px;
            padding-bottom:40px;
            .save:not(.right){
                margin-right:8px;
            }
            .fourth{
                margin-right:0px !important;
            }
            .save{
                height:265px;
                overflow:hidden;
                margin-top:40px;
                .delete{
                    color:$red-delete;
                    margin-top:25px;
                    margin-bottom:10px;
                }
            }
        }
    }
