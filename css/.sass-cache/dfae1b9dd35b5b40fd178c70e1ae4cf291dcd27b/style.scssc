    .pictured{
	   .text{
		  display: none;
        }
	   .img{
           width: 100%;
           position:absolute;
           height:115px;
		   bottom:0px;
	   }
    }
    .not-pictured{
        .text{
		  margin: 10px;
		  margin-top: 0px;
		  padding: 0px;
		  -webkit-line-clamp: 7;
        }
