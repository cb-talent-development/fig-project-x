                @media only screen and (max-width: 768px){
                    padding:17px;
                    padding-bottom:14px;
                }
                .title{
                    margin-left:5px;
                }
                border-bottom:1px solid $lightGrey;  
            }
        }
        .work-experience-elem,.education-elem{
            position:relative;
            overflow:hidden;

        }
        .skills,.technology,.career-goals{
            .content{
                padding:20px;
            }
        }
        .education,.work-experience{
            .content{
                padding:20px;padding-bottom:0px;
            }
        }
        .website,.period,.place{
            @include font-size(15px);
            line-height:27px;
        }
        .website,.place{
            color:rgb(238,120,53);
        }
        .period{
            color:rgb(128,128,128);
        }
        .job-title,.education-elem .name{
            @include font-size(20px);
            line-height:30px;
        }
        .detail{
            margin-left:52px;//20+32
            width:75%;
            @media only screen and (max-width: 768px){
                margin-left:15%;
                width:auto;
            }
        }
        .timeline{
            position:absolute;
            height:100%;
            .dashed-bar{
                width:0px;
                margin-left:17px;
                height:90%;
                border-left:1px dashed $lightGrey;
                @media only screen and (max-width: 768px){
                    margin-left:13px;
                }
            }
        }
        .work-experience{
            .period{margin-bottom:3px;}
            .detail{
                padding-bottom:40px;
                @media only screen and (max-width: 768px){
                    padding-bottom:25px;    
                }
                
            }
            .website{
                text-transform:uppercase;
            }
           .occupation,.income{
                text-transform:uppercase;
                padding-left:15px;padding-right:15px;
                @media only screen and (max-width: 768px){
                    padding-left:12px;padding-right:12px;
                    margin-bottom:5px;
                }
            }
            .occupation{
                margin-right:10px;
            }
            .income{
                background:rgb(107,212,138);color:white;
            }
        }
        /*
        EDUCATION
        */
        .education{
            .dashed-bar{
                margin-left:16px;
                @media only screen and (max-width: 768px){
                    margin-left:13px;    
                }
            }
            .detail{
                padding-bottom:30px;
                @media only screen and (max-width: 768px){
                    padding-bottom:25px;    
                }
            }
        }
        /*
        TECHNOLOGY+SKILLS
        */
        .add-elem,.technology-elem,.skill,.occupation,.income{
            @include font-size(16px);
            text-align:center;
            padding-top:12px;
            padding-bottom:8px;
        }
        .occupation,.technology-elem{
            @include blue-element;
        }
        .add-elem-input{
            @include font-size(16px);
            input{
                width:100%;
                padding:12px;
                margin:0px;
                padding-top:12px;
                padding-bottom:8px;
                background:rgb(237,237,237);
                border:1px dashed rgb(190,190,190);
            }
        }
        .add-elem{
            background:rgb(237,237,237);
            border:1px dashed rgb(190,190,190);
        }
        .skills{
            .left{
                margin-right:26px;
                @media only screen and (max-width: 768px){
                    margin-right:8%;
                }
            }
            .add-elem,.add-elem-input,.skill{
                width:46%;
            }
            .add-elem-input{
                float:left;
            }
            .add-elem{margin:0;}
            .skill{
                color:white;
                margin-bottom:20px;
                background:rgb(92,26,71);
            }
        }
        .technology{
            .add-elem,.add-elem-input input,.technology-elem{
                width:100%;
            }
            .technology-elem{
                padding-top:12px;
                padding-bottom:8px;
                margin-bottom:20px;
            }
            @media only screen and (max-width: 768px){
                margin-bottom:30px;
            }
        }
        .career-goals{
            @include font-size(19px);
            .content{
                .goal{
                    margin-top:31px;
                    width:80%;
                    @media only screen and (max-width: 768px){
                        margin-top:20px; 
                    }
                }
                .first{margin-top:0px;}
            }
        }
    }
}
.saved-content-page{
    @include main-block;
    .saved-content{
        margin-top:65px;
        @media only screen and (max-width: 768px){
            margin-top:15px;
        }
        .content{
            height:auto;
            padding:0px 100px;
            padding-bottom:40px;
            .save:not(.right){
                margin-right:8px;
            }
            .fourth{
                margin-right:0px !important;
            }
            .save{
                height:265px;
                overflow:hidden;
                margin-top:40px;
                .delete{
                    color:$red-delete;
                    margin-top:25px;
                    margin-bottom:10px;
                }
            }
            @media only screen and (max-width: 768px){
                padding:20px;
                .save{
                    height:240px;
                    margin-left:0px;
                    margin-right:0px;
                    margin-top:0px;margin-bottom:0px;
                    display:inline-block;
                    vertical-align:top;float:none;
                }
                .save:not(.right){
                    margin-right:0px;
                }
            }
        }
    }
