}

.transp-black{
	background: rgba(0, 0, 0, 0.6);
	.title{
		color: white;
	}
	position:absolute;
}

@mixin fontMedia{
	overflow: hidden;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 1;
}
@mixin video-info-promo{
    position: relative;
    .transp-black{
        position:absolute;
        z-index: 2;
        h2.title{
			-webkit-line-clamp: 3;
        }
    }
    .comment{
        display:none;
    }
    .img{
		@include fontMedia;
	}
}
.infographic{
    @include video-info-promo;
	.icon{
		position:absolute;
		z-index: 3;
		bottom: 10px;
		right: 10px;
	}
}
@mixin video-info-promo-1{
    .transp-black{
        position:relative;
        top:0px;left:0px;
		@media only screen and (max-width: 768px){
			width: 100%;
		}
		@media only screen and (min-width:769px){
			height: 100%;
			width: 363px;
		}
		h2.title{
			padding: 25px;
			padding-bottom: 0px;
			margin: 0px;
			//font-size: $large-title;
			@media only screen and (max-width: 768px) {
				//font-size: 25px;
                padding:15px;padding-bottom: 0px;

			}
		}
    }
    
}
@mixin video-info-1{
    @include video-info-promo-1;
    .transp-black{
		h3.subtitle{
			margin: 0px;
			//font-size: 1.4em;
			padding-top: 17px;
			padding-bottom: 20px;
			padding-left: 30px;
            @media only screen and (max-width: 768px) {
                padding-top:15px;
                padding-bottom: 55px;padding-left: 15px;
			}
		}
        .comment{
            width:95%;
            padding:12px 20px;
            z-index:4;
         	background: rgba(0, 0, 0, 0.7);
            position:absolute;bottom:0px;left:0px;
            .commentCount{
                color:white;
                font-weight:normal;
               // font-size:15px;
                display:inline-block;
                margin-top:2px;
                margin-left:3px;
            }
            @media only screen and (max-width: 768px) {
                width:100%;
                padding-left:15px;padding-right:15px;
                padding-top:10px;padding-bottom:8px;
            }
            .icon-bar-grey{
                margin-left:10px;margin-right:8px;
            }
        }
	}
}
/* 
Inforgraphic
*/
@mixin i-small{
    @media only screen and (max-width: 768px){
        padding:8px;
    }
	.transp-black{
        @media only screen and (max-width: 768px) {
		    width: 100%;
            padding:10px;padding-bottom:0px;
        }
		h2.title{
			@media only screen and (max-width: 768px) {
                -webkit-line-clamp: 5;
			}
		}
		h3.subtitle{
			@media only screen and (max-width: 768px) {
				padding-top: 10px;
				padding-bottom: 0px;
			}
		}
	}
}
/*
Common css
*/
.i-v-p-2{
    .transp-black{
		padding: 20px;
		width: 100%;	
        top:0px;left:0px;
		padding-bottom: 0px;
