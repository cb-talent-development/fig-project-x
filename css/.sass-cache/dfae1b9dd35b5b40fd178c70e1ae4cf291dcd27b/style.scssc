/*
General CSS
*/
@mixin main-block{
    width:$total-width;
    display:block;
    margin:auto;
    @media only screen and (max-width: 768px){
        width:97%;
    }
}
@mixin webkit-box{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow:hidden;

}
@mixin absoluteFill{
    position:absolute;left:0px;right:0px;top:0px;bottom:0px;
    width:100%;height:100%;
}
@mixin box-shadow($left,$bottom,$spread,$thickness,$opacity){
    box-shadow:$left $bottom $spread $thickness rgba(0,0,0,$opacity);
    -webkit-box-shadow: $left $bottom $spread $thickness rgba(0,0,0,$opacity);
}
@mixin inline-top{
   display:inline-block;
   vertical-align:top;
}
.visible{
    display:block;
}
.invisible{
    display:none;
}
.clear{
    clear:both;
}
@mixin no_select{
   -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
.no_select{
    -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
.main-container{
    @include main-block;
}
.link{
    position:relative;
}
.link > a{
    @include absoluteFill;
    z-index:10;
}
.link:hover{
    cursor:pointer;
}
table{
    border-collapse:collapse;
}
table td{
    vertical-align:middle;
}
.left{
    float:left;
}
.right{
    float:right;
}
.inline{
    @include inline-top;
}
.custom-checkbox{
    position:relative;
    input[type=checkbox] {
	    display: none;
    } 
    label:hover{ cursor:pointer;}
    label:before{
        content:"";
        position:absolute;
        left:0px;
        top:50%;
        text-align: center;  
    }
    label:hover:before { 
        box-shadow:0px 0px 2px 0px rgba(0,0,0,0.3);
        -webkit-box-shadow:0 0px 2px 0px rgba(0,0,0,0.3);
    }
    input[type=checkbox]:checked + label:before { 
        content: "\2713";  
    }  
}
.toggleMenu,.toggleMenuSimple{
    position:relative;
    .subMenu{
        position:absolute;
        left:0px;
        z-index:100;
        width:100%;
        color:inherit;
        display:none;
        background-color:inherit;
    }
}
/*
COLOR
*/
@mixin shadow-color{
    @include box-shadow(0px,1px,1px,0px,0.3);
}
$grey-color:rgb(237,237,237);
$dark-grey-color:rgb(207,207,207);
$grey-font:rgb(157,157,157);
@mixin back-grey{
    @include shadow-color;
    background-color:$grey-color !important;color:black;
}
.back-grey{
    @include back-grey;
}
.back-grey-no-hover{
    @include back-grey;
}
.back-grey:hover{
    cursor:pointer;
    background-color:$dark-grey-color !important;
}
.grey{
    color:$grey-font;
}
//green
$green-color:rgb(112,194,57);
$dark-green-color:rgb(95,174,45);
@mixin back-green{
    @include shadow-color;
    background-color:$green-color !important;color:white;
}
.back-green-no-hover{
    @include back-green;
}
.back-green{
    @include back-green;
}
.back-green:hover{
    cursor:pointer;
    background-color:$dark-green-color !important;
}
.green{
    color:$green-color !important;
}
//white
@mixin back-white{
    @include shadow-color;
    background-color:white !important;color:black;
}
.back-white-no-hover{
    @include back-white;
}
.back-white{
    @include back-white;
}
.back-white:hover{
    cursor:pointer;
    color:rgb(80,80,80) !important;
}
.white{
    color:white !important;
}
/*
Arrow
*/
@mixin arrow-up-down{
    width: 0; 
	height: 0; 
	border-left-style:solid;
    border-left-color:transparent;
    border-right-style:solid;  
    border-right-color:transparent;  
	font-size: 0;
	line-height: 0;
    
}
.arrow-up {
    @include arrow-up-down;
    border-bottom-style:solid; 
}
.arrow-down {    
    @include arrow-up-down;
	border-top-style:solid; 
}
/*
Leaflet
*/
.leaflet-top-pane {
    pointer-events: none;
}
.leaflet-control-attribution,.map-legends {
    display:none !important;
}
