            .logo{
                position:absolute;top:25px;left:50%;
            }
            .icon-careerscape-small{
                margin-left:-70px;
            }
            .icon-resumePro-small{
                margin-left:-73.5px;
            }
            .icon-jobinality-small{
                margin-left:-60px;
            }
            .icon-localygrown-small{
                margin-left:-79px;
            }
            .info{
                width:65%;margin:auto;margin-top:200px;
                .title{
                    font-size:36px;
                }
                .summary{
                    margin-top:20px;font-size:23px;
                }
                .icon-arow-right-light{
                    margin-left:3px;margin-top:2px;
                }
            }
        }
        .right{
            height:100%;
            background-color:$grey;
            width:69.2%;
        }
    }
}
@mixin blue-element{
    background-color:$navcolor;
    color:white;
}
/*
DIALOG
*/
.dialog{
    z-index:300;
    display:none;
    position:fixed;top:0px;left:0px;bottom:0px;right:0px;width:100%;height:100%;
    .background{
        background:rgba(0,0,0,0.5);
        position:fixed;top:0px;left:0px;bottom:0px;right:0px;width:100%;height:100%;
    }
    .section{
        position:relative;display:none;
        width:60%;margin:auto;margin-top:100px;background-color:rgb(243,245,245);
        .header{
            position:relative;
            font-size:27px;padding-left:30px;
            padding-right:15px;
            padding-top:20px;
            font-weight:bold;
            padding-bottom:12px;
            border-bottom:1px solid $lightGrey;
            .close{
                position:absolute;
                top:50%;
                right:20px;
                margin-top:-15px;
            }
        }
        .intro{
            width:100%;
            font-weight:18px;padding:22px;
            border-bottom:1px solid $lightGrey;
            td{vertical-align:middle;}
            .logo{position:relative;top:26px;left:17px;}
            strong{
                font-weight:bold;
            }
        }
        .content{width:100%;}
        .footer-dialog{
            padding:22px;
                .button-save,.button-next,.button-previous{
                    position:relative;
                    padding:0px 14px;
                    padding-top:12px;
                    padding-bottom:7px;
                }
                .button-next,.button-previous{
                    i{
                        position:absolute;
                        top:50%;
                        margin-top:-8.5px;
                    }
                }
                .button-previous[class~=selected],.button-next[class~=selected]{
                    opacity:0.5;
                }
                .button-previous{
                    padding-left:40px;
                    margin-right:15px;
                    i{left:15px;}
                }
                .button-next{
                    padding-right:40px;
                    i{right:15px;}
                }
        }
    }
}
/*
Tool CSS
*/
.tool{
    @include main-block;
    /*
    Button for tools
    */
    .header-tool{
        width:83%;
        margin:auto;
        margin-top:60px;
        margin-bottom:60px;
        .title-social{
            @include shadow;
            position:relative;
            float:left;width:840px;margin-right:28px;
            height:94px;
            padding-top:27px;
            padding-bottom:27px;
            .social{
                float:right;
                height:100%;
                margin-right:18px;
            }
            .icon-jobinality-small,.icon-resumePro-small,.icon-localygrown-small,.icon-careerscape-small{
                position:absolute;
                top:-20px;left:73px;
            }
            .icon-arrow-right-big-grey{
                position:absolute;top:50%;margin-top:-26px;
                left:431px;
            }
            .title{
                top:0px;
                text-transform:capitalize;
                font-size:37px;
                position:absolute;
                line-height:108px;
                height:94px;
                left:238px;
            }
        }
        .help{
            @include shadow;
            float:right;
            height:94px;
            padding:25px;
            width:90px;
            .icon-help:hover{
                cursor:pointer;
            }
        }
        .icon-bar-grey{
            margin:9px;
        }
    }
    .buttons_zoom{
        position:absolute;
        z-index:100;
        top:15px;
        left:12px;
        .selected{
            background:rgba(110,110,110,0.8);
        }
    }
    .selector{
        width:100%;
        margin:auto;
        margin-bottom:30px;
        .input-button{
            padding-top:19px;padding-bottom:15px;
            padding-left:15px;
            width:100%;height:100%;color:white;
            font-size:22px;
            border:none;background:none;
        }
        .industries,.occupations{
            padding-top:22px;padding-bottom:17px;
            font-size:22px;
            position:relative;
            .title{
                margin-left:20px;
            }
            b{
                right:18px;
                top:32.5px;
                margin-top:-2px;
                position:absolute;
            }
            .subMenu{
                left:0px;
                top:65px;
                font-size:17px;
                line-height:40px;
                color:white !important;
                .elem{
                    padding-left:20px;
                }
                .elem:hover{
                    color:rgba(255,255,255,0.8) !important;
                }
            }
            .mode-0{
                background-color:$darkBlue;
            }
            input[type=checkbox]{
                margin-right:3px;
            }
        }
        .industries:hover,.occupations:hover{
            color:rgba(255,255,255,0.8);
            cursor:pointer;
        }
        .industries{
            @include blue-element;
            float:left;
            width:23%;
            margin-left:30px;
            height:100%;
        }
        .occupations{
            @include blue-element;
            float:right;
            width:48.7%;
            height:100%;
            .elem{
                float:left;
                width:50%;
            }
            .mode-1{
                background-color:$darkBlue;
            }
        }
    }
    .messages-jobs{
        position:relative;
        width:100%;
        @include blue-element;
        text-align:center;
        font-size:20px;
        line-height:65px;
        height:60px;
    }
    .arrow{
        margin-top:7px;margin-bottom:3px;
        text-align:center;width:100%;
    }
    .arrow-down {
       border-left-width:20px;
       border-right-width:20px;
       border-top-width:20px;
       border-top-color:$navcolor;
    }
    .dialog{
        .section{
            .content{padding:15px;}
        }
    }
    .help-section{
        .section{
            width:85%;
        }
    }
    .save-section{
        .section{
            width:60%;
            .content{  border-bottom:1px solid $lightGrey;text-align:center;
                .logo{ display:block;margin:auto; }
                input{ margin:auto; }
            }
            .footer-dialog{
                
                .button{
                    float:right;
                }
            }
        }
    }
}
/*
Locally Grown
*/
.locallyGrown{
    .selector{
        .input-zip{
            width:23%;
            min-width:267px;
            float:left;
            @include blue-element;
        }
    }
    .mapbox{
        @include main-block;
        height:590px;
    }
}

/*
Career Scape
*/
.careerScape{
    canvas{
        background-color:white;
        margin:auto;cursor:move;padding:0px;display:block;z-index:1;
    }
    .canvas-container{
        margin:auto;
        text-align:center;
        position:relative;
    }
    .buttons-changes{
            width:23%;
            min-width:267px;
            height:65px;
            float:left;
            background-color:$navcolor;
    }
    .button-change{
        color:white;
        font-size:18px;
        padding-top:23px;padding-bottom:10px;
        display:inline-block;
        height:100%;
        width:49%;
        .carre{
            float:left;
            width:20px;
            height:20px;
            background:white;
            position:relative;
            margin-left:15px;
            margin-right:10px;
        }
        .txt{
            float:left;            
        }
        
    }
    .button-change:hover{
        cursor:pointer
    }
    .button-change:hover:not(.selected) .txt{
        color:rgba(255,255,255,0.8);
    }
    .button-change:hover:not(.selected) .carre{
        background-color:rgba(255,255,255,0.8);
    }
    .buttons-changes .selected .txt,.buttons-changes .selected:hover .txt{
        color:rgb(238,120,53);
    }
    .buttons-changes .selected .carre,.buttons-changes .selected:hover .carre{
        background:rgb(238,120,53);
    }
    .zoom{
        width:38px;
        font-weight:600;
        font-size:25px;
        background:rgb(80,80,80);
        color:white;
        margin-bottom:12px;
        padding-top:5px;
        padding-bottom:3px;
    }
    .zoom:hover:not(.selected){
            background: rgba(50,50,50,0.8);
            cursor:pointer;
    }
    .button:hover{
        cursor:pointer;
    }
    #json{
        margin:auto;
        display:block;
        margin-top:40px;
        background:#FFFEAA;
        color:rgb(90,90,90);
        width:70%;
        height:400px;
    }
    /*
    Legend part
    */
    .legend{
        z-index:10;
        position:absolute;
        width:200px;
        height:300px;
        top:40px;
        right:22px;  
        -moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        -webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
        box-shadow: 0px 2px 2px rgba(0,0,0,0.3);
    }
    .legend > div{
        position:relative;
        width:100%;
        height:150px;
        background:white;
        /*border-bottom:1.5px solid rgba(200,200,200,0.7);
        border-right:1.5px solid rgba(200,200,200,0.7);
        border-left:1.5px solid rgba(200,200,200,0.7);*/
    }
    .legend >div.growth{
        border-bottom:1.5px solid rgba(200,200,200,0.7);

    }
    .legend .ball{
        display:inline-block;
        position:absolute;
        border:2px solid grey;
        background:lightgrey;
    }
    .legend .big{
        top:24px;
        right:26px;
        width:68px;
        height:68px;
        border-radius:39px;
    }
    .legend .medium{
        top:24px;
        right:85px;
        width:54px;
        height:54px;
        border-radius:32px;
        
    }
    .legend .small{
        top:24px;
        right:133px;
        width:38px;
        height:38px;
        border-radius:24px;            
    }
    .legend .info{
        text-align:left;
        position:absolute;
        bottom:18px;
        left:22px;
    }
    .legend .info .title{
        font-weight:200;
        font-size:18px;
    }
    .legend .info .comment{
        font-style:italic;
        font-size:12px;
    }
    /*
    /END Legend part
    */
}

/*
JOB RESULTS/RECOMMENDATIONS
*/
.job-results,.saved-content{
        @include main-block;
        text-align:center;
        position:relative;
        @include shadow;
        margin-bottom:60px;
        background:white;
        .icon-arrow-left-big-blue,.icon-arrow-right-big-blue{
            position:absolute;top:50%;margin-top:-26px;
            @include no_select;
        }
        .icon-arrow-left-big-blue{
            left:40px;
        }
        .icon-arrow-right-big-blue{
            right:40px;
        }
        i:hover{
            cursor:pointer;
        }
        .title{
            font-size:20px;
            padding:0px 25px;
            padding-top:22px;
            padding-bottom:20px;
            
            border-bottom:1px solid $grey;
        }
        .content{
            position:relative;
            .scrollable-content{
                width:80%;
                margin:auto;
                height:82%;
                overflow:hidden;
            }
            .scrollable{
                height:100%;
                text-align:left;
                width:10000px;
            }
            .job-recommandation,.save{
                text-align:center;
                text-transform:uppercase;
                height:100%;
                width:187px;
                @include inline-top;

            }
            .pages{
                text-align:center;
                .activated{
                    background-color:$navcolor !important;
                }
                .ball{
                    margin:0px 5px;
                    @include inline-top;
                    width:15px;height:15px;border-radius:10px;
                    background-color:$lightBlue;
                }
                .ball:hover{
                    cursor:pointer;
                }
            }
        }
}
.saved-content{
    .content{
        height:325px;
        .scrollable-content{
            height:85%;
        }
        .save{
            margin-top:45px;
            position:relative;
            height:210px;
            font-size:17px;
            line-height:23px;
            .content{
                position:absolute;
                top:145px;
                width:100%;
                .name{
                    color:black;
                }
                .type{
                    font-size:15px;
                    color:$grey;
                }
            }
        }
    }
}
.job-results{
    .content{
        height:275px;
        .job-recommandation{
            .icon-suitcase-job{
                margin-top:40px;
                margin-bottom:20px;
            }
            font-size:17px;
            .title-job{
                color:black;
                line-height:26px;
            }
            .info-job{
                line-height:23px;
                color:$grey;
            }
        }
    }
}
/*
Dashboard
*/
.dashboard{
    @include main-block;
    .resume{
        @include whitebox;
        position:relative;
        margin-top:60px;
        margin-bottom:25px;
        > .left,>.right{
            position:relative;
            height:350px;
            padding:45px 30px;
            width:33.31%;
        }
        >.left{
            border-right:1px solid $lightGrey;
        }
        .person{
            text-align:center;
            .icon-pencil{
                position:absolute;
                top:45px;
                left:30px;
                
            }
            .picture{
                width:175px;height:175px;
                border-radius:100px;
                border:2px solid white;
                @include box-shadow(0px,1px,2px,0px,0.2);
                margin:auto;margin-bottom:20px;
                
            }
            .name{
                font-size:35px;
            }
            .mail{
                color:$grey;
                font-size:20px;                                                              
            }
        }
        .presentation{
            padding-top:30px;
            padding-bottom:0px;
            font-size:20px;
            line-height:20px;
            table{
                tr{
                 height:74px;
                    .logo{
                        margin:0px 15px;
                    }
                }
            }
        }
        .job-phase{
            font-size:20px;
            .title{
                margin-bottom:10px;
                .txt{width:70%;
                    margin-left:20px;
                }
            }
            .phase-content{
                .cursor{
                    padding-top:6px;
                    margin-right:30px;
                    margin-left:6px;
                    position:relative;
                    .icon-cursor-red{
                        position:absolute;
                        left:-4px;top:65px;
                    }
                }
                .phase{
                    margin-bottom:13px;
                    i{
                        margin-right:15px;}
                    >span{
                        line-height:40px;
                        position:relative;
                        top:1px;
                    }
                }
            }
        }
    }
    .saved-content{
        @include whitebox;
        margin-bottom:25px;
        
    }
    .job-results,.saved-content{
        .title{text-align:left;}
    }
    .content{
        .col-left,.col-right{
            width:370px;
        }
        .col-left{
            margin-right:25px;
        }
        .education,.work-experience,.skills,.technology,.career-goals{
            @include whitebox;
            margin-bottom:25px;
            .title-section{
                font-size:19px;
                padding:22px;
                padding-bottom:19px;
                .title{
                    margin-left:5px;
                }
                border-bottom:1px solid $lightGrey;  
            }
        }
        .work-experience-elem,.education-elem{
            position:relative;
            overflow:hidden;

        }
        .skills,.technology,.career-goals{
            .content{
                padding:20px;
            }
        }
        .education,.work-experience{
            .content{
                padding:20px;padding-bottom:0px;
            }
        }
        .website,.period,.place{
            font-size:15px;
            line-height:27px;
        }
        .website,.place{
            color:rgb(238,120,53);
        }
        .period{
            color:rgb(128,128,128);
        }
        .job-title,.education-elem .name{
            font-size:20px;line-height:30px;
        }
        .detail{
            margin-left:52px;//20+32
            width:75%;
        }
        .timeline{
            position:absolute;
            height:100%;
            .dashed-bar{
                width:0px;
                margin-left:17px;
                height:90%;
                border-left:1px dashed $lightGrey;
            }
        }
        .work-experience{
            .period{margin-bottom:3px;}
            .detail{
                padding-bottom:40px;
            }
            .website{
                text-transform:uppercase;
            }
           .occupation,.income{
                text-transform:uppercase;
                padding-left:15px;padding-right:15px;
            }
            .occupation{
                margin-right:10px;
            }
            .income{
                background:rgb(107,212,138);color:white;
            }
        }
        .education{
            .dashed-bar{margin-left:16px;}
            .detail{
                padding-bottom:30px;
            }
        }
        .add-elem,.technology-elem,.skill,.occupation,.income{
            font-size:16px;
            text-align:center;
            padding-top:12px;
            padding-bottom:8px;
        }
        .occupation,.technology-elem{
            @include blue-element;
        }
        .add-elem{
            background:rgb(237,237,237);
            border:1px dashed rgb(190,190,190);
        }
        .skills{
            .left{
                margin-right:26px;
            }
            .add-elem,.skill{
                width:46%;
            }
            .add-elem{margin:0;}
            .skill{
                color:white;
                margin-bottom:20px;
                background:rgb(92,26,71);
            }
        }
        .technology{
            .add-tech,.technology-elem{
                padding-top:12px;
                padding-bottom:8px;
            }
            .technology-elem{
                margin-bottom:20px;
            }
        }
        .career-goals{
            font-size:19px;
            .content{
                .goal{margin-top:31px;width:80%;}
                .first{margin-top:0px;}
            }
        }
    }
    .popup{
        .section{
            width:90%;
            .header{
                font-size:24px;
                .grey{
                    color:$grey;
                }
            }
            .content{
                .custom-checkbox{
                    margin-right:25px;
                    position:relative;
                    input[type=checkbox] {
                        display: none;
                    } 
                    label{margin-left:40px;}
                    label:hover{ cursor:pointer;}
                    label:before{
                        height:20px;
                        width:20px;
                        content:"";
                        position:absolute;
                        left:0px;
                        top:50%;
                        text-align: center;  
                        margin-top:-15px;
                        @include box-shadow(0.5px,-1px,2px,0px,0.3);
                        border-right:1px solid $lightGrey;
                        background:white;
                    }
                    input[type=checkbox]:checked + label:before { 
                        background:$orange;
                    }  
                }
                .custom-checkbox{
                    font-size:23px;
                    color:$darkGrey;
                }
                .message{
                    font-size:17px;
                    text-transform:uppercase;
                    color:rgb(178,178,178);
                    margin-bottom:10px;
                }
                .optional{color:$lightGrey;text-transform:uppercase;font-size:17px;}
                .select,input,input[placeholder]{  
                    padding-top:20px;padding-bottom:16px;
                    font-size:23px;
                }   
                .large{width:340px;margin-right:25px;}
                .medium{width:140px;margin-right:25px;}
                .address{
                    padding:17px;
                    .street{margin-bottom:20px;}
                }
                .general{
                    padding:17px;
                    border-bottom:1px solid $lightGrey;
                    .gender{margin-right:135px;
                        .custom-checkbox{margin-top:15px;}
                    }
                    
                }
            }
            .footer-dialog{
                border-top:1px solid $lightGrey;
                padding-top:12px;
                padding-bottom:15px;
            }
        }
    }
