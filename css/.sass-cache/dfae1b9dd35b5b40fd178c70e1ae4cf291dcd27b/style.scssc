    @include shadow-effect;
    background:white;
}
@mixin invert-shadow-effect{
    box-shadow:0px -1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow:0px -1px 1px rgba(0,0,0,0.2);
    border:none;
}
@mixin invert-shadow{
    @include invert-shadow-effect;
    background:white;
}
@mixin button-style{
    font-size:18px;
    @include inline-top; 
    position:relative;
    padding-top:16px;padding-bottom:11px;
    padding-left:15px;padding-right:15px;
    text-align:left;
    color:$darkGrey;
     b,i{
        position:absolute;
        top:50%;
        right:15px;
    }
}
.green{
    background:green;
    color:white;
}
.button, .btn{
    @include shadow;
    @include button-style;
}
.button:hover:not(.no-hover){
    cursor:pointer;
    color: #6b6b6b;
	.grey-icon{
		opacity: 0.8;
	}
}
.select{
    @include shadow;
    @include button-style;
    border-bottom:2px solid $lightGrey;
    b,i{margin-top:-4.5px;}
    .subMenu{
        @include shadow;
        border-top:1px solid $lightGrey;
        top:100%;
        padding:5px 15px;
        > div{
            margin:5px 0px
        }
        >div:hover{
            color:black !important;
        }
    }
}
.select:hover{cursor:pointer;}
.input-button{
    margin:0px;
    @extend .button;
    border-top:2px solid $lightGrey;
    padding:0px;
    box-shadow:0 -1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow:0 -1px 1px rgba(0,0,0,0.2);
    input{
        padding-top:13px;padding-bottom:9px;padding-left:15px;padding-right:15px; // default padding
