@mixin main-block{
    width:$total-width;
    display:block;
    margin:auto;
    @media only screen and (max-width: 768px){
        width:97%;
    }
}
@mixin webkit-box{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow:hidden;

}
@mixin absoluteFill{
    position:absolute;left:0px;right:0px;top:0px;bottom:0px;
    width:100%;height:100%;
}
@mixin box-shadow($left,$bottom,$spread,$thickness,$opacity){
    box-shadow:$left $bottom $spread $thickness rgba(0,0,0,$opacity);
    -webkit-box-shadow: $left $bottom $spread $thickness rgba(0,0,0,$opacity);
}
@mixin inline-top{
   display:inline-block;
   vertical-align:top;
}
.visible{
    display:block;
}
.invisible{
    display:none;
}
.clear{
    clear:both;
}
@mixin no_select{
   -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
.no_select{
    -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
.main-container{
    @include main-block;
}
.link{
    position:relative;
}
.link > a{
    @include absoluteFill;
    z-index:10;
}
.link:hover{
    cursor:pointer;
}
table{
    border-collapse:collapse;
}
table td{
    vertical-align:middle;
}
.left{
    float:left;
}
.right{
    float:right;
}
.inline{
    @include inline-top;
}
.custom-checkbox{
    position:relative;
    input[type=checkbox] {
	    display: none;
    } 
    label:hover{ cursor:pointer;}
    label:before{
        content:"";
        position:absolute;
        left:0px;
        top:50%;
        text-align: center;  
    }
    label:hover:before { 
        box-shadow:0px 0px 2px 0px rgba(0,0,0,0.3);
        -webkit-box-shadow:0 0px 2px 0px rgba(0,0,0,0.3);
    }
    input[type=checkbox]:checked + label:before { 
        content: "\2713";  
    }  
}
.custom-radio{
    position:relative;
    input[type=radio] {
	    display: none;
    } 
    label:hover{ cursor:pointer;}
    label:before{
        content:"";
        position:absolute;
        left:0px;
        top:50%;
        text-align: center;  
    }
    label:hover:before { 
        box-shadow:0px 0px 2px 0px rgba(0,0,0,0.3);
        -webkit-box-shadow:0 0px 2px 0px rgba(0,0,0,0.3);
    } 
}
.toggleMenu,.toggleMenuSimple{
    position:relative;
    .subMenu{
        position:absolute;
        left:0px;
        z-index:100;
        width:100%;
        color:inherit;
        display:none;
        background-color:inherit;
    }
}
