}
table{
    border-collapse:collapse;
}
table td{
    vertical-align:middle;
}
.left{
    float:left;
}
.right{
    float:right;
}
.inline{
    @include inline-top;
}
.custom-checkbox{
    position:relative;
    input[type=checkbox] {
	    display: none;
    } 
    label:hover{ cursor:pointer;}
    label:before{
        content:"";
        position:absolute;
        left:0px;
        top:50%;
        text-align: center;  
    }
    label:hover:before { 
        box-shadow:0px 0px 2px 0px rgba(0,0,0,0.3);
        -webkit-box-shadow:0 0px 2px 0px rgba(0,0,0,0.3);
    }
    input[type=checkbox]:checked + label:before { 
        content: "\2713";  
    }  
}
.custom-radio{
    position:relative;
    input[type=radio] {
	    display: none;
    } 
    label:hover{ cursor:pointer;}
    label:before{
        content:"";
        position:absolute;
        left:0px;
        top:50%;
        text-align: center;  
    }
    label:hover:before { 
        box-shadow:0px 0px 2px 0px rgba(0,0,0,0.3);
        -webkit-box-shadow:0 0px 2px 0px rgba(0,0,0,0.3);
    } 
}
.toggleMenu,.toggleMenuSimple{
    position:relative;
    .subMenu{
        position:absolute;
        left:0px;
        z-index:100;
        color:inherit;
        display:none;
        background-color:inherit;
    }
}
