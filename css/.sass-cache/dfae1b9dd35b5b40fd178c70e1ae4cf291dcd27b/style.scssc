                    .left{margin-right:2.2%;}
                    .before-last{
                        margin-right:0px;
                    }
                     .phase[class~="selected"]{
                        opacity:1;
                    }
                }
                .job-occupation{
                    .large{width:465px;margin:0px;}
                    padding:0px 20px;
                    height:200px;
                    border-bottom:1px solid $lightGrey;
                    .job{
                        height:100%;
                        width:42%;
                        position:relative;
                        .entry{position:absolute;bottom:30px;
                            .message{margin-bottom:10px;}
                        }
                        
                    }
                    .job-correction{
                        height:200px;
                        width:50.7%;
                        position:relative;
                        .logo{
                            position:absolute;
                            bottom:30px;
                        }
                        .correction{
                            position:absolute;
                            bottom:30px;
                            right:0px;
                            .mess{
                                font-size:16px;
                                margin-bottom:10px;
                                text-transform:uppercase;
                                color:$orange;
                                @include webkit-box;
                                -webkit-line-clamp:4;
                            }
                            .wrong{color:black;}
                            .select{border:1px solid $orange;}
                        }
                    }
                }
                .job-info{
                    padding:0px 15px;
                    padding-top:25px;padding-bottom:30px;
                    
                }
            }
            .footer-dialog{
                border-top:1px solid $lightGrey;
                padding-top:12px;
                padding-bottom:15px;
            }
        }
    }
}
.saved-content-page{
    .saved-content{
        margin-top:65px;
        .content{
            height:auto;
            padding:0px 100px;
            padding-bottom:40px;
            .save:not(.right){
                margin-right:8px;
            }
            .fourth{
                margin-right:0px !important;
            }
            .save{
                height:265px;
                overflow:hidden;
                margin-top:40px;
                .delete{
                    color:$red-delete;
                    margin-top:25px;
                    margin-bottom:10px;
                }
            }
        }
    }
