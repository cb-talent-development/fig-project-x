    .comments{
        .new-comment{
			padding:20px;
            position:relative;
            border-bottom:1px solid $lightGrey;
            .img-user{
                float:left;
                width:40px;height:40px;
            }
            .input{
                position:relative;
                height:40px;
                margin-left:40px;
                width:auto;
                display:block;
                background-color:$backgroundGrey;color:black;
                input{
                    z-index:4;
                    position:absolute;
                    padding:10px;
                    top:0px;left:0px;width:100%;
                    background-color:rgba(0,0,0,0);
                    height:40px;
                    border:none;
                }
                .placeholder{
                    z-index:1;
                    text-transform:capitalize;
                    position:absolute;
                    line-height:40px;height:40px;padding-left:10px;
                    top:0px;left:0px;width:100%;
                }
				@media only screen and (max-width: 1200px) and (min-width:1001px){
					width: 220px;
					input{
						//height:24px;
						padding:3px;
					}
					.placeholder{
						font-size: 12px;
					}
				}
				@media only screen and (max-width: 479px){
					width: 80%;
					input{
						//height:20px;
						padding: 4px;
					}
				}
            }
        }
        .comments-list{
            padding:20px;
            padding-right:0px;
            padding-bottom:0px;
            background-color:white;
            @media only screen and (max-width: 1000px){
				padding: 15px;
                padding-right:0px;
                padding-bottom:0px;
			}
            .level-2 .content,.level-3 .content{
                border-left:1px solid $backgroundGrey;               
            }
            .level-2 .img{
                margin-right:20px;
            }
            .level-3 .img{
                margin-right:40px;
            }
            .level-1 .content{
                width:80%;
               .new-comment .input{
                    width:78%;
            
                }
                @media only screen and (max-width: 1000px){

                    width:84%;
                    .new-comment .input{
                        width:80%;
            
                    }
                }
            }
            .level-2 .content{
                width:73%;
                .new-comment .input{
                        width:78%;
            
                }
                @media only screen and (max-width: 1000px){
                    width:78%;
                    .new-comment .input{
                        width:78%;
                    }
                }
            }
            .level-3 .content{
                width:64%;
                .new-comment .input{
                        width:80%;
            
                }
                @media only screen and (max-width: 1000px){
                    width:70%;
                    .new-comment .input{
                        width:76%;
                        
                    }
                }
            }
            .separator{
                height:1px;
                border-bottom:1px solid $backgroundGrey;
                margin-bottom: 15px;
                margin-top:15px;
                margin-left:-20px;
            }
            .comment{
                font-size:13px;
                .img{
                    height:40px;width:40px;float:left;
                }
                .name{
                    font-weight:bold;
                    margin-right:12px;
                }
                .time{
                    color:$lightGrey;
                }
                .statement{
                    margin:10px 0px;
                    overflow:hidden;
                }
                .reply{
                    position:relative;
                    margin-right:5px;
                    text-transform:capitalize;
                    font-weight:bold;
                    color:$orange;
                }
                .reply:hover{
                    cursor:pointer;
                    text-decoration:underline;
                }
                .closeReply{
                    @extend .reply;
                    color:rgb(212,98,104);
                }
                .content{
                    padding-left:20px;
                    float:left;
                    padding-right:20px;
                    transition: padding-bottom, 0.5s;
                    padding-bottom:30px;
                    .new-comment{
                        width:100%;
                        border:none;
                        padding:15px 0px;
                        display:none;
                        .placeholder{
                            color:$grey;
                        }
                    }
                    .closeReply{
                        display:none;
                    }
                    @media only screen and (max-width: 1000px){
                        padding-left:20px * $rapp-resp;
                        padding-bottom:30px * $rapp-resp;
                        padding-right:15px;
                    }
                }
            }
            .no-first-bar .separator,.no-second-bar .separator{
                display:none;
            }
            .no-padding-bottom .comment .content{
                padding-bottom:0px;
            }
        }
    }
