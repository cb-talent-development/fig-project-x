$sprite-width: 3000px;
$sprite-height: 3597px;
@mixin dimensionPosition($posX,$posY,$height,$width,$rapp){
    display: inline-block;
	vertical-align: top;
    background: url("assets/x_sprite.png") no-repeat;
    background-size:ceil($sprite-width/2 * $rapp) ceil($sprite-height/2 * $rapp);
    background-position: ceil($rapp * $posX) ceil($rapp * $posY);
    height: ceil($height * $rapp);
    width: ceil($width * $rapp);
}
.icon-box-white{
    @include dimensionPosition( -340px, -56px, 21px, 21px, 1.0);
    @media only screen and (max-width: 1000px){
        @include dimensionPosition( -340px, -56px, 21px, 21px, 0.7);
	}
}
.icon-box-orange{
    @include dimensionPosition( -306px, -56px, 21px, 21px, 1.0);
    @media only screen and (max-width: 1000px){
        @include dimensionPosition( -306px, -56px, 21px, 21px, 0.7);
	}
}
.icon-box-checked{
    @include dimensionPosition( -376px, -59px, 16px, 16px, 1.0);
    @media only screen and (max-width: 1000px){
        @include dimensionPosition( -376px, -59px, 16px, 16px, 0.7);
	}
}
.icon-box-not-checked{
    @include dimensionPosition( -409px, -59px, 16px, 16px, 1.0);
    @media only screen and (max-width: 1000px){
        @include dimensionPosition( -409px, -59px, 16px, 16px, 0.7);
	}
}

