		}
        @include font-size-3;
    }
     @media only screen and (max-width: 1024px) and (min-width:1001px){
        //to be sure
        .not-pictured .left,.not-pictured .right{
            width: 100%;
        }
		.pictured .left{
			width: 105px;
		}
		.pictured .right{
			width: 193px;
		}
		.pictured{
			.title{
				width: 100%;
				padding: 15px;
			}
			.img{
               height:90px;
           }
		}
		.pictured, .not-pictured{	
			@include font-size-2-3;
		}
        @include font-size-2;
    }
	@media only screen and (max-width: 1000px){
        @include phone-height;
        //to be sure
        .pictured .left,.pictured .right,.not-pictured .left,.not-pictured .right{
            width:100%;
            display:block;
        }
        .pictured h3.subtitle,.pictured h2.title,.pictured div.text,.not-pictured h3.subtitle,.not-pictured h2.title,.not-pictured div.text{
            padding-left: 10px;padding-right: 10px;
        }
